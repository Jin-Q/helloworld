		<Field id="pyee_name" columnName="pyee_name" cnname="收款人账户名" length="100" JSPTag="text" JDBCType="VARCHAR2"/>
		<Field id="paorg_no" columnName="paorg_no" cnname="收款人开户行行号" length="20" JSPTag="text" JDBCType="VARCHAR2"/>
		<Field id="paorg_name" columnName="paorg_name" cnname="收款人开户行行名" length="100" JSPTag="text" JDBCType="VARCHAR2"/>
	</TableModel>
</LmtAppFinSubpay.xml>