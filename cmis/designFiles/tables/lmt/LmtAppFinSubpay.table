<?xml version="1.0" encoding="UTF-8" ?>


<LmtAppFinSubpay.xml>
	<TableModel id="LmtAppFinSubpay" pageSize="10" cnname="融资性担保公司代偿" isPaged="true" dbTableName="lmt_app_fin_subpay">
		<TableInfo/>
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="cus_id" columnName="cus_id" cnname="客户码" length="30" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="manager_id" columnName="manager_id" cnname="责任人" length="20" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="manager_br_id" columnName="manager_br_id" cnname="责任机构" length="20" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JSPTag="text" canBeNull="true" JDBCType="CHAR"/>
		<Field id="approve_status" dictname="WF_APP_STATUS" columnName="approve_status" cnname="申请状态" length="4" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="subpay_cur_type" dictname="STD_ZX_CUR_TYPE" columnName="subpay_cur_type" cnname="代偿币种" length="10" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="subpay_totl_limit" columnName="subpay_totl_limit" cnname="代偿总额" length="16" JSPTag="text" canBeNull="true" JDBCType="number"/>
		<Field id="subpay_times" columnName="subpay_times" cnname="本次代偿笔数" length="10" JSPTag="text" canBeNull="true" JDBCType="number"/>
		<Field id="subpay_app_date" columnName="subpay_app_date" cnname="代偿申请日期" length="10" JSPTag="date" canBeNull="true" JDBCType="CHAR"/>
		<Field id="subpay_end_date" columnName="subpay_end_date" cnname="代偿办结日期" length="10" JSPTag="date" canBeNull="true" JDBCType="CHAR"/>
