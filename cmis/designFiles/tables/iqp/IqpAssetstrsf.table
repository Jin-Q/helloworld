		<Field id="prd_id" columnName="prd_id" cnname="产品编码" length="6" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="takeover_type" dictname="STD_ZB_TAKEOVER_MODE" columnName="takeover_type" cnname="转让方式" length="5" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="toorg_no" columnName="toorg_no" cnname="交易对手行号" length="20" JSPTag="pop" canBeNull="true" JDBCType="VARCHAR2"/>
