		<Field id="limit_acc_no" columnName="limit_acc_no" cnname="授信台帐编号" length="40" JSPTag="text" JDBCType="VARCHAR2"/>
		<Field id="topp_acct_no" columnName="topp_acct_no" cnname="交易对手账户" length="40" JSPTag="text" JDBCType="VARCHAR2"/>
