<?xml version="1.0" encoding="UTF-8" ?>


<IqpAssetstrsf.xml>
	<TableModel id="IqpAssetstrsf" pageSize="10" cnname="资产转让申请" isPaged="true" dbTableName="iqp_assetstrsf">
		<TableInfo/>
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
