		<Field id="topp_acct_name" columnName="topp_acct_name" cnname="交易对手户名" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="tooorg_no" columnName="tooorg_no" cnname="交易对手开户行行号" length="20" JSPTag="pop" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="tooorg_name" columnName="tooorg_name" cnname="交易对手开户行行名" length="100" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="tooorg_city" columnName="tooorg_city" cnname="交易对手所在城市" length="50" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="this_acct_no" columnName="this_acct_no" cnname="本行账户" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
