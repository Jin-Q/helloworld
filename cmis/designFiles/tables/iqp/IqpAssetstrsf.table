		<ConditionFields>asset_no;serno;</ConditionFields>
		<FilterFields>asset_no;takeover_type;toorg_no;asset_total_amt;takeover_total_amt;takeover_date;takeover_int;manager_br_id;input_date;approve_status;</FilterFields>
