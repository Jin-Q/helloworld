		<Field id="asset_total_amt" dataType="Currency" columnName="asset_total_amt" cnname="资产总额" length="16" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="takeover_total_amt" dataType="Currency" columnName="takeover_total_amt" cnname="转让本金" length="16" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="takeover_int" dataType="Currency" columnName="takeover_int" cnname="转让利息" length="16" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
