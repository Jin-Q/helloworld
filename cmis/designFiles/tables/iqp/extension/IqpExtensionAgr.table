<?xml version="1.0" encoding="UTF-8" ?>


<IqpExtensionAgr.xml>
	<TableModel id="IqpExtensionAgr" pageSize="10" cnname="展期协议表" isPaged="true" dbTableName="iqp_extension_agr">
		<TableInfo/>
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="agr_no" columnName="agr_no" cnname="协议编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="fount_bill_no" columnName="fount_bill_no" cnname="原借据编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="fount_cont_no" columnName="fount_cont_no" cnname="原合同编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
