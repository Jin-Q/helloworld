		<ConditionFields>serno;agr_no;fount_bill_no;fount_cont_no;cus_id;</ConditionFields>
		<RecordRestrict query="true" delete="true" update="true"/>
		<FilterFields>serno;agr_no;fount_bill_no;fount_cont_no;cus_id;fount_cur_type;fount_loan_amt;fount_loan_balance;extension_amt;fount_end_date;extension_date;manager_id;manager_br_id;input_br_id;status;</FilterFields>
