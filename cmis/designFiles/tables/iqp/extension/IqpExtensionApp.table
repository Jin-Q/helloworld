<?xml version="1.0" encoding="UTF-8" ?>


<IqpExtensionApp.xml>
	<TableModel id="IqpExtensionApp" pageSize="10" cnname="展期申请表" isPaged="true" dbTableName="iqp_extension_app">
		<TableInfo/>
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="bill_no" columnName="bill_no" cnname="借据编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
