		<Field id="start_date" columnName="start_date" cnname="起贷日期" length="10" JSPTag="date" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="end_date" columnName="end_date" cnname="止贷日期" length="10" JSPTag="date" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="extension_amt" dataType="Currency" columnName="extension_amt" cnname="展期金额" length="16" JSPTag="text" canBeNull="false" JDBCType="NUMBER"/>
		<Field id="extension_date" columnName="extension_date" cnname="展期日期" length="10" JSPTag="date" canBeNull="false" JDBCType="VARCHAR2"/>
