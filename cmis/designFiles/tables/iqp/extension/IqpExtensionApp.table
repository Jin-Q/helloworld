		<RecordRestrict query="true" delete="true" update="true"/>
		<ConditionFields>serno;bill_no;cont_no;cus_id;</ConditionFields>
		<FilterFields>serno;bill_no;cont_no;cus_id;cur_type;loan_amt;loan_balance;end_date;extension_date;manager_br_id;manager_id;input_date;approve_status;</FilterFields>
