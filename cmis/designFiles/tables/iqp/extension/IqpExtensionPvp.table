		<Field id="fount_end_date" columnName="fount_end_date" cnname="原止贷日期" length="10" JSPTag="date" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="extension_amt" dataType="Currency" columnName="extension_amt" cnname="展期金额" length="16" JSPTag="text" canBeNull="false" JDBCType="NUMBER"/>
