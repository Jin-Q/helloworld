		<Field id="fnc_loan_name" columnName="fnc_loan_name" cnname="债权人名称" length="10" JSPTag="text" canBeNull="true" JDBCType="CHAR"/>
		<Field id="fnc_loan_use" columnName="fnc_loan_use" cnname="借款用途" length="10" JSPTag="text" canBeNull="true" JDBCType="CHAR"/>
		<OrderFields>pk_id[desc];</OrderFields>
		<ConditionFields>cus_id;cus_name;</ConditionFields>
		<FilterFields>cus_id;cus_name;fnc_loan_typ;fnc_amt;fnc_blc;input_id;input_date;input_br_id;</FilterFields>
		<RecordRestrict query="true" delete="true" update="true"/>
	</TableModel>
</FncLoan.xml>