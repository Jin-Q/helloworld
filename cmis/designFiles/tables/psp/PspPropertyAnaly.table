		<Field id="property_id" isPK="true" columnName="property_id" pkGenerator="UNID" cnname="资产编号" JSPTag="text" length="32" JDBCType="VARCHAR2"/>
		<Field id="task_id" columnName="task_id" cnname="任务编号" length="32" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="cus_id" columnName="cus_id" cnname="客户码" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="property_type" columnName="property_type" cnname="资产类型" length="5" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="owner" columnName="owner" cnname="所有权人" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
