		<Field id="case_no" isPK="true" columnName="case_no" cnname="案件编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="proposer" columnName="proposer" cnname="申请人" length="80" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="review_reason" dictname="STD_ZB_REVIEW_REASON" columnName="review_reason" cnname="再审原因" length="10" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="review_court" columnName="review_court" cnname="再审法院" length="80" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="main_judge" columnName="main_judge" cnname="主审法官" length="80" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="judgment_date" columnName="judgment_date" cnname="再审判决(裁定)日期" length="10" JSPTag="date" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="court_date" columnName="court_date" cnname="再审开庭日期" length="10" JSPTag="date" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="law_writ_inure_date" columnName="law_writ_inure_date" cnname="法律文书生效日期" length="10" JSPTag="date" canBeNull="true" JDBCType="VARCHAR2"/>
