		<Field id="judgment_summary" columnName="judgment_summary" cnname="判决内容摘要" length="250" JSPTag="textarea" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="whether_appeal" dictname="STD_ZX_YES_NO" columnName="whether_appeal" cnname="是否上诉" length="10" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="whether_final_judgment" dictname="STD_ZX_YES_NO" columnName="whether_final_judgment" cnname="是否为终审判决(裁定)" length="10" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
