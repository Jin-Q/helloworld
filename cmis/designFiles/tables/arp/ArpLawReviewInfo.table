<?xml version="1.0" encoding="UTF-8" ?>


<ArpLawReviewInfo.xml>
	<TableModel id="ArpLawReviewInfo" pageSize="10" cnname="再审情况表" isPaged="true" dbTableName="arp_law_review_info">
