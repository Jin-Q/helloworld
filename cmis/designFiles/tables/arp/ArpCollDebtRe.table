<?xml version="1.0" encoding="UTF-8" ?>


<ArpCollDebtRe.xml>
	<TableModel id="ArpCollDebtRe" cnname="抵债物关联表" dbTableName="arp_coll_debt_re">
		<TableInfo/>
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="debt_acc_no" columnName="debt_acc_no" cnname="抵债台账编号" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="guaranty_no" isPK="true" columnName="guaranty_no" cnname="押品编号" length="40" JSPTag="text" canBeNull="false" JDBCType="VARCHAR2"/>
		<Field id="debt_in_amt" dataType="Currency" columnName="debt_in_amt" cnname="抵入金额" length="16" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="status" dictname="STD_ZB_DEBT_RE_STATUS" columnName="status" cnname="状态" length="5" JSPTag="select" canBeNull="true" JDBCType="VARCHAR2"/>
		<OrderFields>serno[desc];guaranty_no[desc];</OrderFields>
		<Field id="rel" columnName="rel" cnname="关系类型（1-引入已存在的，2-新增的抵债物）" JSPTag="text" length="5" JDBCType="VARCHAR2"/>
	</TableModel>
</ArpCollDebtRe.xml>