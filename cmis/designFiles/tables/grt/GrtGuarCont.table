		<Field id="guar_cont_type" dictname="STD_GUAR_CONT_TYPE" columnName="guar_cont_type" cnname="担保合同类型" length="1" JSPTag="select" canBeNull="true" JDBCType="CHAR"/>
		<Field id="guar_way" dictname="STD_GUAR_TYPE" columnName="guar_way" cnname="担保方式" length="5" JSPTag="select" canBeNull="true" JDBCType="CHAR"/>
