		<Field id="guar_term" columnName="guar_term" cnname="担保期限" length="0" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="guar_start_date" columnName="guar_start_date" cnname="担保起始日" length="10" JSPTag="text" canBeNull="true" JDBCType="CHAR"/>
		<Field id="guar_end_date" columnName="guar_end_date" cnname="担保终止日" length="10" JSPTag="text" canBeNull="true" JDBCType="CHAR"/>
