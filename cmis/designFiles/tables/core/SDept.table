		<Field id="memo" columnName="memo" cnname="备注" length="60" JSPTag="textarea" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="orgid" columnName="orgid" extAttr="hidden=&quot;true&quot;" cnname="组织号" length="16" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<OrderFields>depno[desc];</OrderFields>
		<ConditionFields>depno;organno;depname;distno;</ConditionFields>
		<FilterFields>depno;organno;depname;distno;</FilterFields>
	</TableModel>
</SDept.xml>