<?xml version="1.0" encoding="UTF-8" ?>


<WfiSignTask.xml>
	<TableModel id="WfiSignTask" pageSize="10" cnname="会签任务主表" isPaged="true" dbTableName="wfi_sign_task">
		<TableInfo/>
		<Field id="st_task_id" JDBCType="VARCHAR2" length="40" columnName="st_task_id" canBeNull="false" pkGenerator="UNID" isPK="true" JSPTag="text" cnname="会签任务ID"/>
		<Field id="st_task_name" columnName="st_task_name" cnname="任务名称" length="60" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_exe_user" columnName="st_exe_user" cnname="会签安排人" length="10" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_exe_org" columnName="st_exe_org" cnname="执行机构" length="10" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_config" columnName="st_config" cnname="会签策略" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_advice" columnName="st_advice" cnname="会签意见" length="1000" JSPTag="textarea" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_members" columnName="st_members" cnname="会签成员" length="500" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_leader" columnName="st_leader" cnname="牵头人" length="10" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_agree_count" dataType="Int" columnName="st_agree_count" cnname="同意票数" length="0" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="st_reject_count" dataType="Int" columnName="st_reject_count" cnname="否决票数" length="0" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="st_noidea_count" dataType="Int" columnName="st_noidea_count" cnname="复议票数" length="0" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="st_result" dictname="WF_SIGN_RESULT" columnName="st_result" cnname="会签结果" length="3" JSPTag="select" canBeNull="true" JDBCType="CHAR"/>
		<Field id="st_task_status" dictname="WF_SIGN_STATUS" columnName="st_task_status" cnname="会签状态" length="3" JSPTag="select" canBeNull="true" JDBCType="CHAR"/>
