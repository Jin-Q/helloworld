		<Field id="biz_type" dictname="ZB_BIZ_CATE" columnName="biz_type" cnname="业务类型" length="3" JSPTag="select" canBeNull="true" JDBCType="CHAR"/>
		<Field id="st_start_time" dataType="Date" columnName="st_start_time" cnname="开始时间" length="10" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_end_time" dataType="Date" columnName="st_end_time" cnname="结束时间" length="10" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
		<Field id="st_task_times" dataType="Int" columnName="st_task_times" cnname="会签次数" length="0" JSPTag="text" canBeNull="true" JDBCType="NUMBER"/>
		<Field id="wfi_node_id" columnName="wfi_node_id" cnname="流程节点ID" length="40" JSPTag="text" canBeNull="true" JDBCType="VARCHAR2"/>
