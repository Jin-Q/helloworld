		<ConditionFields>st_task_name;serno;biz_type;st_result;</ConditionFields>
		<FilterFields>st_task_id;st_task_name;st_exe_user;st_exe_org;st_result;st_task_status;serno;biz_type;st_start_time;st_end_time;</FilterFields>
		<Field id="st_request_day" dataType="Int" columnName="st_request_day" cnname="要求办理天数" length="0" JSPTag="text" JDBCType="NUMBER"/>
		<Field id="st_duty" columnName="st_duty" cnname="会签成员岗位" length="40" JSPTag="text" JDBCType="VARCHAR2"/>
