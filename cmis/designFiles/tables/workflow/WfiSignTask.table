		<Field id="wfi_scene_id" columnName="wfi_scene_id" cnname="场景ID" length="10" JSPTag="text" JDBCType="VARCHAR2"/>
		<Field id="wfi_sign" columnName="wfi_sign" cnname="物理流程标识" length="40" JSPTag="text" JDBCType="VARCHAR2"/>
		<Field id="wfi_biz_page" columnName="wfi_biz_page" cnname="申请页面" length="100" JSPTag="text" JDBCType="VARCHAR2"/>
	</TableModel>
</WfiSignTask.xml>