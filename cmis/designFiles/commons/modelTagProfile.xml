<?xml version="1.0" encoding="GB18030" ?>

<EditorProfile rootElement="JspTagConfigure">
	<elementCatalog id="JspTagConfigure" name="标准Tag属性及生成器定义">
		<Element id="text" name="文本输入框" implClass="com.ecc.emp.ext.tag.field.EMPExtText">
			<attributes>
				<attr id="dataType" attrName="数据类型"
					editorClass="com.ecc.emp.ide.table.upgrade.DataTypeSelectDialog" />
				<attr id="dictname" attrName="字典名称"
					editorClass="com.ecc.emp.ide.table.upgrade.DictionarySelectDialog" />
				<attr id="readonly" attrName="只读" valueList="true;false;" />
			</attributes>
			<document />
		</Element>
		<Element id="select" name="下拉框"
			implClass="com.ecc.emp.ext.tag.field.EMPExtSelect">
			<attributes>
				<attr id="dataType" attrName="数据类型"
					editorClass="com.ecc.emp.ide.table.upgrade.DataTypeSelectDialog" />
				<attr id="dictname" attrName="字典名称"
					editorClass="com.ecc.emp.ide.table.upgrade.DictionarySelectDialog" />
			</attributes>
		</Element>
		<Element id="textarea" name="输入框"
			implClass="com.ecc.emp.ext.tag.field.EMPExtTextArea" />
		<Element id="date" name="日期" implClass="com.ecc.emp.ext.tag.field.EMPExtDate" />
		<Element id="link" name="链接" implClass="com.ecc.emp.ext.tag.field.EMPExtLink" />
		<Element id="pop" name="弹出选择" implClass="com.ecc.emp.ext.tag.field.EMPExtPop">
			<attributes>
                <attr id="url" attrName="连接" />
                <attr id="returnmethod" attrName="回调方法" />
                <attr id="datamapping" attrName="数据应射关系" />
                
				<attr id="readonly" attrName="只读" valueList="true;false;" />
			</attributes>
			<document />
		</Element>
	</elementCatalog>
</EditorProfile>