<?xml version="1.0" encoding="UTF-8" ?>


<empDataTypeDef.xml>
	<Decimal id="Currency" name="金额类型" max="100000000000" min="-100000000000"
		showSymbol="false" keepStringValue="true" scale="2" showComma="true"
		example="数值类型" />
	<Date id="Date" name="日期数据类型" dateTimeFormat="yyyy-MM-dd" type="DateType"
		example="2010-10-10" />
	<DateMonDay id="DateMonDay" name="日期数据类型-月日" dateTimeFormat="MM-dd"
		type="DateType" example="10-10" />
	<DateYearMon id="DateYearMon" name="日期数据类型-年月"
		dateTimeFormat="yyyy-MM" type="DateType" example="2010-09" />
	<Pattern id="Phone" name="固定电话号码类型"
		regFormat="^(0?[0-9]{2,3}-?){0,1}[0-9]{7,8}(-[0-9]{1,4}){0,1}$"
		example="010-1234567" />
	<Pattern id="Mobile" name="手机号码类型" regFormat="^[0-9]\\d{10}$"
		example="13100000000" />
	<Pattern id="Email" name="电子邮箱类型"
		regFormat="^\\w+((-\\w+)|(\\.\\w+))*\\@[A-Za-z0-9]+((\\.|-)[A-Za-z0-9]+)*\\.[A-Za-z0-9]+$"
		example="xxx@163.com" />
	<Identity id="Identity" name="身份证号码类型" />
	<Percent id="Rate" name="利率类型" max="999" min="-999"
		keepStringValue="true" scale="8" showComma="false" isPercent="true"
		example="数值类型" />
	<Percent id="Rate4Month" name="月利率" isPercent="false"
		keepStringValue="true" scale="9" min="0" showComma="false" isInput="false"
		max="1000" example="数值类型" />
	<Percent id="Percent" name="百分率" max="1000" min="0"
		keepStringValue="true" scale="4" showComma="false" isPercent="true"
		example="数值类型" />
	<Percent id="Percent2" name="可负的百分率" max="999" min="-999"
		keepStringValue="true" showComma="false" isPercent="true" example="数值类型" />
	<Percent id="Permille" name="千分率" max="1" min="0"
		keepStringValue="true" scale="9" showComma="false" isPercent="false"
		example="数值类型" />
	<Byte id="Byte" name="字节型类型数据" example="字节类型" />
	<Short id="Short" name="短整型类型数据" keepStringValue="false" example="数值类型" />
	<Int id="Int" name="整型类型数据" regFormat="/^-?\d+$/" example="(只能为整数)" />
	<Long id="Long" name="长整型类型数据" example="数值类型" />
	<Float id="Float" name="浮点型类型数据" example="数值类型" />
	<Decimal id="Double" name="双浮点型类型数据" example="数值类型" />
	<Pattern id="Postcode" name="邮政编码" regFormat="[1-9]\\d{5}(?!\\d)"
		example="100032" />
	<Decimal id="Decimal5" name="双浮点型类型数据" scale="5" example="数值类型" />
	<Decimal id="DecimalA" name="双浮点型类型数据" scale="2" max="10000000000"
		min="0" example="数值类型" />
	<!-- 编码类型，只能是字母、数字、_、-、! -->
	<Pattern id="CodeNo" name="编码" regFormat="^(\\w*_*!*-*)+$"
		example="abc_1233(只能是字母、数字、_、-、!)" />
</empDataTypeDef.xml>