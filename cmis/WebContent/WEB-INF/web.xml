	<context-param>
		<param-name>resourceFileName</param-name>
		<param-value>WEB-INF/commons/resource.xml</param-value>
	</context-param>
	<context-param>
		<param-name>logSettingFile</param-name>
		<param-value>WEB-INF/commons/logging.xml</param-value>
	</context-param>
	<context-param>
		<param-name>logImplClass</param-name>
		<param-value>com.ecc.emp.log.EMPLog4jLog</param-value>
	</context-param>
	<context-param>
		<param-name>functionFileName</param-name>
		<param-value>WEB-INF/commons/function.xml</param-value>
	</context-param>
	<filter>
		<filter-name>CMISMvcEncoding</filter-name>
		<filter-class> com.ecc.emp.web.jsptags.SetCharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>CMISMvcEncoding</filter-name>
		<url-pattern>*.do</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class> com.ecc.emp.util.StartupListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>definitionservlet</servlet-name>
		<servlet-class> com.ecc.echain.workflow.servlet.DefinitionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>monitorservlet</servlet-name>
		<servlet-class> com.ecc.echain.workflow.servlet.MonitorServlet</servlet-class>
	</servlet>
	
	
	<servlet>
		<servlet-name>TestTCPIPChanel</servlet-name>
		<servlet-class>com.ecc.emp.access.tcpip.EMPTCPIPServiceServlet</servlet-class>
		<init-param>
			<param-name>factoryName</param-name>
			<param-value>CMISBiz</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/channels/TestTCPIPChanel.xml</param-value>
		</init-param>
		<init-param>
			<param-name>sessionIdField</param-name>
			<param-value/>
		</init-param>
		<load-on-startup>2</load-on-startup>
	</servlet>
