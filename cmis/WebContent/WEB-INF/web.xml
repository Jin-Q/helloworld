	<servlet-mapping>
		<servlet-name>ShuffleServlet</servlet-name>
		<url-pattern>/ShuffleServlet</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>shufflejnlpservlet</servlet-name>
