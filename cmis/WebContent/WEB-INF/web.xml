		<init-param>
			<param-name>factoryName</param-name>
			<param-value>CMISBiz</param-value>
		</init-param>
		<init-param>
			<param-name>servletContextFile</param-name>
			<param-value>WEB-INF/channels/YM_HTTP_ESB.xml</param-value>
		</init-param>
		<init-param>
		    <param-name>encoding</param-name>
		    <param-value>UTF-8</param-value>
