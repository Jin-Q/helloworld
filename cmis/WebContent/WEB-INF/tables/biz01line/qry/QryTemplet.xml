<?xml version="1.0" encoding="UTF-8" ?>


<QryTemplet.xml>
	<TableModel id="QryTemplet" cnname="查询信息配置模板" dbTableName="qry_templet">
		<Field id="temp_no" isPK="true" columnName="temp_no" cnname="查询模板编号" length="20" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="temp_name" columnName="temp_name" cnname="查询名称" length="200" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="organlevel" columnName="organlevel" cnname="适用机构等级" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="templet_type" columnName="templet_type" cnname="查询类型" length="1" JDBCType="CHAR" canBeNull="true"/>
		<Field id="temp_pattern" columnName="temp_pattern" cnname="查询模式" length="1" JDBCType="CHAR" canBeNull="true"/>
		<Field id="classpath" columnName="classpath" cnname="扩展类路径" length="250" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="temp_enable" columnName="temp_enable" cnname="是否启用" length="1" JDBCType="CHAR" canBeNull="true"/>
		<Field id="query_sql" columnName="query_sql" cnname="查询SQL语句" length="4000" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="jsp_file_name" columnName="jsp_file_name" cnname="查询页面文件名" length="250" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="order_id" columnName="order_id" cnname="排序字段" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<One2MultiRef toField="temp_no" refModelId="QryParam" fromField="temp_no"/>
		<One2MultiRef toField="temp_no" refModelId="QryResult" fromField="temp_no"/>
	</TableModel>
</QryTemplet.xml>