<?xml version="1.0" encoding="UTF-8" ?>


<CusIndivInsu.xml>
	<TableModel id="CusIndivInsu" cnname="商业保险情况" dbTableName="cus_indiv_insu">
		<Field id="cus_id" isPK="true" columnName="cus_id" cnname="客户代码" length="30" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="indiv_ins_id" isPK="true" columnName="indiv_ins_id" cnname="保险编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="indiv_ins_cvg" columnName="indiv_ins_cvg" cnname="保险名称（险种）" length="80" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_com" columnName="indiv_ins_com" cnname="保险公司" length="80" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_typ" columnName="indiv_ins_typ" cnname="保险种类" length="2" JDBCType="CHAR" canBeNull="true"/>
		<Field id="policyholders" columnName="policyholders" cnname="投保人" length="60" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="beneficiaries" columnName="beneficiaries" cnname="受益人" length="60" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_sub" columnName="indiv_ins_sub" cnname="保险标的" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="indiv_ins_val" columnName="indiv_ins_val" cnname="保险价值" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="indiv_ins_und" columnName="indiv_ins_und" cnname="承保公司" length="80" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_fee" columnName="indiv_ins_fee" cnname="保险费" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="indiv_ins_tot_amt" columnName="indiv_ins_tot_amt" cnname="缴费总额/当前余额" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="indiv_ins_str_dt" columnName="indiv_ins_str_dt" cnname="投保日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_end_dt" columnName="indiv_ins_end_dt" cnname="到期日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_amt" columnName="indiv_ins_amt" cnname="保险金额" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="remark" columnName="remark" cnname="备注" length="250" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="last_upd_id" columnName="last_upd_id" cnname="更新人" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="last_upd_date" columnName="last_upd_date" cnname="更新日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_ins_status" columnName="indiv_ins_status" cnname="抵押状况" length="1" JDBCType="VARCHAR2"/>
	</TableModel>
</CusIndivInsu.xml>