<?xml version="1.0" encoding="UTF-8" ?>


<CusIndivBond.xml>
	<TableModel id="CusIndivBond" cnname="持有金融债券" dbTableName="cus_indiv_bond">
		<Field id="cus_id" isPK="true" columnName="cus_id" cnname="客户代码" length="30" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="indiv_bond_id" isPK="true" columnName="indiv_bond_id" cnname="债券编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="indiv_bond_typ" columnName="indiv_bond_typ" cnname="债券类别" length="1" JDBCType="CHAR" canBeNull="true"/>
		<Field id="indiv_bond_eva_amt" columnName="indiv_bond_eva_amt" cnname="债券估价总额" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="indiv_bond_pub" columnName="indiv_bond_pub" cnname="发行商" length="80" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_bond_str_dt" columnName="indiv_bond_str_dt" cnname="持有起始日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_bond_end_dt" columnName="indiv_bond_end_dt" cnname="持有终止日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="remark" columnName="remark" cnname="备注" length="250" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="last_upd_id" columnName="last_upd_id" cnname="更新人" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="last_upd_date" columnName="last_upd_date" cnname="更新日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="indiv_bond_name" columnName="indiv_bond_name" cnname="证券名称" length="80" JDBCType="VARCHAR2"/>
		<Field id="indiv_bond_status" columnName="indiv_bond_status" cnname="抵押状况" length="1" JDBCType="CHAR"/>
	</TableModel>
</CusIndivBond.xml>