		<Field id="guaranty_no" columnName="guaranty_no" cnname="押品编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="credit_currency_cd" columnName="credit_currency_cd" cnname="信用证币种" length="3" JDBCType="CHAR" canBeNull="true"/>
		<Field id="credit_value" columnName="credit_value" cnname="信用证金额" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="apply_user" columnName="apply_user" cnname="开证申请人名称" length="100" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="apply_area" columnName="apply_area" cnname="开证申请人国家或地区" length="100" JDBCType="VARCHAR2" canBeNull="true"/>
