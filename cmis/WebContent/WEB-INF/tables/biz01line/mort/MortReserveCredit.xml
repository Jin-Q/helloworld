		<Field id="issue_area" columnName="issue_area" cnname="开证行所在国家或地区" length="100" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="remark" columnName="remark" cnname="备注" length="1000" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="sys_update_date" columnName="sys_update_date" cnname="更新日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="handling_user" columnName="handling_user" cnname="更新人" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="issue_amt" columnName="issue_amt" cnname="开证金额（元）" length="16" JDBCType="NUMBER" canBeNull="true"/>
