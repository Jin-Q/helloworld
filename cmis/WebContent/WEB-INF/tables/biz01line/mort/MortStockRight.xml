		<Field id="buy_price" columnName="buy_price" cnname="购入价格" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="buy_date" columnName="buy_date" cnname="购入日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="memo" columnName="memo" cnname="备注" length="1000" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="sys_update_time" columnName="sys_update_time" cnname="系统更新时间" length="10" JDBCType="CHAR" canBeNull="true"/>
	</TableModel>
</MortStockRight.xml>