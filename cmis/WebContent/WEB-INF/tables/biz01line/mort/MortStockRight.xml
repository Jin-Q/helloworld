		<Field id="guaranty_no" columnName="guaranty_no" cnname="押品编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="stock_right_type" columnName="stock_right_type" cnname="股权类型//" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="issue_org_name" columnName="issue_org_name" cnname="发行单位" length="100" JDBCType="VARCHAR2" canBeNull="true"/>
