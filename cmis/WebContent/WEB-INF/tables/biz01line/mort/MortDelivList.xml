<?xml version="1.0" encoding="UTF-8" ?>


<MortDelivList.xml>
	<TableModel id="MortDelivList" cnname="提货清单" dbTableName="mort_deliv_list">
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="cargo_id" columnName="cargo_id" cnname="货物编号" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="guaranty_no" columnName="guaranty_no" cnname="押品编号" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="guaranty_catalog" columnName="guaranty_catalog" cnname="押品所属目录" length="60" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="storage_qnt" columnName="storage_qnt" cnname="库存数量" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="storage_value" columnName="storage_value" cnname="库存价值" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="deliv_qnt" columnName="deliv_qnt" cnname="提货数量" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="deliv_value" columnName="deliv_value" cnname="提货价值" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="memo" columnName="memo" cnname="备注" length="600" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="cargo_status" columnName="cargo_status" cnname="状态" length="2" JDBCType="CHAR" canBeNull="true"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="reg_date" columnName="reg_date" cnname="登记日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="surplus_qnt" columnName="surplus_qnt" cnname="剩余数量" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="surplus_value" columnName="surplus_value" cnname="剩余价值" length="20" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="deliv_date" columnName="deliv_date" cnname="提货日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="op_type" columnName="op_type" cnname="操作类型（1--货物置换，2--保证金提货）" length="2" JDBCType="VARCHAR2"/>
		<Field id="re_serno" columnName="re_serno" cnname="关联业务流水号" length="40" JDBCType="VARCHAR2"/>
	</TableModel>
</MortDelivList.xml>