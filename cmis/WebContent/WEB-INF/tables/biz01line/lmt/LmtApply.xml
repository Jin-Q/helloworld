<?xml version="1.0" encoding="UTF-8" ?>


<LmtApply.xml>
	<TableModel id="LmtApply" cnname="单一法人授信申请/变更表" dbTableName="lmt_apply">
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="app_type" columnName="app_type" cnname="申请类型：新增/变更/冻结/解冻" length="2" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="cus_id" columnName="cus_id" cnname="客户码" length="30" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="biz_type" columnName="biz_type" cnname="授信业务类型 ：内部授信/公开授信" length="2" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="cur_type" columnName="cur_type" cnname="授信币种" length="3" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="crd_totl_amt" columnName="crd_totl_amt" cnname="授信总额" length="16" JDBCType="NUMBER" canBeNull="false"/>
		<Field id="crd_cir_amt" columnName="crd_cir_amt" cnname="循环授信敞口" length="16" JDBCType="NUMBER" canBeNull="false"/>
		<Field id="crd_one_amt" columnName="crd_one_amt" cnname="一次性授信敞口" length="16" JDBCType="NUMBER" canBeNull="false"/>
		<Field id="over_date" columnName="over_date" cnname="办结日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="flow_type" columnName="flow_type" cnname="流程类型：正常流程/简短流程" length="2" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="memo" columnName="memo" cnname="备注" length="200" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="manager_id" columnName="manager_id" cnname="责任人" length="20" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="manager_br_id" columnName="manager_br_id" cnname="责任机构" length="20" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="approve_status" columnName="approve_status" cnname="申请状态" length="3" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="agr_no" columnName="agr_no" cnname="协议编号" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="org_crd_totl_amt" columnName="org_crd_totl_amt" cnname="原授信总额度" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="org_crd_cir_amt" columnName="org_crd_cir_amt" cnname="原循环授信额度" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="org_crd_one_amt" columnName="org_crd_one_amt" cnname="原一次性授信额度" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="grp_serno" columnName="grp_serno" cnname="集团授信编号" length="40" JDBCType="VARCHAR2"/>
		<Field id="app_date" columnName="app_date" cnname="申请日期" length="10" JDBCType="VARCHAR2"/>
		<Field id="lrisk_type" columnName="lrisk_type" cnname="低风险类型" length="10" JDBCType="VARCHAR2"/>
		<Field id="lmt_type" columnName="lmt_type" cnname="授信类别（区分条线）" length="10" JDBCType="VARCHAR2"/>
		<Field id="is_overdue" columnName="is_overdue" cnname="逾期或欠息，垫款" length="2" JDBCType="VARCHAR2"/>
	</TableModel>
</LmtApply.xml>