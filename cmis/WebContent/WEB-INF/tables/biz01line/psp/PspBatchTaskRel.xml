		<Field id="task_type" columnName="task_type" cnname="任务类型（STD_ZB_TASK_TYPE）" length="2" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="check_freq" columnName="check_freq" cnname="检查频率" length="2" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="batch_task_type" columnName="batch_task_type" cnname="批量任务类型" length="2" JDBCType="VARCHAR2"/>
