		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="30" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="type" columnName="type" cnname="操作类型（1--应收账款，2--保理）" length="2" JDBCType="VARCHAR2"/>
	</TableModel>
</IqpBuscontInfo.xml>