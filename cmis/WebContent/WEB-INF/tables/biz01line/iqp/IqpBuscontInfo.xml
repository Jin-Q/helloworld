		<Field id="tcont_amt" columnName="tcont_amt" cnname="贸易合同金额" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="start_date" columnName="start_date" cnname="合同起始日期" length="10" JDBCType="CHAR" canBeNull="true"/>
		<Field id="end_date" columnName="end_date" cnname="合同到期日期" length="10" JDBCType="CHAR" canBeNull="true"/>
