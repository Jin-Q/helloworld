		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="oper_type" columnName="oper_type" cnname="操作类型：1-准入 2-退出 " length="1" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="model" columnName="model" cnname="型号" length="40" JDBCType="VARCHAR2"/>
