<?xml version="1.0" encoding="UTF-8" ?>


<IqpAppDepotAgr.xml>
	<TableModel id="IqpAppDepotAgr" cnname="保兑仓协议申请表" dbTableName="iqp_app_depot_agr">
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="depot_agr_no" isPK="true" columnName="depot_agr_no" cnname="协议编号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="cus_id" columnName="cus_id" cnname="借款人客户码" length="30" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="psale_cont" columnName="psale_cont" cnname="购销合同" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="desgoods_plan_no" columnName="desgoods_plan_no" cnname="订货计划" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="fst_bail_perc" columnName="fst_bail_perc" cnname="首次保证金比例" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="contacc_freq_unit" columnName="contacc_freq_unit" cnname="对账频率单位" length="2" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="contacc_freq" columnName="contacc_freq" cnname="对账频率" length="10" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="contacc_mode" columnName="contacc_mode" cnname="对账方式" length="2" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="start_date" columnName="start_date" cnname="协议起始日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="end_date" columnName="end_date" cnname="协议到期日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="memo" columnName="memo" cnname="备注" length="500" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="fst_deliv_agreed" columnName="fst_deliv_agreed" cnname="首次提货约定" length="2" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="agreed_rate" columnName="agreed_rate" cnname="约定比率" length="10" JDBCType="NUMBER" canBeNull="true"/>
	    <Field id="status" columnName="status" cnname="生效状态0失效  1生效 STD_ZB_STATUS" length="1" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="cont_status" columnName="cont_status" cnname="协议状态 0原有 1新增 STD_ZB_NET_STATUS" length="1" JDBCType="VARCHAR2" canBeNull="true"/>
	</TableModel>
</IqpAppDepotAgr.xml>