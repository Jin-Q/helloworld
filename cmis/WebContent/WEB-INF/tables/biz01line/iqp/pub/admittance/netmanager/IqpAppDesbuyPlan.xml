<?xml version="1.0" encoding="UTF-8" ?>


<IqpAppDesbuyPlan.xml>
	<TableModel id="IqpAppDesbuyPlan" cnname="订货计划申请表" dbTableName="iqp_app_desbuy_plan">
		<Field id="serno" isPK="true" columnName="serno" cnname="业务编号" length="40" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="desgoods_plan_no" isPK="true" columnName="desgoods_plan_no" cnname="订货流水号" length="40" JDBCType="VARCHAR2" canBeNull="false"/>
		<Field id="cus_id" columnName="cus_id" cnname="客户码" length="30" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="commo_name" columnName="commo_name" cnname="商品名称" length="100" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="desbuy_qnt" columnName="desbuy_qnt" cnname="订购数量" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="desbuy_qnt_unit" columnName="desbuy_qnt_unit" cnname="订购数量单位" length="2" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="desbuy_unit_price" columnName="desbuy_unit_price" cnname="订购单价（元）" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="desbuy_total" columnName="desbuy_total" cnname="订购总价（元）" length="16" JDBCType="NUMBER" canBeNull="true"/>
		<Field id="start_date" columnName="start_date" cnname="起始日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="end_date" columnName="end_date" cnname="到期日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="fore_disp_date" columnName="fore_disp_date" cnname="预计发货日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="memo" columnName="memo" cnname="备注" length="500" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="for_manuf" columnName="for_manuf" cnname="供货商客户码" length="30" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="status" columnName="status" cnname="生效状态0失效  1生效 STD_ZB_STATUS" length="1" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="cont_status" columnName="cont_status" cnname="协议状态 0原有 1新增 STD_ZB_NET_STATUS" length="1" JDBCType="VARCHAR2" canBeNull="true"/>
	</TableModel>
</IqpAppDesbuyPlan.xml>