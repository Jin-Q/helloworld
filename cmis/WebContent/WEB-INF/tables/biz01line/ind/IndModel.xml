<?xml version="1.0" encoding="UTF-8" ?>


<IndModel.xml>
	<TableModel id="IndModel" cnname="模型信息" dbTableName="ind_model">
		<Field id="model_no" isPK="true" columnName="model_no" cnname="模型编号" length="8" JDBCType="CHAR" canBeNull="false"/>
		<Field id="model_name" columnName="model_name" cnname="模型名称" length="60" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="rating_rules" columnName="rating_rules" cnname="模型评分规则" length="100" JDBCType="VARCHAR2" canBeNull="true"/>
		<Field id="remark" columnName="remark" cnname="备注" length="20" JDBCType="VARCHAR2" canBeNull="true"/>
		<One2MultiRef toField="model_no" refModelId="IndModelGroup" fromField="model_no"/>
		<Field id="input_id" columnName="input_id" cnname="登记人" length="20" JDBCType="VARCHAR2"/>
		<Field id="input_date" columnName="input_date" cnname="登记日期" length="10" JDBCType="VARCHAR2"/>
		<Field id="input_br_id" columnName="input_br_id" cnname="登记机构" length="20" JDBCType="VARCHAR2"/>
		<Field id="ceiling_limit" columnName="ceiling_limit" cnname="得分上限" length="16" JDBCType="NUMBER"/>
		<Field id="lower_limit" columnName="lower_limit" cnname="得分下限" length="16" JDBCType="NUMBER"/>
		<Field id="biz_belg" columnName="biz_belg" cnname="业务归属" length="30" JDBCType="VARCHAR2"/>
	</TableModel>
</IndModel.xml>