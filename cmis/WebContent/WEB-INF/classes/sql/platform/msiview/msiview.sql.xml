<?xml version="1.0" encoding="UTF-8"?>
<S>

<!-- s_msi_class_view插入数据  -->
<SQL id="insertMsiCalssView" parameterClass="java.lang.String" valueClass ="java.util.HashMap" resultClass="java.lang.String" onlyReturnFirst="true">
  <INSERT>
  <![CDATA[
  	insert into s_msi_class_view
  		(modual_id,modual_name,msi_class_id,msi_class_desc,msi_class_name,msi_class_example)
  	values
  		(${modual_id},${modual_name},${msi_class_id},${msi_class_desc},${msi_class_name},${msi_class_example})
		
  ]]>
  </INSERT> 
</SQL>

<!-- insertMsiMethodView插入数据  -->
<SQL id="insertMsiMethodView" parameterClass="java.lang.String" valueClass ="java.util.HashMap" resultClass="java.lang.String" onlyReturnFirst="true">
  <INSERT>
  <![CDATA[
  	insert into s_msi_method_view
  		(msi_class_id,msi_method_id,msi_method_name,msi_method_desc,msi_method_param_in,
						msi_method_param_out,msi_method_type,msi_method_example)
	values
		(${msi_class_id},${msi_method_id},${msi_method_name},${msi_method_desc},${msi_method_param_in},
						${msi_method_param_out},${msi_method_type},${msi_method_example})
		
  ]]>
  </INSERT> 
</SQL>

<!-- 清空数据 -->
<SQL id="deleteAllMsiCalssView" parameterClass="java.lang.String" valueClass ="java.util.HashMap" resultClass="java.lang.String" onlyReturnFirst="true">
  <DELETE>
  <![CDATA[
		delete from s_msi_class_view
  ]]>
  </DELETE> 
</SQL>

<!-- 清空数据 -->
<SQL id="deleteAllMsiMethodView" parameterClass="java.lang.String" valueClass ="java.util.HashMap" resultClass="java.lang.String" onlyReturnFirst="true">
  <DELETE>
  <![CDATA[
		delete from s_msi_method_view
  ]]>
  </DELETE> 
</SQL>


<!-- 查询MSI中注册的所有模块 -->
<SQL id="queryAllMsiModual" parameterClass="java.lang.String" resultClass="java.util.HashMap" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select distinct modual_id as id, modual_name as name from s_msi_class_view 
  ]]>
  </SELECT> 
</SQL>

<!-- 根据模块ID查询MSI中注册的服务集 -->
<SQL id="queryClassByModualId" parameterClass="java.lang.String" resultClass="java.util.HashMap" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select msi_class_id as id , msi_class_desc as name from s_msi_class_view where modual_id=${_SIG_VALUE} 
  ]]>
  </SELECT> 
</SQL>

<!-- 根据服务集ID查询MSI中注册的服务 -->
<SQL id="queryMethodByClassId" parameterClass="java.lang.String" resultClass="java.util.HashMap" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select msi_method_id as id ,msi_method_desc as name from s_msi_method_view where msi_class_id=${_SIG_VALUE} 
  ]]>
  </SELECT> 
</SQL>

<!-- 根据服务集ID查询服务集信息 -->
<SQL id="queryClassViewByClassId" parameterClass="java.lang.String" resultClass="com.ecc.emp.data.KeyedCollection" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
	select * from s_msi_class_view where msi_class_id = ${_SIG_VALUE} 
  ]]>
  </SELECT>
</SQL>

<!-- 根据服务ID查询服务信息-->
<SQL id="queryMethodByMethodId" parameterClass="java.lang.String" resultClass="com.ecc.emp.data.KeyedCollection" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select * from s_msi_method_view where msi_method_id=${_SIG_VALUE} 
  ]]>
  </SELECT> 
</SQL>

<!-- 根据服务ID查询服务集信息-->
<SQL id="queryClassByMethodId" parameterClass="java.lang.String" resultClass="com.ecc.emp.data.KeyedCollection" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select a.* from s_msi_class_view a ,s_msi_method_view b where a.msi_class_id = b.msi_class_id and b.msi_method_id=${_SIG_VALUE} 
  ]]>
  </SELECT> 
</SQL>


</S>