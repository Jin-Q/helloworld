<!-- 根据机构编号，查询机构的级别，例如：01总行 -->
<SQL id="queryOrgLevel" parameterClass="java.lang.String" resultClass="java.lang.String" onlyReturnFirst="true">
  <SELECT>
  <![CDATA[
	select orgzdlevel FROM  s_org WHERE organno = ${_SIG_VALUE} 
  ]]>
  </SELECT>
</SQL>
<!-- 根据机构号码的上级机构，查询本机机构的机构号码 -->
<SQL id="queryorgNo" parameterClass="java.lang.String" resultClass="com.ecc.emp.data.KeyedCollection" onlyReturnFirst="true">
  <SELECT>
  <![CDATA[
	select * FROM  s_org WHERE suporganno = ${_SIG_VALUE} and orgzdlevel='04'
  ]]>
  </SELECT>
</SQL>
<!--  根据分行的大额行号编号，查询该机构对应的营业部信息-->
<SQL id="queryOrgMsg" parameterClass="java.lang.String" resultClass="com.ecc.emp.data.KeyedCollection" onlyReturnFirst="true">
  <SELECT>
  <![CDATA[
	select * FROM  s_org WHERE suporganno = (select organno from s_org where bank_srrno= ${_SIG_VALUE}) and orgzdlevel='04'
  ]]>
  </SELECT>
</SQL>
<!-- 根据大额行号编号，查询该机构的级别，支行，分行还是总行 -->
<SQL id="queryOrganlevel" parameterClass="java.lang.String" resultClass="java.lang.String" onlyReturnFirst="true">
  <SELECT>
  <![CDATA[
	select orgzdlevel from s_org where bank_srrno = ${_SIG_VALUE} 
  ]]>
  </SELECT>
</SQL>

<!-- 根据岗位编号获取操作员号 -->
<SQL id="getDutyUserNO" parameterClass="java.lang.String" resultClass="java.lang.String" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
	 select actorno from s_dutyuser where dutyno=${_SIG_VALUE}
  ]]>
  </SELECT>
</SQL>

<!-- 根据机构号获取对应的机构用户 -->
<SQL id="getOrgUserNo" parameterClass="java.lang.String" resultClass="java.lang.String" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
	 select ACTORNO from s_deptuser where organno=${_SIG_VALUE}
  ]]>
  </SELECT>
</SQL>

<!-- 根据团队编号获取团队人员 -->
<SQL id="getTeamUsers" parameterClass="java.lang.String" resultClass="java.lang.String" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[ 
	 select ACTORNO from s_userteam where TEAMNO=${_SIG_VALUE}
  ]]>
  </SELECT>
</SQL>
