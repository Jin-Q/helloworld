<SQL id="deleActionRole"  parameterClass="com.ecc.emp.data.KeyedCollection"  >
  <DELETE>
  <![CDATA[
  delete from S_ROLERIGHT where RESOURCEID=${resourceid} and ACTID=${actid}
  ]]>
  </DELETE>
</SQL>
<!-- 删除角色权限 20120426周凤雷 -->
<SQL id="deleteRoleRight"  parameterClass="java.lang.String">
  <DELETE>
  <![CDATA[
  delete from S_ROLERIGHT where ROLENO=${_SIG_VALUE}
  ]]>
  </DELETE>
</SQL>
<!-- 删除角色用户20120426周凤雷 -->
<SQL id="deleteRoleUser"  parameterClass="java.lang.String"  >
