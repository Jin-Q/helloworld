  </SQL>
<!-- 批量插入执行操作 -->
<SQL id="batchInsertRscTaskInfoSubHis" parameterClass="java.util.HashMap">
  <INSERT>
  <![CDATA[
  	insert into RSC_TASK_INFO_SUB_HIS(
          pk_id,
                serno,
                identy_duty,
                class_adjust_rst,
                remark,
                class_date
               )values(
           ${pk_id},
                ${serno},
                ${identy_duty},
                ${class_adjust_rst},
                ${remark},
                ${class_date}
              )]]>
  </INSERT>
</SQL>
