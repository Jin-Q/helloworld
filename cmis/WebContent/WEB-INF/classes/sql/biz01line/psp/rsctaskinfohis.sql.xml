<S>
<!-- 获得系统中所有数据集合,快捷查询，字段间关系为and，查询方式为精确查询，需与前端like一致 -->
<SQL id="queryRscTaskInfoHisLike" parameterClass="java.util.HashMap" resultClass="com.yucheng.cmis.psp.rectask.domain.RscTaskInfoHis" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select * from RSC_TASK_INFO_HIS where 1=1    ${condition1} ${condition2} ${condition3} ${condition4}	    ]]>
  </SELECT>
   <OPT_CONDITION id="condition1" relationType="and">
	  <![CDATA[
	  serno = ${serno} 
	  ]]> 
  </OPT_CONDITION>
   <OPT_CONDITION id="condition2" relationType="and">
	  <![CDATA[
	  bill_no = ${bill_no} 
	  ]]> 
  </OPT_CONDITION>
  <OPT_CONDITION id="condition3" relationType="and">
	  <![CDATA[
	  cus_id = ${cus_id} 
	  ]]> 
  </OPT_CONDITION>
  <OPT_CONDITION id="condition4" relationType="and">
	  <![CDATA[
	  class_date = ${class_date} 
	  ]]> 
  </OPT_CONDITION>
  </SQL>

<!-- 列表的快捷查询，字段间的关系为OR，查询方式为模糊查询，需与前端like一致 -->
<SQL id="queryRscTaskInfoHisLikeForQuick" parameterClass="java.util.HashMap" resultClass="com.yucheng.cmis.psp.rectask.domain.RscTaskInfoHis" onlyReturnFirst="false">
  <SELECT>
  <![CDATA[
  	select * from RSC_TASK_INFO_HIS where     ${condition1} ${condition2} ${condition3}	 	    ]]>
  </SELECT>
  <CONDI_GROUP defaultCondition='1=1'>
   <OPT_CONDITION id="condition1" relationType="or">
	  <![CDATA[
	  serno = ${serno} 
	  ]]> 
  </OPT_CONDITION>
   <OPT_CONDITION id="condition2" relationType="or">
	  <![CDATA[
	  bill_no = ${bill_no} 
	  ]]> 
  </OPT_CONDITION>
  <OPT_CONDITION id="condition3" relationType="or">
	  <![CDATA[
	  cus_id = ${cus_id} 
	  ]]> 
  </OPT_CONDITION>
    </CONDI_GROUP>
</SQL>

