如果
	$IN_保证金占比 = '在我行存入保证金占其在我行担保额的10%（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_保证金占比 = '在我行存入保证金占其在我行担保额的10%以下'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
