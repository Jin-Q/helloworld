	$OUT_得分 := 'A';
}否则{
如果
	$IN_有专业经验的人员占比 = '具有银行信贷工作经验的人员占全体员工的比例在30%（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_有专业经验的人员占比 = '具有银行信贷工作经验的人员占全体员工的比例在30%以下'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03014" salience="1" isolation="yes">如果 
	$IN_核心部门管理人员稳定度 = '业务营销、风险控制、财务管理核心部门管理人员在该企业平均工作年限为5年（含）以上'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_核心部门管理人员稳定度 = '业务营销、风险控制、财务管理核心部门管理人员在该企业平均工作年限2年（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_核心部门管理人员稳定度 = '业务营销、风险控制、财务管理核心部门管理人员在该企业平均工作年限小于2年'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03015" salience="1" isolation="yes">如果 
	$IN_员工稳定度 = '员工在该企业平均工作年限为3年（含）以上'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_员工稳定度 = '员工在该企业平均工作年限2年（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_员工稳定度 = '员工在该企业平均工作年限小于2年'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03016" salience="1" isolation="yes">如果 
	$IN_对投保客户的风险评估水平 = '水平较好'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_对投保客户的风险评估水平 = '水平一般'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_对投保客户的风险评估水平 = '水平较差'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03017" salience="1" isolation="yes">如果 
	$IN_集体决策制度 = '建立了有效的集体决策制度，上会项目通过率在80%以下'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_集体决策制度 = '建立了集体决策体制，上会项目通过率在80%（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_集体决策制度 = '未建立集体决策体制或虽建立集体决策体制，但未得到有效贯彻，形同虚设，上会项目通过率在90%（含）以上'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03018" salience="1" isolation="yes">如果 
	$IN_保后管理 = '至少每季度一次的客户风险检查，且保后管理档案齐备'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_保后管理 = '其他'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_保后管理 = '频率少于每季度一次的不定期检查，或虽有检查，但保后管理档案混乱'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03019" salience="1" isolation="yes">如果 
	$IN_投保客户提供反担保情况 = '投保客户提供反担保措施充足，能有效防范风险'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_投保客户提供反担保情况 = '其他'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_投保客户提供反担保情况 = '投保客户提供反担保措施流于形式，很难有效防范风险'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03020" salience="1" isolation="yes">如果 
	$IN_内部信用评级体系 = '担保机构内有客户信用评级打分卡，并严格贯彻执行，评分结果作为政策优惠和客户准入的关键参考因素'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_内部信用评级体系 = '担保机构内有客户信用评级打分卡，执行效果一般，在客户准入方面起到了一定的辅助作用'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_内部信用评级体系 = '担保机构内有客户信用评级打分卡，但执行效果较差，或根本没有客户信用评级打分卡'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03021" salience="1" isolation="yes">如果 
	$IN_行业间的分散程度 = '投保客户很好的分散在不同的行业'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_行业间的分散程度 = '投保客户行业间的相关性比较适度'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_行业间的分散程度 = '投保客户较为集中于某个或某类行业'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03022" salience="1" isolation="yes">如果 
	$IN_长期投资占比 &gt;= 30
那么{
	$OUT_得分 := 'E';
}否则{
如果
	$IN_长期投资占比 &gt;= 15
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_长期投资占比 &lt; 15
那么{
	$OUT_得分 := 'A';
}
}
}</rule>
<rule name="Norm03023" salience="1" isolation="yes">如果 
	$IN_投资的风险状况 &gt;= 40
那么{
	$OUT_得分 := 'E';
}否则{
如果
	$IN_投资的风险状况 &gt;= 20
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_投资的风险状况 &lt; 20
那么{
	$OUT_得分 := 'A';
}
}
}</rule>
<rule name="Norm03024" salience="1" isolation="yes">如果 
	$IN_投资的风险回报情况 = '机构的投资回报率长期保持在一个稳定的范围，过去3年中任何2年的投资回报率之间的差距都保持在5个百分点之内'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_投资的风险回报情况 = '机构的投资回报率有一定的波动，但长期保持在一个较为稳定的范围，过去3年中任何2年的投资回报率之间的差距都保持在10个百分点之内'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_投资的风险回报情况 = '机构的投资回报率波动很大，过去3年中有超过2年的投资回报率之间的差距在10个百分点以上'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03025" salience="1" isolation="yes">如果 
	$IN_保证金占比 = '在我行存入保证金占其在我行担保额的20%（含）以上，或愿意为其在我行的担保贷款存入20%（含）以上的保证金'
那么{
	$OUT_得分 := 'A';
