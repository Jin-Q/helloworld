<rule name="Norm03004" salience="1" isolation="yes">如果 
	$IN_股东实力 = '控股股东为市、区级财政部门'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_股东实力 = '最大的3家股东中至少有1家属于大中型国有控股企业或市级财政部门，且该股东派驻高级管理人员参与公司经营决策'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_股东实力 = '其他'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03001" salience="1" isolation="yes">如果 
	$IN_在保客户数量 &gt;= 100
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_在保客户数量 &gt;= 50
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_在保客户数量 &lt; 50
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03002" salience="1" isolation="yes">如果 
	$IN_合作银行数量 &gt;= 6
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_合作银行数量 &gt;= 3
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_合作银行数量 &lt; 3
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03003" salience="1" isolation="yes">如果 
	$IN_政策性补贴占比 = '全年所有补贴收入占总收入的10%（含）以上。补贴包括一般风险准备金补贴、代偿损失补贴、保费收入差价补贴、营业税减免、对鼓励民间出资的财政性奖励等'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_政策性补贴占比 = '全年所有补贴收入占总收入的2%（含）以上。补贴种类见上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_政策性补贴占比 = '全年所有补贴收入占总收入的比例小于2%。补贴种类见上'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03005" salience="1" isolation="yes">如果 
	$IN_股东稳定性 = '最大的3家股东3年内未发生变更'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_股东稳定性 = '其他'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_股东稳定性 = '最大的3家股东3年内发生了2次（含）以上变更'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03006" salience="1" isolation="yes">如果 
	$IN_担保基金的补充机制 = '建立了稳定可靠的资金补偿机制，并有效实施，现有股东或地方政府能根据业务发展和市场需求每年补充一定比例的风险准备金'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_担保基金的补充机制 = '建立了风险准备金的补偿机制，能够有效实施，每年能按业务规模提取一定比例的风险准备金。'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_担保基金的补充机制 = '未建立风险准备金的补偿机制'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03007" salience="1" isolation="yes">如果 
	$IN_担保机构正常开展业务的时间 = '业务开展历史在5年（含）以上'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_担保机构正常开展业务的时间 = '业务开展历史在3年（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_担保机构正常开展业务的时间 = '业务开展历史在3年以下'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03008" salience="1" isolation="yes">如果 
	$IN_持续经营能力 = '该机构开业以来一直在持续经营，保持较为稳定的业务量和净利润的增长速度'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_持续经营能力 = '该机构一直在保持经营，每年的业务量和净利润都没有较大的变化，或者历年的业务量或利润水平略有波动'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_持续经营能力 = '该机构有间断经营或业务规模或净利润显著下降的记录'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03009" salience="1" isolation="yes">如果 
	$IN_内部制度流程的完整有效性 = '具有完整的覆盖全部业务环节的内部规章制度和流程（包括对投保企业的风险评估、担保评审、对在保企业或个人的风险监控、代偿回收等）等的规章制度或流程'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_内部制度流程的完整有效性 = '具有覆盖全部主要业务环节（包括对投保企业的风险评估、担保评审、对在保企业或个人的风险监控、代偿回收等）等的规章制度或流程'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_内部制度流程的完整有效性 = '对于主要业务环节（包括对投保企业的风险评估、担保评审、对在保企业或个人的风险监控、代偿回收等），缺少明确的规章制度或流程'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03010" salience="1" isolation="yes">如果 
	$IN_担保业务的风险偏好 = '较为保守。在客户的风险评估和风险缓释手段等方面特别谨慎，只选择违约可能性很低或反担保能力很强的客户提供担保。'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_担保业务的风险偏好 = '一般。在客户的风险评估和风险缓释手段尚采用适中的标准'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_担保业务的风险偏好 = '较为激进。在客户的风险评估和风险缓释手段尚采用较为宽松的标准'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03011" salience="1" isolation="yes">如果 
	$IN_非担保业务的风险偏好 = '较为保守，只接受风险很低的国债、金融债券等的投资'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_非担保业务的风险偏好 = '一般，介于保守和激进之间'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_非担保业务的风险偏好 = '较为激进，愿意进行风险较高的股票、基金等的投资'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03012" salience="1" isolation="yes">如果 
	$IN_高级管理层的经验和能力 = '普遍具有良好的行业背景和较强的管理能力'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_高级管理层的经验和能力 = '大部分高级管理人员具有一定的行业背景和管理能力'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_高级管理层的经验和能力 = '普遍缺乏行业背景或管理经验或能力'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm03013" salience="1" isolation="yes">如果 
	$IN_有专业经验的人员占比 = '具有银行信贷工作经验的人员占全体员工的比例在50%（含）以上'
