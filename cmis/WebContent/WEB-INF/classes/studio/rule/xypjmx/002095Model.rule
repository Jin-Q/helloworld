<rule name="Norm02017" salience="1" isolation="yes">如果 
	$IN_资金封闭运行情况 = '实行全封闭贷款管理，签订了《资金监管协议》，有严格的资金监控措施，能够确保大额资金支出用于项目建设'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_资金封闭运行情况 = '实行全封闭贷款管理，签订了《资金监管协议》，有一定的资金监控措施，基本能够保证大额资金支出用于项目建设'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_资金封闭运行情况 = '虽签订了《资金监管协议》，但缺乏有效的监控措施，大额资金支出难以控制，或未签订《资金监管协议》'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm02018" salience="1" isolation="yes">如果 
	$IN_抵押情况 = '我行抵押贷款占比为100%，客户在我行的贷款全部为抵押贷款'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_抵押情况 = '我行抵押贷款占比小于100%，且客户在我行的抵押贷款占比大于或等于其总的抵押贷款占比'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_抵押情况 = '我行抵押贷款占比小于100%，且客户在我行的抵押贷款占比小于其总的抵押贷款占比'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
