<?xml version="1.0" encoding="UTF-8"?>
<rule-set name="Model002095Rule">
<rule name="Model002095" salience="1" isolation="yes">如果 1=1
那么{
   $OUT_评级等级 := 'AAA';
}</rule>
<parameters>
<input name="IN_总得分" type="float" />
<output name="OUT_评级等级" type="String" />
<output name="OUT_得分" type="string" />
<input name="IN_实收资本" type="float" />
<input name="IN_项目自有资金占比" type="string" />
<input name="IN_主要负责人从业经验" type="string" />
<input name="IN_项目位置" type="string" />
<input name="IN_周边配套设施" type="string" />
<input name="IN_建设进度" type="string" />
<input name="IN_施工总承包企业资质" type="string" />
<input name="IN_项目周边楼盘销售或出租率" type="string" />
<input name="IN_项目价格定位" type="string" />
<input name="IN_净现值" type="string" />
<input name="IN_内部收益率" type="string" />
<input name="IN_贷款偿还期" type="string" />
<input name="IN_资金封闭运行情况" type="string" />
<input name="IN_抵押情况" type="string" />
<input name="IN_已经抵押给银行的资产占比" type="float" />
<input name="IN_销售回款情况" type="string" />
<input name="IN_销售还款比例" type="string" />
<input name="IN_项目按揭情况" type="string" />
<input name="IN_主要管理人员工作经验" type="string" />
<input name="IN_主要股东实力" type="string" />
<input name="IN_项目类型" type="float" />
<input name="IN_已投入资金占比" type="float" />
</parameters>
<rule name="Norm02001" salience="1" isolation="yes">如果 
	$IN_实收资本 &gt;= 50000000
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_实收资本 &gt;= 30000000
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_实收资本 &gt;= 10000000
那么{
	$OUT_得分 := 'D';
}否则{
如果
	$IN_实收资本 &lt; 10000000
那么{
	$OUT_得分 := 'E';
}
}
}
}</rule>
<rule name="Norm02002" salience="1" isolation="yes">如果 
	$IN_项目自有资金占比 = '50%（含）以上'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_项目自有资金占比  = '45%（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_项目自有资金占比  = '35%（含）以上'
那么{
	$OUT_得分 := 'D';
}否则{
如果
	$IN_项目自有资金占比  = '35%以下'
那么{
	$OUT_得分 := 'E';
}
}
}
}</rule>
<rule name="Norm02003" salience="1" isolation="yes">如果 
	$IN_主要负责人从业经验 = '主要负责人在房地产行业从事管理工作10年以上'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_主要负责人从业经验 = '主要负责人在房地产行业从事管理工作5年（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_主要负责人从业经验 = '主要负责人在房地产行业从事管理工作5年以下'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm02006" salience="1" isolation="yes">如果 
	$IN_项目位置 = '位于城八区经济发展水平较高的区域'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_项目位置 = '位于昌平、通州、顺义近郊区中心城区或亦庄经济开发区'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_项目位置 = '其他'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
<rule name="Norm02007" salience="1" isolation="yes">如果 
