<?xml version="1.0" encoding='ISO-8859-1'?>
<rule-set name="BusinessRulesSample">
<java:import>java.lang.Object</java:import>
<java:import> java.lang.String </java:import>
<java:import> java.util.HashMap </java:import>
<java:import> java.util.Map </java:import>

<java:functions>
	public static void printMap(java.util.HashMap paramMap){
	System.out.println("paramMap is:"+paramMap);
	}
	public static void printMap22(java.util.HashMap paramMap){
	System.out.println("paramMap is:"+paramMap);
	System.out.println("paramMap is222:"+paramMap);
	}
</java:functions>


<rule name="USSTSchoole" salience="200" isolation="no">
<parameter identifier="evo">
	<class>java.util.HashMap</class>
</parameter>
<parameter identifier="sign">
	<class>java.lang.String</class>
</parameter>
<java:condition> evo.get("name").equals("newbies") </java:condition> 
<java:condition> evo.get("tip").equals("USST") </java:condition>
<java:condition> sign.equals("ecc") </java:condition>

<!-- What happens when the business rule is activated -->
<java:consequence> 
    System.out.println("=======match USSTSchoole=========");
	evo.put("school","USSTSchoole");
	evo.put("name","ljy");
	printMap(evo);
</java:consequence>
</rule>

