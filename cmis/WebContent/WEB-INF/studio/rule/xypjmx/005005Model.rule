那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_经营规划评价 = '对未来虽有明确规划，但实施存在一定的不确定性'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_经营规划评价 = '基本没有明确规划或虽有规划但可实施性较差'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
