<rule name="Norm06001" salience="1" isolation="yes">如果 
	$IN_行业经验 = '从事本行经营年限10年（含）以上'
那么{
	$OUT_得分 := 'A';
}否则{
如果
	$IN_行业经验 = '从事本行经营年限3年（含）以上'
那么{
	$OUT_得分 := 'C';
}否则{
如果
	$IN_行业经验 = '从事本行经营年限3年以下'
那么{
	$OUT_得分 := 'E';
}
}
}</rule>
