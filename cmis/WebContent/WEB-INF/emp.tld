<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN" "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>emp</short-name>
	<uri>/WEB-INF/emp.tld</uri>
	<description>
		YuCheng EMP JSP Tag Library - (C) Copyright YuCheng Corp.
		2007
	</description>

	<tag>
		<name>page</name>
		<tag-class>com.ecc.emp.ext.tag.page.EMPExtPage</tag-class>
		<description>整个页面(未完善)</description>
		<attribute>
			<name>autoLoad</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
				是否自动的对页面内的标签进行注册，缺省是true
				如果不自动，则需要手工在onLoad中调用page.renderEmpObjects()进行注册
			</description>
		</attribute>
	</tag>


	<tag>
		<name>url</name>
		<tag-class>com.ecc.emp.web.jsptags.EMPActionTag</tag-class>
		<description>
			在页面上为特定的对象提供URL，如JavaScript中需要提交表单的Action等，
			系统自动与框架配合，添加必要的信息，包括：session跟踪，webContext路径处理等
		</description>
		<attribute>
			<name>action</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>url定义</description>
		</attribute>
		<attribute>
			<name>reqParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>url中需要携带的参数</description>
		</attribute>
	</tag>


	<tag>
		<name>file</name>
		<tag-class>com.ecc.emp.web.jsptags.CTPFileTag</tag-class>
		<description>将相对路径的fileName转化为WebContext下的绝对路径</description>
		<attribute>
			<name>fileName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>相对路径的定义</description>
		</attribute>
		<attribute>
			<name>needLocale</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否需要使用多语言处理，在文件名后加后缀</description>
		</attribute>
	</tag>


	<tag>
		<name>tabGroup</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtTabGroup</tag-class>
		<description>标签页集合</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>惟一标识(与其它标签的ID需要区分)</description>
		</attribute>
		<attribute>
			<name>mainTab</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>主页签，用于载入后缺省显示的页签</description>
		</attribute>
	</tag>

	<!-- EMPExtActTab 配置化TAB页 -->
	<tag>
		<name>ExtActTab</name>
		<tag-class>com.yucheng.cmis.biz01line.prd.prdtools.tag.EMPActTab</tag-class>
		<description>自动化标签页</description>
	</tag>
	
	<tag>
		<name>tab</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtTab</tag-class>
		<description>标签页</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>惟一标识(与其它标签的ID需要区分)</description>
		</attribute>
		<attribute>
			<name>label</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>页签上显示的标题</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>该页签所对应的请求url</description>
		</attribute>
		<attribute>
			<name>reqParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>url中需要添加的参数</description>
		</attribute>
		<attribute>
			<name>initial</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否在载入时就对页签进行初始化(发起请求)</description>
		</attribute>
		<attribute>
			<name>needFlush</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否每次点击标题栏都要刷新页签的内容</description>
		</attribute>
	</tag>

	<!-- <empext:relatedSelectGroup/> tag -->
	<tag>
		<name>relatedSelectGroup</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtRelatedSelectGroup</tag-class>
		<description>联动下拉框组</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>惟一标识(与其它标签的ID需要区分)</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>下拉框的值改变时，需要发起请求的url</description>
		</attribute>
		<attribute>
			<name>reqParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>发起的请求中需要携带的参数</description>
		</attribute>
		<attribute>
			<name>needCache</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否对已经获取的下拉框内容进行缓存</description>
		</attribute>
	</tag>

	<!-- <empext:relatedSelect/> tag -->
	<tag>
		<name>relatedSelect</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtRelatedSelect</tag-class>
		<description>联动下拉框</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>对应的下拉框ID(对应于emp:select标签)</description>
		</attribute>
	</tag>


	<tag>
		<name>table</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtDataTable</tag-class>
		<description>数据列表</description>
		<attribute>
			<name>icollName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表所要展现的IndexedCollection数据名称</description>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>整个table标签的样式，缺省是emp_table</description>
		</attribute>
		<attribute>
			<name>cssTitleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>标题栏的样式，缺省是emp_table_title</description>
		</attribute>
		<attribute>
			<name>cssStatisticClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>统计栏的样式，缺省是emp_table_statistic</description>
		</attribute>
		<attribute>
			<name>selectType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表的选择方式：0-不能选择；1-单选；2-多选</description>
		</attribute>
		<attribute>
			<name>needTableTitle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否需要显示标题栏，缺省是需要显示的</description>
		</attribute>
		<attribute>
			<name>editable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表是否可编辑，缺省是不可编辑的</description>
		</attribute>
		<attribute>
			<name>statisticType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表统计的类型</description>
		</attribute>
		<attribute>
			<name>pageMode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表是否分页，缺省是进行分页的。该属性应该与后台的列表查询配合使用</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>分页查询的url</description>
		</attribute>
		<attribute>
			<name>reqParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>分页查询的url中需要携带的参数</description>
		</attribute>
	</tag>

	<tag>
		<name>grouptable</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtDataGroupTable</tag-class>
		<description>可分组的数据列表</description>
		<attribute>
			<name>icollName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表所要展现的IndexedCollection数据名称</description>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>整个table标签的样式，缺省是emp_table</description>
		</attribute>
		<attribute>
			<name>cssTitleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>标题栏的样式，缺省是emp_table_title</description>
		</attribute>
		<attribute>
			<name>cssStatisticClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>统计栏的样式，缺省是emp_table_statistic</description>
		</attribute>
		<attribute>
			<name>selectType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表的选择方式：0-不能选择；1-单选；2-多选</description>
		</attribute>
		<attribute>
			<name>needTableTitle</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否需要显示标题栏，缺省是需要显示的</description>
		</attribute>
		<attribute>
			<name>editable</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表是否可编辑，缺省是不可编辑的</description>
		</attribute>
		<attribute>
			<name>statisticType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表统计的类型</description>
		</attribute>
		<attribute>
			<name>pageMode</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表是否分页，缺省是进行分页的。该属性应该与后台的列表查询配合使用</description>
		</attribute>
		<attribute>
			<name>url</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>分页查询的url</description>
		</attribute>
		<attribute>
			<name>reqParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>分页查询的url中需要携带的参数</description>
		</attribute>
		<attribute>
			<name>groupColumn</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>用于分类的列编号（从1启）</description>
		</attribute>
		
	</tag>
	<tag>
		<name>gridLayout</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtGridLayout</tag-class>
		<description>页面表格布局</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>为每个表格布局注册一个页面dataGroup对象</description>
		</attribute>
		<attribute>
			<name>maxColumn</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>表格布局的最大列数，缺省是两列显示的</description>
		</attribute>
		<attribute>
			<name>title</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>表格布局的标题显示</description>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>表格布局中整个表格的样式，缺省是emp_gridLayout_table</description>
		</attribute>
		<attribute>
			<name>cssTitleClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>表格布局中标题的样式，缺省是emp_gridlayout_title</description>
		</attribute>
	</tag>


	<tag>
		<name>label</name>
		<tag-class>com.ecc.emp.web.jsptags.CTPLabelTag</tag-class>
		<description>显示文本信息，支持多语言</description>
		<attribute>
			<name>text</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>需要显示的文本内容，可以是多语言中的key</description>
		</attribute>
		<attribute>
			<name>CSSClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>显示文本的样式，如果定义了该属性，则使用一个span来包含文本信息</description>
		</attribute>
	</tag>


	<!-- <empext:form/> tag -->
	<tag>
		<name>form</name>
		<tag-class>com.ecc.emp.ext.tag.EMPExtForm</tag-class>
		<description>页面表单</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>method</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>enctype</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>action</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>target</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>onsubmit</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>

	</tag>


	<!-- <empext:text/> tag -->
	<tag>
		<name>text</name>
		<tag-class>com.ecc.emp.ext.tag.field.EMPExtText</tag-class>
		<description>普通的文本输入框</description>
		<attribute>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
			<description>HTML标准属性</description>
		</attribute>
		<attribute>
			<name>label</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据的描述</description>
		</attribute>
		<attribute>
			<name>cssClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>相当于HTML中的class属性，整个SPAN的样式，缺省是emp_field</description>
		</attribute>
		<attribute>
			<name>cssRequiredClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>必输标志(*号)的样式，缺省是emp_field_required</description>
		</attribute>
		<attribute>
			<name>cssErrorClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>出错显示信息的样式，缺省是emp_field_error</description>
		</attribute>
		<attribute>
			<name>cssLabelClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>label部分的样式，只在gridlayout中起作用，缺省是emp_field_label</description>
		</attribute>
		<attribute>
			<name>cssTDClass</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>当前标签所在的td的样式，只在gridlayout和table中起作用，缺省是emp_field_td</description>
		</attribute>
		<attribute>
			<name>defvalue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>数据为空时的缺省值</description>
		</attribute>
		<attribute>
			<name>tabindex</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML Standard Attribute</description>
		</attribute>
		<attribute>
			<name>help</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>tooltip属性</description>
		</attribute>
		<attribute>
			<name>dataType</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>所使用的数据类型</description>
		</attribute>
		<attribute>
			<name>dictname</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>所使用的标准字典数据</description>
		</attribute>
		<attribute>
			<name>required</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否是必输项</description>
		</attribute>
		<attribute>
			<name>hidden</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否隐藏</description>
		</attribute>
		<attribute>
			<name>readonly</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否只读</description>
		</attribute>
		<attribute>
			<name>disabled</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否处于不可用状态</description>
		</attribute>
		<attribute>
			<name>flat</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否只以文本方式显示数值</description>
		</attribute>
		<attribute>
			<name>colSpan</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>在布局组件中所占用的列数</description>
		</attribute>
		<attribute>
			<name>statistic</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>在列表中是否作为统计列</description>
		</attribute>
		<attribute>
			<name>fieldErrorDataName</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>输入校验失败的错误信息数据域名称</description>
		</attribute>
		<attribute>
			<name>onlyControlElement</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>当前标签是否只针对自身的span进行控制(不控制周边的td、label等)</description>
		</attribute>
		<attribute>
			<name>onclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>ondblclick</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onmousedown</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onmouseup</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onmouseover</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onmousemove</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onmouseout</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onkeypress</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>HTML标准事件</description>
		</attribute>
		<attribute>
			<name>onkeyup</name>
