<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 鑾峰彇鎿嶄綔涓婚〉闈� -->
	<action id="querySDicTestList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.ops.dic.sdictest.QuerySDicTestListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="dic/SDicTest/querySDicTestList.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱缂栬緫椤甸潰 -->
	<action id="getSDicTestUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.ops.dic.sdictest.QuerySDicTestDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="dic/SDicTest/updateSDicTestRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 鏇存柊璁板綍 -->
	<action id="updateSDicTestRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.ops.dic.sdictest.UpdateSDicTestRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=querySDicTestList.do" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏌ョ湅椤甸潰 -->
	<action id="getSDicTestViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.ops.dic.sdictest.QuerySDicTestDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="dic/SDicTest/querySDicTestDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏂板椤甸潰 -->
	<action id="getSDicTestAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="dic/SDicTest/addSDicTestRecord.jsp"/>
	</action>
	
	<!-- 鏂板璁板綍 -->
	<action id="addSDicTestRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.ops.dic.sdictest.AddSDicTestRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=querySDicTestList.do" />
		</outputViews>
	</action>	
	
	<!-- 鍒犻櫎璁板綍 -->
	<action id="deleteSDicTestRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.ops.dic.sdictest.DeleteSDicTestRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=querySDicTestList.do" />
		</outputViews>
	</action>
	
	<!-- 鍒嗛〉鏌ヨ璁板綍鍒楄〃 -->
		<action id="pageSDicTestQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.ops.dic.sdictest.QuerySDicTestListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="SDicTestList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



