	<!-- 表格数据获取，带Footer-->
	<action id="getRscTaskInfoSubData" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.rectask.QueryRscTaskInfoSubListOp" />
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="RscTaskInfoSubList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 列表页面 -->
	<action id="getRscTaskInfoSubListPage" type="normal" checkSession="true" >
