<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryPspRepaySrcList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.QueryPspRepaySrcListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/psp/PspRepaySrc/queryPspRepaySrcList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getPspRepaySrcUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.QueryPspRepaySrcDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/psp/PspRepaySrc/updatePspRepaySrcRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updatePspRepaySrcRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.UpdatePspRepaySrcRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getPspRepaySrcViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.QueryPspRepaySrcDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/psp/PspRepaySrc/queryPspRepaySrcDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getPspRepaySrcAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/psp/PspRepaySrc/addPspRepaySrcRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addPspRepaySrcRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.AddPspRepaySrcRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deletePspRepaySrcRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.DeletePspRepaySrcRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pagePspRepaySrcQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.QueryPspRepaySrcListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="PspRepaySrcList">
						<kColl>
						</kColl>
					</iColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 校验借据号是否已经存在 -->
	<action id="checkAccLoanForPspRe" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.psprepaysrc.CheckAccLoanForPspReOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
</actionDefines>



