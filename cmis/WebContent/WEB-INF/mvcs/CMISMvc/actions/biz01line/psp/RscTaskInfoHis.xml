	</action>

	<!-- 修改页面 -->
	<action id="getRscTaskInfoHisUpdatePage" type="normal" checkSession="true" >
		<jspView id="updatePage" url="psp/rectask/rsctaskinfohis/modifyRscTaskInfoHis.jsp"/>
	</action>

	<!-- 详情页面 -->
