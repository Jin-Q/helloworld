	<!-- 表格数据获取，带Footer-->
	<action id="getRscTaskInfoHisData" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.rectask.QueryRscTaskInfoHisListOp"  />
		    <transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="RscTaskInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 列表页面 -->
	<action id="getRscTaskInfoHisListPage" type="normal" checkSession="true" >
	    <flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.rectask.QueryRscTaskInfoHisListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/psp/RecTask/RscTaskInfoHis/listRscTaskInfoHis.jsp" />
		</outputViews>
     </action>
	
	
	<!-- 新增页面 -->
	<action id="getRscTaskInfoHisAddPage" type="normal" checkSession="true" >
