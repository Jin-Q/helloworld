		<outputViews>
			<jspView id="retPage" url="biz01line/psp/PspBatchTaskRel/queryPspBatchTaskList.jsp" />
		</outputViews>
	</action>
	
	<!-- 查询子任务关联分页 -->
	<action id="getPspBatchTaskList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.GetPspBatchTaskListOp"/>
			<transition dest="tabQueryListRes"/>
		</flowInvoker>
		<outputViews>
