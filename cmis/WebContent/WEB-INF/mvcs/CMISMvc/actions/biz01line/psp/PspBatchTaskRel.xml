<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryPspBatchTaskRelList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.QueryPspBatchTaskRelListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/psp/PspBatchTaskRel/queryPspBatchTaskRelList.jsp" />
		</outputViews>
	</action>

	<!-- 获得查看页面 -->
	<action id="getPspBatchTaskRelViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.QueryPspBatchTaskRelDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/psp/PspBatchTaskRel/queryPspBatchTaskRelDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增准备信息页面 -->
	<action id="getPreparePspBatchTaskRelPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.PreparePspBatchTaskRelInfoOp"/>
			<transition dest="prePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="prePage" url="biz01line/psp/PspBatchTaskRel/preparePspBatchTaskRelInfo.jsp"/>
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getPspBatchTaskRelAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.AddPspBatchTaskRelRecordOp"/>
			<transition dest="prePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="prePage" url="biz01line/psp/PspBatchTaskRel/addPspBatchTaskRelRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 获得修改页面 -->
	<action id="getPspBatchTaskRelUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.QueryPspBatchTaskRelDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/psp/PspBatchTaskRel/addPspBatchTaskRelRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 异步保存方法 -->
	<action id="savePspBatchTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.UpdatePspBatchTaskRecordOp"/> 
			<transition dest="savePage"/>
		</flowInvoker>
		<outputViews>
			<view id="savePage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deletePspBatchTaskRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.DeletePspBatchTaskRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
