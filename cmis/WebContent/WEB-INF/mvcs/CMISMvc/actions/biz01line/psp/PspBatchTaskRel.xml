						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获取子任务关联页面 -->
	<action id="queryPspBatchTaskList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.psp.op.pspbatchtaskrel.GetPspBatchTaskListOp"/>
			<transition dest="retPage"/>
