			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/pvp/PvpAssetstrsf/updatePvpAssetstrsfRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updatePvpAssetstrsfRecord" type="normal" checkSession="true" >
