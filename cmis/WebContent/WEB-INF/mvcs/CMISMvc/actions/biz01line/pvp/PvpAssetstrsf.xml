	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryPvpAssetstrsfList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.pvp.op.pvpassetstrsf.QueryPvpAssetstrsfListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/pvp/PvpAssetstrsf/queryPvpAssetstrsfList.jsp" />
		</outputViews>
	</action>
	
	<!--出账查询 add by WS 2013-08-27  -->
