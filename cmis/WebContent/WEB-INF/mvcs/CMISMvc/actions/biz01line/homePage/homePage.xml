			<operation class="com.yucheng.cmis.biz01line.homepage.QueryGadgetResizeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/homePage/queryGadgetResize.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取homePage的工具集 -->
	<action id="queryCustomHomePageGadget" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.homepage.CustomHomePageGadgetOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="gadgetStr" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
