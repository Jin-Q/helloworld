	<!-- 导出成生成excel -->                                                                                                                                                
	<action id="iqpBillDetailListToExcel" type="normal" checkSession="true" >                                                                                         
		<flowInvoker>                                                                                                                                                       
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbilldetail.IqpBillDetailListToExcel"/>                                                               
			<transition dest="retPage"/>                                                                                                                                      
		</flowInvoker>                                                                                                                                                      
		<outputViews>                                                                                                                                                       
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">                                                                                                                                         
 				<excelWorkbook fileName="IqpBillDetailList.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">                                                                    
 					<excelSheet id="1" name="票据信息" isTitle="true" iCollName="IqpBillDetailList" class="com.yucheng.cmis.view.excel.ExcelSheet">                                     
 						<excelColumn dataName="porder_no" name="汇票号码" cellType="string" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                       
 						<excelColumn dataName="bill_isse_date" name="票据签发日" class="com.yucheng.cmis.view.excel.ExcelColumn"/>     
 						<excelColumn dataName="porder_end_date" name="汇票到期日" class="com.yucheng.cmis.view.excel.ExcelColumn"/>   
 						<excelColumn dataName="drft_amt" name="票面金额" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                
 						<excelColumn dataName="isse_name" name="出票/付款人名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>   
 						<excelColumn dataName="pyee_name" name="收款人名称"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>               
 						<excelColumn dataName="aaorg_no" name="承兑人开户行行号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                       
 						<excelColumn dataName="aaorg_name" name="承兑人开户行名称"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                 
 						<excelColumn dataName="aorg_no" name="承兑行行号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                                         
 						<excelColumn dataName="aorg_name" name="承兑行名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                                         
 						<excelColumn dataName="status" name="票据状态" dictName="STD_ZB_DRFT_STATUS" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                                         
 						<excelColumn dataName="dscnt_type" name="贴现状态" dictName="STD_ZB_RPDDSCNT_MODE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                                         
 					</excelSheet>                                                                                                                                                 
 				</excelWorkbook>                                                                                                                                                
 			</view>                                                                                                                                                           
		</outputViews>                                                                                                                                                      
	</action>
