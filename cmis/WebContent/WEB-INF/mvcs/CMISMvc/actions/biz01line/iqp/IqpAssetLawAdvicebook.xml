<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpAssetLawAdvicebookList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.QueryIqpAssetLawAdvicebookListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpAssetLawAdvicebook/queryIqpAssetLawAdvicebookList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpAssetLawAdvicebookUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.QueryIqpAssetLawAdvicebookDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpAssetLawAdvicebook/updateIqpAssetLawAdvicebookRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpAssetLawAdvicebookRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.UpdateIqpAssetLawAdvicebookRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpAssetLawAdvicebookViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.QueryIqpAssetLawAdvicebookDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/IqpAssetLawAdvicebook/queryIqpAssetLawAdvicebookDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpAssetLawAdvicebookAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/iqp/IqpAssetLawAdvicebook/addIqpAssetLawAdvicebookRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpAssetLawAdvicebookRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.AddIqpAssetLawAdvicebookRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpAssetLawAdvicebookRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.DeleteIqpAssetLawAdvicebookRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageIqpAssetLawAdvicebookQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpassetlawadvicebook.QueryIqpAssetLawAdvicebookListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpAssetLawAdvicebookList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



