			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpRpddscnt/queryIqpRpddscntList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取历史操作主页面 -->
