<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpExtensionPvpList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.QueryIqpExtensionPvpListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/extension/IqpExtensionPvp/queryIqpExtensionPvpList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpExtensionPvpUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.QueryIqpExtensionPvpDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/extension/IqpExtensionPvp/updateIqpExtensionPvpRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpExtensionPvpRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.UpdateIqpExtensionPvpRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpExtensionPvpViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.QueryIqpExtensionPvpDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/extension/IqpExtensionPvp/queryIqpExtensionPvpDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 查询历史模块 -->
	<action id="getIqpExtensionPvpViewHis" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.QueryIqpExtensionPvpDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/extension/IqpExtensionPvp/hisIqpExtensionPvpDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpExtensionPvpAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/iqp/extension/IqpExtensionPvp/addIqpExtensionPvpRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpExtensionPvpRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.AddIqpExtensionPvpRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
						<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpExtensionPvpRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.DeleteIqpExtensionPvpRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageIqpExtensionPvpQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.QueryIqpExtensionPvpListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpExtensionPvpList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 取借据编号pop -->
	<action id="extensionAgrNoPop" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.ExtensionAgrNoPop" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/extension/IqpExtensionPvp/extensionAgrNoPop.jsp" />
		</outputViews>
	</action>
	
	<action id="pageExtensionAgrNo" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionpvp.ExtensionAgrNoPop" />
			<transition dest="queryListRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="resultSet">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>