<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpMortValueManaList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/queryIqpMortValueManaList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpMortValueManaUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaDetailOp"/>
			<transition dest="updatePage" condition="($retValue='updatePage')"/>
			<transition dest="isAdjPage" condition="($retValue='isAdjPage')"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/updateIqpMortValueManaRecord.jsp" />
			<jspView id="isAdjPage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/IqpMortValueAdj.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpMortValueManaRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.UpdateIqpMortValueManaRecordOp"/>
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="msg" />
					<field id="flag" />
					<field id="value_no" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpMortValueManaViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/queryIqpMortValueManaDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getMortValueManaViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/queryIqpMortValueManaDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpMortValueManaAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.GetIqpMortValueManaAddPageOp"/>
			<transition dest="addPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="addPage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/updateIqpMortValueManaRecord.jsp"/>
		</outputViews>		
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpMortValueManaRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.AddIqpMortValueManaRecordOp"/>
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="msg" />
					<field id="flag" />
					<field id="value_no" />
				</output>
			</jsonView>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpMortValueManaRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.DeleteIqpMortValueManaRecordOp"/>
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 修改押品目录状态  -->
	<action id="updateValueManaStatus" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.UpdateValueManaStatus"/>
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="msg" />
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageIqpMortValueManaQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpMortValueManaList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 价格变动历史分页  -->
	<action id="pageIqpMortValueAdj" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaDetailOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpMortValueAdjList">
						<kColl>
							<field id="org_valve" />
							<field id="change_valve" />
							<field id="info_sour" />
							<field id="inure_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!--获取押品价格管理Pop -->
	<action id="queryIqpMortValueManaPopList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaPopListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/IqpMortValueMana/queryIqpMortValueManaPopList.jsp" />
		</outputViews>
	</action>
	<!-- 分页查询记录列表（pop） -->
	<action id="pageIqpMortValueManaPopQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.QueryIqpMortValueManaPopListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpMortValueManaList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 异步校验所选择押品目录的类型属性  -->
	<action id="checkAttrType" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpmortvaluemana.CheckAttrTypeOp"/>
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="attr_type" />
				</output>
			</jsonView>
		</outputViews>
	</action>
</actionDefines>



