<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpPsaleContGoodList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.QueryIqpPsaleContGoodListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/netmanager/IqpPsaleContGood/queryIqpPsaleContGoodList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpPsaleContGoodUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.QueryIqpPsaleContGoodDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/netmanager/IqpPsaleContGood/updateIqpPsaleContGoodRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpPsaleContGoodRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.UpdateIqpPsaleContGoodRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryIqpPsaleContGoodList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpPsaleContGoodViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.QueryIqpPsaleContGoodDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/pub/admittance/netmanager/IqpPsaleContGood/queryIqpPsaleContGoodDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpPsaleContGoodAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/iqp/pub/admittance/netmanager/IqpPsaleContGood/addIqpPsaleContGoodRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpPsaleContGoodRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.AddIqpPsaleContGoodRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryIqpPsaleContGoodList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpPsaleContGoodRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.DeleteIqpPsaleContGoodRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryIqpPsaleContGoodList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageIqpPsaleContGoodQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqppsalecontgood.QueryIqpPsaleContGoodListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpPsaleContGoodList">
						<kColl>
							<field id="commo_name" />
							<field id="qnt" />
							<field id="unit_price" />
							<field id="total" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



