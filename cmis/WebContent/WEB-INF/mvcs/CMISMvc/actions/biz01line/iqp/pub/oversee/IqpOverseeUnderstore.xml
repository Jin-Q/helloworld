<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpOverseeUnderstoreList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.QueryIqpOverseeUnderstoreListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/oversee/IqpOverseeUnderstore/queryIqpOverseeUnderstoreList.jsp" />
		</outputViews>
	</action>
	
	<!-- 下属仓库POP框 -->
	<action id="IqpUnderstore4PopList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.GetIqpOverseeUnderstorePageListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/iqp/pub/oversee/IqpOverseeUnderstore/IqpUnderstore4PopList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpOverseeUnderstoreUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.QueryIqpOverseeUnderstoreDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/oversee/IqpOverseeUnderstore/updateIqpOverseeUnderstoreRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpOverseeUnderstoreRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.UpdateIqpOverseeUnderstoreRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpOverseeUnderstoreViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.QueryIqpOverseeUnderstoreDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/pub/oversee/IqpOverseeUnderstore/queryIqpOverseeUnderstoreDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpOverseeUnderstoreAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/iqp/pub/oversee/IqpOverseeUnderstore/addIqpOverseeUnderstoreRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpOverseeUnderstoreRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.AddIqpOverseeUnderstoreRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpOverseeUnderstoreRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.DeleteIqpOverseeUnderstoreRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 下属仓库分页查询记录列表 -->
	<action id="getIqpOverseeUnderstorePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.GetIqpOverseeUnderstorePageListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpOverseeUnderstoreList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageIqpOverseeUnderstoreQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.oversee.iqpoverseeunderstore.QueryIqpOverseeUnderstoreListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpOverseeUnderstoreList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
</actionDefines>