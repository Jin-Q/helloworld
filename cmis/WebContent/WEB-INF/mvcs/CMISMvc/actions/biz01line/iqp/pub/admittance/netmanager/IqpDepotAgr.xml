			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqpdepotagr.UpdateIqpDepotAgrRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpDepotAgrViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.netmanager.iqpdepotagr.QueryIqpDepotAgrDetailOp"/>
