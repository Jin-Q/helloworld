<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpRentInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.QueryIqpRentInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpRentInfo/queryIqpRentInfoList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpRentInfoUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.QueryIqpRentInfoDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpRentInfo/updateIqpRentInfoRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpRentInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.UpdateIqpRentInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="message" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpRentInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.QueryIqpRentInfoDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/IqpRentInfo/queryIqpRentInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpRentInfoAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/iqp/IqpRentInfo/addIqpRentInfoRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpRentInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.AddIqpRentInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="message" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpRentInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.DeleteIqpRentInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="message" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageIqpRentInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqprentinfo.QueryIqpRentInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpRentInfoList">
						<kColl>
							<field id="lessee_name" />
							<field id="lessee_cert_no" />
							<field id="every_rent_amt" />
							<field id="pld_amt" />
							<field id="total_rent_amt" />
							<field id="start_date" />
							<field id="end_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



