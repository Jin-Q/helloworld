	<!-- 唯一校验 -->
	<action id="checkUnique" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionapp.CheckUniqueOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
