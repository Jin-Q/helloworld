	<action id="pageOutBillNoPop" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionapp.QueryGoverBillNoPop" />
			<transition dest="queryListRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="resultSet">
						<kColl>
						</kColl>
					</iColl>
