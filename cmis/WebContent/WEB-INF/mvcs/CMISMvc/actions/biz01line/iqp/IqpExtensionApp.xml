	<action id="CheckExtensionDate" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpextensionapp.CheckExtensionDateOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="term_type" class="com.ecc.emp.data.DataField" />
					<field id="cont_term" class="com.ecc.emp.data.DataField" />
					<field id="extension_date" class="com.ecc.emp.data.DataField" />
					<field id="date_type" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
