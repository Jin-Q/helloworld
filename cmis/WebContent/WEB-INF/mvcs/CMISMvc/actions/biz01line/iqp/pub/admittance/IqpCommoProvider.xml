<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpCommoProviderList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryIqpCommoProviderListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/IqpCommoProvider/queryIqpCommoProviderList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpCommoProviderUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryIqpCommoProviderDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/IqpCommoProvider/updateIqpCommoProviderRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpCommoProviderRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.UpdateIqpCommoProviderRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="msg" />
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpCommoProviderViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryIqpCommoProviderDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/pub/admittance/IqpCommoProvider/queryIqpCommoProviderDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getIqpCommoProviderAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.AddIqpCommoProviderOp"/>
			<transition dest="addPage"/>
		</flowInvoker>
		<outputViews>
		<jspView id="addPage" url="biz01line/iqp/pub/admittance/IqpCommoProvider/updateIqpCommoProviderRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpCommoProviderRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.AddIqpCommoProviderRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="msg" />
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>	
	
	<!-- 检查当前所选客户是否存在待发起的申请 -->
	<action id="queryProviderNoListPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryProviderNoListPageOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteIqpCommoProviderRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.DeleteIqpCommoProviderRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 准入 -->
	<action id="queryInsertCommoProvider" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryInsertCommoProviderOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 退出 -->
	<action id="queryOutCommoProvider" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryOutCommoProviderOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获取供应商编号  返回给pop-->
	<action id="getCommoProviderNoPopList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.GetCommoProviderNoPopListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/pub/admittance/IqpCommoProvider/queryCommoProviderListPop.jsp" />
		</outputViews>
	</action>
	
	<!-- POP分页查询记录列表  -->
	<action id="pageCommoProviderNoPopList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.GetCommoProviderNoPopListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpCommoProviderList">
						<kColl>
							<field id="mort_catalog_no" />
							<field id="provider_no" />
							<field id="linkman" />
							<field id="link_addr" />
							<field id="link_phone" />
							<field id="input_id" />
							<field id="status" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	
	<!-- 分页查询记录列表 -->
	<action id="pageIqpCommoProviderQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.pub.admittance.iqpcommoprovider.QueryIqpCommoProviderListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpCommoProviderList">
						<kColl>
							<field id="mort_catalog_no" />
							<field id="provider_no" />
							<field id="linkman" />
							<field id="link_addr" />
							<field id="link_phone" />
							<field id="input_id" />
							<field id="status" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
</actionDefines>