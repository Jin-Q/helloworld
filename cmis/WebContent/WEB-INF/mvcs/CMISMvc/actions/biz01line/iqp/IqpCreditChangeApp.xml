			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpcreditchangeapp.QueryIqpCreditChangeAppDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/IqpCreditChangeApp/queryIqpCreditChangeAppDetail.jsp" />
		</outputViews>
	</action>
	
