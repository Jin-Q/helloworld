			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpcreditchangeapp.QueryIqpCreditChangeAppListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpCreditChangeAppList">
						<kColl>
							<field id="serno" />
							<field id="cus_id" />
							<field id="cus_id_displayname" />
							<field id="bill_no" />
							<field id="assure_main" />
							<field id="cont_cur_type" />
							<field id="cont_amt" />
							<field id="new_apply_amt" />
							<field id="apply_date" />
							<field id="input_id_displayname" />
							<field id="input_br_id_displayname" />
							<field id="approve_status" />
							<field id="end_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
