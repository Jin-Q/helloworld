					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>   
		</outputViews>
	</action>
	
	<!-- 更新变更类型记录 --> 
	<action id="updateCorreRelRecord" type="normal" checkSession="true" >   
