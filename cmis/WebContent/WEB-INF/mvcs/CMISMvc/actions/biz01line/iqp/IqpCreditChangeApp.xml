<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpCreditChangeAppList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpcreditchangeapp.QueryIqpCreditChangeAppListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpCreditChangeApp/queryIqpCreditChangeAppList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取查看历史主页面 --> 
	<action id="queryIqpCreditChangeHisList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpcreditchangeapp.QueryIqpCreditChangeHisListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews> 
			<jspView id="retPage" url="biz01line/iqp/IqpCreditChangeApp/queryIqpCreditChangeHisList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpCreditChangeAppUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.iqp.op.iqpcreditchangeapp.QueryIqpCreditChangeAppDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpCreditChangeApp/updateIqpCreditChangeAppRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 检查是否已存在该笔修改信用证信息 --> 
	<action id="queryIsConfIqpCreditChange" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpcreditchangeapp.QueryIsConfIqpCreditChangeOp"/>
			<transition dest="retPage"/> 
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpCreditChangeAppRecord" type="normal" checkSession="true" >
