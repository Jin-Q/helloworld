<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 查询记录列表 -->
	<action id="queryIqpBksyndicList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.QueryIqpBksyndicListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/iqp/IqpBksyndic/queryIqpBksyndicList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取新增页面 -->
	<action id="getIqpBksyndicAddPage" type="normal" checkSession="true" >
		<jspView id="queryListRes" url="biz01line/iqp/IqpBksyndic/addIqpBksyndicRecord.jsp" />
	</action>
	
	<!-- 新增信息 -->
	<action id="addIqpBksyndicRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.AddIqpBksyndicRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/iqp/IqpBksyndic/updateIqpBksyndicRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpBksyndicUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.QueryIqpBksyndicDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpBksyndic/updateIqpBksyndicRecord.jsp" />
		</outputViews>
	</action>

	<!-- 修改信息 -->
	<action id="updateIqpBksyndicRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.UpdateIqpBksyndicRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<view id="queryListRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 删除信息 -->
	<action id="deleteIqpBksyndicRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.DeleteIqpBksyndicRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="operationResult.jsp?url=queryIqpBksyndicList.do" />
		</outputViews>
	</action>
	
	<!-- 查询详情 -->
	<action id="queryIqpBksyndicDetail" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.QueryIqpBksyndicDetailOp"/>
			<transition dest="queryDetailRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryDetailRes" url="biz01line/iqp/IqpBksyndic/queryIqpBksyndicDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageIqpBksyndicQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbksyndic.QueryIqpBksyndicListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpBksyndicList">
						<kColl>
							<field id="bank_syndic_type" />
							<field id="agent_org_flag" />
							<field id="bank_syndic_amt" />
							<field id="agent_rate" />
							<field id="amt_arra_rate" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
					
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	
	<!--*************************子表【银团信息】*******************************-->
	
	<!-- 作为子表的列表页面 -->
	<action id="queryIqpBksyndicIqpBksyndicInfoList" type="normal" checkSession="true" >
