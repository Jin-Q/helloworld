<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpGuarantChangeAppList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIqpGuarantChangeAppListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpGuarantChangeApp/queryIqpGuarantChangeAppList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取历史主页面 -->
	<action id="queryIqpGuarantChangeAppHistory" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIqpGuarantChangeAppHistoryOp"/> 
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpGuarantChangeApp/queryIqpGuarantChangeAppHistory.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpGuarantChangeAppUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIqpGuarantChangeAppDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpGuarantChangeApp/updateIqpGuarantChangeAppRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateIqpGuarantChangeAppRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.UpdateIqpGuarantChangeAppRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>    
		</outputViews> 
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpGuarantChangeAppViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIqpGuarantChangeAppDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/IqpGuarantChangeApp/queryIqpGuarantChangeAppDetail.jsp" />
		</outputViews>
	</action>
	
	<!--获得新增下一步页面-->
	<action id="getIqpGuarantChangeAppAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.AddIqpGuarantChangeAppListOp"/>
			<transition dest="addPage"/>
		</flowInvoker>       
		<outputViews>
			<jspView id="addPage" url="biz01line/iqp/IqpGuarantChangeApp/addIqpGuarantChangeAppRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 从向导页进入新增页面 -->
	<action id="addIqpGuarantChangeAppPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.AddIqpGuarantChangeAppPageOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpGuarantChangeApp/addIqpGuarantChangeAppPage.jsp" />
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addIqpGuarantChangeAppRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.AddIqpGuarantChangeAppRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="serno" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>    
	</action>	      
	
	<!-- 检查是否已存在该笔修改保函信息 --> 
	<action id="queryIsConfIqpGuarantChange" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIsConfIqpGuarantChangeOp"/>
			<transition dest="retPage"/> 
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action> 
	
	<!-- 删除记录 -->
	<action id="deleteIqpGuarantChangeAppRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.DeleteIqpGuarantChangeAppRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>  
			</view> 
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageIqpGuarantChangeAppQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIqpGuarantChangeAppListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpGuarantChangeAppList">
						<kColl>
							<field id="serno" />
							<field id="cus_id" />
							<field id="cus_id_displayname" />
							<field id="assure_main" />
							<field id="cont_cur_type" />
							<field id="cont_amt" />
							<field id="new_cont_amt" />
							<field id="apply_date" />
							<field id="input_id_displayname" />
							<field id="input_br_id_displayname" />
							<field id="approve_status" />
							<field id="end_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页历史查询记录列表 -->
		<action id="pageIqpGuarantChangeHisQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.QueryIqpGuarantChangeAppHistoryOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpGuarantChangeAppList">
						<kColl>
							<field id="serno" />
							<field id="cus_id" />
							<field id="cus_id_displayname" />
							<field id="assure_main" />
							<field id="cont_cur_type" />
							<field id="cont_amt" />
							<field id="new_cont_amt" />
							<field id="apply_date" />
							<field id="input_id_displayname" />
							<field id="input_br_id_displayname" />
							<field id="approve_status" />
							<field id="end_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 新增下一步分页查询列表 -->
		<action id="pageAddIqpGuarantQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarantchangeapp.AddIqpGuarantChangeAppListOp"/>
			<transition dest="queryListRes"/>   
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CtrLoanContList">
						<kColl> 
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



