<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryIqpBailSubDisList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryIqpBailSubDisListOp"/>
			<transition dest="sub" condition="($retValue='1')"/>
			<transition dest="dis" condition="($retValue='2')"/>
		</flowInvoker>
		<outputViews>
			<jspView id="sub" url="biz01line/iqp/IqpBailSubDis/queryIqpBailSubList.jsp" />
			<jspView id="dis" url="biz01line/iqp/IqpBailSubDis/queryIqpBailDisList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取操作主页面(for 保证金台账) -->
	<action id="queryIqpBailSubDisList4BailAcc" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryIqpBailSubDisList4BailAccOp"/>
			<transition dest="sub" />
		</flowInvoker>
		<outputViews>
			<jspView id="sub" url="biz01line/iqp/IqpBailSubDis/queryIqpBailSub4PubBailList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getIqpBailSubDisUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryIqpBailSubDisDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpBailSubDis/addIqpBailSubDisRecord.jsp" />
		</outputViews>
	</action>
	
	<!--根据合同编号异步返回保证金比例和保证金金额 modified by yangzy 2015/07/20 需求：XD150407026， 存量外币业务取当时时点汇率 -->
	<action id="queryBailAmtByContNo" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryBailAmtByContNoOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="ori_bail_perc" class="com.ecc.emp.data.DataField" />
					<field id="ori_bail_amt" class="com.ecc.emp.data.DataField" />
					<field id="cont_amt" class="com.ecc.emp.data.DataField" />
					<field id="cont_cur_type" class="com.ecc.emp.data.DataField" />
					<field id="exchange_rate" class="com.ecc.emp.data.DataField" />
					<field id="cont_rmb_amount" class="com.ecc.emp.data.DataField" />
					<field id="security_cur_type" class="com.ecc.emp.data.DataField" />
					<field id="security_exchange_rate" class="com.ecc.emp.data.DataField" />
					<field id="security_rmb_amt" class="com.ecc.emp.data.DataField" />
					<field id="adj_security_exchange_rate" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getIqpBailSubDisViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryIqpBailSubDisDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/iqp/IqpBailSubDis/queryIqpBailSubDisDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增向导页面 -->
	<action id="getIqpBailSubDisAddLeadPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/iqp/IqpBailSubDis/addIqpBailSubDisLeadRecord.jsp"/>
	</action>

	<!-- 获得新增页面 -->
	<action id="getIqpBailSubDisAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.GetAddIqpBailSubDisRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpBailSubDis/addIqpBailSubDisRecord.jsp"/>
		</outputViews>
	</action>
	<!-- 新增/更新记录 -->
	<action id="addIqpBailSubDisRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.AddIqpBailSubDisRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="serno" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	<!-- 检查指定合同的保证金追加或者提取记录是否存在 -->
	<action id="checkBailExist" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.IqpCheckBailExistOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="result" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	<!-- 删除记录 -->
	<action id="deleteIqpBailSubDisRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.DeleteIqpBailSubDisRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageIqpBailSubDisQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryIqpBailSubDisListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpBailSubDisList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 (for 保证金台账)-->
		<action id="pageIqpBailSubDisQuery4PubBail" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.QueryIqpBailSubDisList4BailAccOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IqpBailSubDisList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 检测保证金追加的记录是否为追加保证金 -->
	<action id="checkIqpBailSubDisRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpbailsubdis.CheckIqpBailSubDisRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="checked" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
</actionDefines>



