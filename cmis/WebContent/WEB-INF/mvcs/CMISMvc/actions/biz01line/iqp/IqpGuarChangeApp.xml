		<outputViews>
			<jspView id="retPage" url="biz01line/iqp/IqpGuarChangeApp/addIqpGuarChangeAppPage.jsp" />
		</outputViews>
	</action>
	
	<!-- 检查是否已存在该笔担保修改信息 --> 
	<action id="queryIsConfIqpGuarChange" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarchangeapp.QueryIsConfIqpGuarChangeOp"/>
			<transition dest="retPage"/> 
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
		
	<!-- 新增记录 -->
	<action id="addIqpGuarChangeAppRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarchangeapp.AddIqpGuarChangeAppRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews> 
