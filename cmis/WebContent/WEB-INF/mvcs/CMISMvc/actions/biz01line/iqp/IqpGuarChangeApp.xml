				</output>
			</view>  
		</outputViews> 
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteIqpGuarChangeAppRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.iqp.op.iqpguarchangeapp.DeleteIqpGuarChangeAppRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view> 
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
