<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryMortStorExwaInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.QueryMortStorExwaInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/MortStorExwaInfo/queryMortStorExwaInfoList.jsp" />
		</outputViews>
	</action>
	<!-- 获取出入库向导页面 -->
	<action id="getMortStorExwaInfoAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/mort/MortStorExwaInfo/addMortStorExwaInfoRecord.jsp"/>
	</action>
	<!-- 新增记录 -->
	<action id="addMortStorExwaInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.AddMortStorExwaInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
					<field id="serno" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 获得编辑页面 -->
	<action id="getMortStorExwaInfoUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.QueryMortStorExwaInfoDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/MortStorExwaInfo/updateMortStorExwaInfoRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateMortStorExwaInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.UpdateMortStorExwaInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getMortStorExwaInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.QueryMortStorExwaInfoDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/mort/MortStorExwaInfo/queryMortStorExwaInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteMortStorExwaInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.DeleteMortStorExwaInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 删除明细记录 -->
	<action id="deleteMortStorExwaDetailRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.DeleteMortStorExwaDetailRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageMortStorExwaInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortstorexwainfo.QueryMortStorExwaInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="MortStorExwaInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



