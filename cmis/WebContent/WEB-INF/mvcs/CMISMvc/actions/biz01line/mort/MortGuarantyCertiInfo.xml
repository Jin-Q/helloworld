		<action id="pageMortGuarantyCertiInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortguarantycertiinfo.QueryMortGuarantyCertiInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="MortGuarantyCertiInfoList">
						<kColl>
							<field id="warrant_cls" />
							<field id="warrant_type" />
							<field id="warrant_no" />
							<field id="guaranty_no" />
							<field id="keep_org_no" />
							<field id="hand_org_no" />
							<field id="warrant_state" />
							<field id="is_main_warrant" />
							<field id="warrant_name" />
							<field id="keep_org_no_displayname" />
							<field id="hand_org_no_displayname" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
