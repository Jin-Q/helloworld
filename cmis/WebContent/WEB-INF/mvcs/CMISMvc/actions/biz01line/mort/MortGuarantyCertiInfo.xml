	<!-- 获取操作主页面 -->
	<action id="queryMortGuarantyCertiInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortguarantycertiinfo.QueryMortGuarantyCertiInfoListOp"/>
			<transition dest="retPage" condition="$retValue='1'"/>
			<transition dest="toPage" condition="$retValue='2'"/>
			<transition dest="exwa" condition="$retValue='3'"/>
		</flowInvoker>
		<outputViews>
		    <jspView id="retPage" url="biz01line/mort/MortGuarantyCertiInfo/queryMortGuarantyCertiInfoList.jsp" />
			<jspView id="toPage" url="biz01line/mort/MortGuarantyCertiInfo/queryMortGuarantyCertiInfoMainList.jsp" />
			<jspView id="exwa" url="biz01line/mort/MortGuarantyCertiInfo/queryMortGuarantyCertiInfoExwaList.jsp" />
		</outputViews>
	</action>
	
