<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryMortCargoReplList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.QueryMortCargoReplListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoRepl/queryMortCargoReplList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getMortCargoReplUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.QueryMortCargoReplDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoRepl/updateMortCargoReplRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateMortCargoReplRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.UpdateMortCargoReplRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryMortCargoReplList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getMortCargoReplViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.QueryMortCargoReplDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/mort/cargo/MortCargoRepl/queryMortCargoReplDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增向导页面 -->
	<action id="getMortCargoReplAddLeadPage" type="normal" checkSession="true" >
	    <flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.GetMortCargoReplLeadRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
		    <jspView id="retPage" url="biz01line/mort/cargo/MortCargoRepl/addMortCargoReplLeadRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getMortCargoReplAddPage" type="normal" checkSession="true" >
	    <flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.AddMortCargoReplLeadRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
		    <jspView id="retPage" url="biz01line/mort/cargo/MortCargoRepl/addMortCargoReplRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addMortCargoReplRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.AddMortCargoReplRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
					<field id="serno" />
				</output>
			</jsonView>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteMortCargoReplRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.DeleteMortCargoReplRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageMortCargoReplQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepl.QueryMortCargoReplListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="MortCargoReplList">
						<kColl>
							<field id="cus_id" />
							<field id="serno" />
							<field id="guaranty_no" />
							<field id="oversee_agr_no" />
							<field id="storage_total" />
							<field id="status" />
							<field id="input_id" />
							<field id="input_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
</actionDefines>



