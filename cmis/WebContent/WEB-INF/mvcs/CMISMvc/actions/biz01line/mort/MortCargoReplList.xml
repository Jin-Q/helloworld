<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryMortCargoReplListList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.QueryMortCargoReplListListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoReplList/queryMortCargoReplListList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getMortCargoReplListUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.QueryMortCargoReplListDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoReplList/updateMortCargoReplListRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateMortCargoReplListRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.UpdateMortCargoReplListRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getMortCargoReplListViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.QueryMortCargoReplListDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/mort/cargo/MortCargoReplList/queryMortCargoReplListDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getMortCargoReplListAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/mort/cargo/MortCargoReplList/addMortCargoReplListRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addMortCargoReplListRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.AddMortCargoReplListRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
					<field id="msg" />
				</output>
			</jsonView>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteMortCargoReplListRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.DeleteMortCargoReplListRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageMortCargoReplListQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargorepllist.QueryMortCargoReplListListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="MortCargoReplListList">
						<kColl>
						</kColl>
					</iColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



