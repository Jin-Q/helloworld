<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryMortCargoStorageList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.QueryMortCargoStorageListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoStorage/queryMortCargoStorageList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getMortCargoStorageUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.QueryMortCargoStorageDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoStorage/updateMortCargoStorageRecord.jsp" />
		</outputViews>
	</action>
	<!-- 获得查看页面 -->
	<action id="getMortCargoStorageViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.QueryMortCargoStorageDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/mort/cargo/MortCargoStorage/queryMortCargoStorageDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得入库操作页面 -->
	<action id="getMortCargoStorageAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.AddMortCargoStorageLeadRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/mort/cargo/MortCargoStorage/addMortCargoStorageRecord.jsp"/>
		</outputViews>
		
	</action>
	
	<!-- 新增记录 -->
	<action id="addMortCargoStorageRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.AddMortCargoStorageRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>	
	<!-- 异步更改押品状态 -->
	<action id="changeGaurantyStatus" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.ChangeGaurantyStatus"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteMortCargoStorageRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.DeleteMortCargoStorageRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageMortCargoStorageQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.cargo.mortcargostorage.QueryMortCargoStorageListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="MortCargoStorageList">
						<kColl>
							<field id="serno" />
							<field id="cus_id" />
							<field id="guaranty_no" />
							<field id="oversee_agr_no" />
							<field id="storage_total" />
							<field id="status" />
							<field id="input_id" />
							<field id="input_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



