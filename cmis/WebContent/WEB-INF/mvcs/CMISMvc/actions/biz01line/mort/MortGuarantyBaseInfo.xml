		<!-- 获取详细信息页面 -->
	<action id="getDetailInformationPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.mort.mortguarantybaseinfo.GetDetailInformationPageOp"/>
			<transition dest="error" condition="$retValue='-1'"/>
			<transition dest="errorMortType" condition="$retValue='-2'"/>
			
			
			<!-- 住宅 -->

			<!--C010101	普通商品用房  -->
			<transition dest="real_house" condition="$retValue='C010101'"/>
			<!-- C010102	别墅 -->
			<transition dest="real_house" condition="$retValue='C010102'"/>
			<!-- C010103	经济适用住房（限价房） -->
			<transition dest="real_house" condition="$retValue='C010103'"/>
			<!-- C010104	车库/车位（区分与住宅一起/独立） -->
			<transition dest="car_barn" condition="$retValue='C010104'"/>	
			<!-- C010105	城市自建房    -->
			<transition dest="real_house" condition="$retValue='C010105'"/>			
			<!-- C010106	农村自建房  2015-08-14 Edited by FCL 原自建房更改为城市自建房，新增农村自建房  -->
			<transition dest="real_house" condition="$retValue='C010106'"/>			
			<!-- C010199	其他居住用房 -->
			<transition dest="real_house" condition="$retValue='C010199'"/>
			
			
			

			<!-- C020201	商铺、商场 -->
			<transition dest="market_store" condition="$retValue='C020201'"/>


			<!--C020301	标准工业厂房（含工业配套宿舍） -->
			<transition dest="plant" condition="$retValue='C020301'"/>
			<!-- C020302	非标准工业厂房（含工业配套宿舍）	 -->
			<transition dest="plant" condition="$retValue='C020302'"/>
			<!-- C020303	标准仓储 -->
			<transition dest="plant" condition="$retValue='C020303'"/>
			<!-- C020304	非标准仓储	 -->
			<transition dest="plant" condition="$retValue='C020304'"/>
			

			<!--C020101	酒店、写字楼 -->

			<!-- C020101	多层写字楼 -->
			<transition dest="hotel_office" condition="$retValue='C020101'"/>
			<!--  C020103	高层写字楼-->
			<transition dest="hotel_office" condition="$retValue='C020103'"/>
			<!-- C020105	公寓式写字楼-->
			<transition dest="hotel_office" condition="$retValue='C020105'"/>
			<!-- C020106	宾馆酒店 -->
			<transition dest="hotel_office" condition="$retValue='C020106'"/>
			<!-- C020107	其他商业用房-->
			<transition dest="hotel_office" condition="$retValue='C020107'"/>

			
			
			<!-- 土地使用权 -->

			
			<!--C030101	居住用地  -->
			<transition dest="usufruct_land" condition="$retValue='C030101'"/>
			<!--C040101	工业用地  -->
			<transition dest="usufruct_land" condition="$retValue='C040101'"/>
			<!-- C040201	商业用地 -->
			<transition dest="usufruct_land" condition="$retValue='C040201'"/>
			<!-- C040301	写字楼用地 -->
			<transition dest="usufruct_land" condition="$retValue='C040301'"/>
			<!--C040401	其他用地  -->
			<transition dest="usufruct_land" condition="$retValue='C040401'"/>
			
			
			<!-- 在建工程 -->
			<transition dest="building_project" condition="$retValue='C050101'"/>
			
			
			<!-- 机动车辆 -->

			<!-- 客运汽车-->
			<transition dest="traffic_car" condition="$retValue='Z040101'"/>
			<!-- 货运汽车-->
			<transition dest="traffic_car" condition="$retValue='Z040102'"/>
			<!-- 专用汽车-->
			<transition dest="traffic_car" condition="$retValue='Z040103'"/>
			<!-- 自用汽车-->
			<transition dest="traffic_car" condition="$retValue='Z040104'"/>
			
			
	        <!-- 船舶 -->
	                  
	        <!--  Z040201	客运  -->
			<transition dest="traffic_ship" condition="$retValue='Z040201'"/>
			 <!-- Z040202	货运（含油轮、集装箱轮等） -->
			<transition dest="traffic_ship" condition="$retValue='Z040202'"/>
			 <!-- Z040203	渔船  -->
			<transition dest="traffic_ship" condition="$retValue='Z040203'"/>
			 <!-- Z040299	其他类型船舶  -->
			<transition dest="traffic_ship" condition="$retValue='Z040299'"/>
			
			
			 <!-- Z040301	客运飞机 -->
            <transition dest="traffic_others" condition="$retValue='Z040301'"/>
             <!-- Z040302	货运飞机 -->
            <transition dest="traffic_others" condition="$retValue='Z040302'"/>
             <!-- Z040303	飞机配套设备 -->
            <transition dest="traffic_others" condition="$retValue='Z040303'"/>
			
			<!-- 森林使用权 -->
			<transition dest="usufruct_forest" condition="$retValue='Z070201'"/>
			
			<!-- 海洋使用权 -->
			<transition dest="usufruct_sea" condition="$retValue='Z070401'"/>
			
			<!-- 机器设备 -->

			<!-- Z030101	锅炉及原动机 -->
			<transition dest="machine_equipment" condition="$retValue='Z030101'"/>
			<!--Z030102	金属加工机械  -->
			<transition dest="machine_equipment" condition="$retValue='Z030102'"/>
			<!-- Z030103	起重运输设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030103'"/>
			<!-- Z030104	泵、阀门、压缩机及类似机械 -->
			<transition dest="machine_equipment" condition="$retValue='Z030104'"/>
			<!-- Z030105	轴承、齿轮、传动和驱动部件 -->
			<transition dest="machine_equipment" condition="$retValue='Z030105'"/>
			<!-- Z030106	风机、衡器、包装设备等通用设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030106'"/>			
			<!-- Z030107	通用零部件制造设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030107'"/>
			<!--  Z030108	金属铸、锻加工设备-->
			<transition dest="machine_equipment" condition="$retValue='Z030108'"/>
			<!-- Z030199	其他通用设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030199'"/>


			<!--Z030201	矿山、冶金、建筑专用设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030201'"/>
			<!--Z030202	化工、木材、非金属加工专用设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030202'"/>
			<!-- Z030203	食品、饮料、烟草及饲料生产专用设备-->
			<transition dest="machine_equipment" condition="$retValue='Z030203'"/>
			<!-- Z030204	印刷、制药、日化生产专用设备-->
			<transition dest="machine_equipment" condition="$retValue='Z030204'"/>
			<!-- Z030205	纺织、服装和皮革工业专用设备-->
			<transition dest="machine_equipment" condition="$retValue='Z030205'"/>
			<!-- Z030206	电子和电工机械专用设备-->
			<transition dest="machine_equipment" condition="$retValue='Z030206'"/>
			<!-- Z030207	农、林、牧、渔专用机械-->
			<transition dest="machine_equipment" condition="$retValue='Z030207'"/>
			<!-- Z030208	医疗仪器设备及器械-->
			<transition dest="machine_equipment" condition="$retValue='Z030208'"/>
			<!--Z030209	环保、社会公共安全及其他专用设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030209'"/>
			<!--Z030299	其他行业专用设备 -->
			<transition dest="machine_equipment" condition="$retValue='Z030299'"/>
		
			<!--  存单质押-->
			
			
			<!-- A010101	存单(本行) -->
			<transition dest="deposit_receipt" condition="$retValue='A010101'"/>
			<!-- A010102	存单(他行) -->
			<transition dest="deposit_receipt" condition="$retValue='A010102'"/>
			<!-- A010201	保证金目前没有也没可以跳转 -->
			<transition dest="deposit_receipt" condition="$retValue='A010201'"/>
		
		
			
			<!--A010301	账户冻结  -->
			<transition dest="account_freeze" condition="$retValue='A010301'"/>
			

			
			<!-- 贵金属 -->

			<!-- A020101	黄金 -->
			<transition dest="noble_metal" condition="$retValue='A020101'"/>
			<!--A020201	标准银、铂金等其他贵金属  -->
			<transition dest="noble_metal" condition="$retValue='A020201'"/>

						
			<!-- 债券抵押 -->
			
			<!--  	凭证式国债-->
			<transition dest="bond_pledge" condition="$retValue='A030101'"/>
			
			<!-- 	记账式国债 -->
			<transition dest="bond_pledge" condition="$retValue='A030102'"/>
			<!-- 	储蓄国债 -->
			<transition dest="bond_pledge" condition="$retValue='A030103'"/>
			
			<!--	境外AA-级及以上国家/地区政府债券  -->
			<transition dest="bond_pledge" condition="$retValue='A030201'"/>
			<!-- 	境外BB-及以上至AA-级以下级别国家/地区政府债券 -->
			<transition dest="bond_pledge" condition="$retValue='A030202'"/>
			
			<!-- 	人民银行金融债券 -->
			<transition dest="bond_pledge" condition="$retValue='A030301'"/>
			<!--	中国政策性银行金融债券  -->
			<transition dest="bond_pledge" condition="$retValue='A030302'"/>
			
			
			<!--	中国商业银行金融债券  -->
			<transition dest="bond_pledge" condition="$retValue='A030303'"/>
			<!-- 	中国商业银行次级债 -->
			<transition dest="bond_pledge" condition="$retValue='A030304'"/>

			<!-- 	中央政府投资的金融资产管理公司为收购国有银行不良贷款而定向发行的债券 -->
			<transition dest="bond_pledge" condition="$retValue='A030305'"/>
