<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 鑾峰彇鎿嶄綔涓婚〉闈� -->
	<action id="queryFncConfItemsList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.QueryFncConfItemsListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/fnc/config/FncConfItems/queryFncConfItemsList.jsp" />
		</outputViews>
	</action>
		<!-- pop -->
	<action id="getFncConfItemsList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.QueryFncConfItemsListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/fnc/config/FncConfItems/queryFncConfItemsListPop.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱缂栬緫椤甸潰 -->
	<action id="getFncConfItemsUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.QueryFncConfItemsDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/fnc/config/FncConfItems/updateFncConfItemsRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 鏇存柊璁板綍 -->
	<action id="updateFncConfItemsRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.UpdateFncConfItemsRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryFncConfItemsList.do" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏌ョ湅椤甸潰 -->
	<action id="getFncConfItemsViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.QueryFncConfItemsDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/fnc/config/FncConfItems/queryFncConfItemsDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏂板椤甸潰 -->
	<action id="getFncConfItemsAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/fnc/config/FncConfItems/addFncConfItemsRecord.jsp"/>
	</action>
	
	<!-- 鏂板璁板綍 -->
	<action id="addFncConfItemsRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.AddFncConfItemsRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
							<field id="flag" class="com.ecc.emp.data.DataField"/>
							<field id="item_id" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 鍒犻櫎璁板綍 -->
	<action id="deleteFncConfItemsRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.DeleteFncConfItemsRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryFncConfItemsList.do" />
		</outputViews>
	</action>
	
	<!-- 鍒嗛〉鏌ヨ璁板綍鍒楄〃 -->
		<action id="pageFncConfItemsQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfitems.QueryFncConfItemsListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="FncConfItemsList">
						<kColl>
							<field id="item_id" />
							<field id="item_name" />
							<field id="fnc_conf_typ" />
							<field id="fnc_no_flg" />
							<field id="remark" />
							<field id="formula" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



