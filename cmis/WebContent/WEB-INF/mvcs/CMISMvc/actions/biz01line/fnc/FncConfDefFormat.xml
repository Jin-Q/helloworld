	<!--异步请求查询页面 -->
	<action id="queryFncConfItemsListByCon" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfdefformat.QueryFncConfItemsListByOp"/>
			<transition dest="queryDetailRes"/>
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					
					<iColl id="FncConfItemsList" class="com.ecc.emp.data.IndexedCollection">
						<kColl class="com.ecc.emp.data.KeyedCollection">
							<field id="item_id" class="com.ecc.emp.data.DataField"/>
