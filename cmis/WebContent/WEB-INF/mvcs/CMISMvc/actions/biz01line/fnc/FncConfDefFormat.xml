	<!-- 更新记录 -->
	<action id="updateFncConfDefFormatRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfdefformat.UpdateFncConfDefFormatRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
