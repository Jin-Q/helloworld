	</action>
	
	
	<!-- 新增记录 -->
	<action id="addFncConfDefFormatRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfdefformat.AddFncConfDefFormatRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryFncConfDefFormatList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteFncConfDefFormatRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfdefformat.DeleteFncConfDefFormatRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=dealFncConfDefFormat.do" />
		</outputViews>
	</action>
	
	<action id="dealFncConfDefFormat" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfdefformat.DealFncConfDefFormatOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/fnc/config/FncConfDefFormat/dealFncConfDefFormat.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageFncConfDefFormatQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.fnc.op.fncconfdefformat.QueryFncConfDefFormatListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="FncConfDefFormatList">
						<kColl>
							<field id="style_id" />
							<field id="item_id" />
							<field id="fnc_conf_order" />
							<field id="fnc_conf_cotes" />
							<field id="fnc_conf_row_flg" />
							<field id="fnc_conf_indent" />
							<field id="fnc_conf_data_col" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	
	<!-- showSelfformula.do -->
		<action id="showSelfformula" type="normal" checkSession="true" >
