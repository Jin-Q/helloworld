<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryAccDrftList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.QueryAccDrftListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccDrft/queryAccDrftList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getAccDrftUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.acc.op.accdrft.QueryAccDrftDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccDrft/updateAccDrftRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateAccDrftRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.UpdateAccDrftRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryAccDrftList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getAccDrftViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.QueryAccDrftDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/acc/AccDrft/queryAccDrftDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getAccDrftAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/acc/AccDrft/addAccDrftRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addAccDrftRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.AddAccDrftRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryAccDrftList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteAccDrftRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.DeleteAccDrftRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryAccDrftList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageAccDrftQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.QueryAccDrftListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="AccDrftList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 导出成生成excel -->                                                                                                                                                
	<action id="accDrftGroupExpBatchToExcel" type="normal" checkSession="true" >                                                                                         
		<flowInvoker>                                                                                                                                                       
			<operation class="com.yucheng.cmis.biz01line.acc.op.accdrft.AccDrftGroupExpBatchToExcel"/>                                                               
			<transition dest="retPage"/>                                                                                                                                      
		</flowInvoker>                                                                                                                                                      
		<outputViews>                                                                                                                                                       
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">                                                                                                                                         
 				<excelWorkbook fileName="AccDrftList.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">                                                                    
 					<excelSheet id="1" name="票据台账" isTitle="true" iCollName="AccDrftList" class="com.yucheng.cmis.view.excel.ExcelSheet">                                     
 						<excelColumn dataName="cont_no" name="合同编号" cellType="string" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                       
 						<excelColumn dataName="bill_no" name="借据编号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>     
 						<excelColumn dataName="discount_per" name="贴现人/交易对手" class="com.yucheng.cmis.view.excel.ExcelColumn"/>   
 						<excelColumn dataName="prd_id_displayname" name="产品名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                
 						<excelColumn dataName="dscnt_type" name="贴现方式" dictName="STD_ZB_RPDDSCNT_MODE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>   
 						<excelColumn dataName="cur_type" name="交易币种" dictName="STD_ZX_CUR_TYPE" class="com.yucheng.cmis.view.excel.ExcelColumn"/> 
 						<excelColumn dataName="drft_amt" name="票面金额" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="rpay_amt" name="实付金额" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                       
 						<excelColumn dataName="porder_no" name="汇票号码"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                 
 						<excelColumn dataName="dscnt_date" name="贴现日期" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                                                         
 						<excelColumn dataName="accp_status" name="台账状态"  dictName="STD_ZB_ACC_TYPE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                             
 					</excelSheet>                                                                                                                                                 
 				</excelWorkbook>                                                                                                                                                
 			</view>                                                                                                                                                           
		</outputViews>                                                                                                                                                      
	</action>     
	
</actionDefines>



