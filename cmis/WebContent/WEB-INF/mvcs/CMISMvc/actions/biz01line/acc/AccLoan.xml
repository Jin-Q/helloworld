 						<excelColumn dataName="prd_id_displayname" name="产品名称"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>  
 						<excelColumn dataName="cur_type" name="币种" dictName="STD_ZX_CUR_TYPE"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                     
