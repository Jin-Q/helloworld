<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryAccAssetstrsfList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.QueryAccAssetstrsfListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccAssetstrsf/queryAccAssetstrsfList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getAccAssetstrsfUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.QueryAccAssetstrsfDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccAssetstrsf/updateAccAssetstrsfRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateAccAssetstrsfRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.UpdateAccAssetstrsfRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryAccAssetstrsfList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getAccAssetstrsfViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.QueryAccAssetstrsfDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/acc/AccAssetstrsf/queryAccAssetstrsfDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getAccAssetstrsfAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/acc/AccAssetstrsf/addAccAssetstrsfRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addAccAssetstrsfRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.AddAccAssetstrsfRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryAccAssetstrsfList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteAccAssetstrsfRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.DeleteAccAssetstrsfRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryAccAssetstrsfList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageAccAssetstrsfQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.QueryAccAssetstrsfListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="AccAssetstrsfList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 导出成生成excel -->                                                                                                                   
	<action id="accAssetstrsfExpBatchToExcel" type="normal" checkSession="true" >                                                                      
		<flowInvoker>                                                                                                                              
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassetstrsf.AccAssetstrsfExpBatchToExcelOp"/>                                                   
			<transition dest="retPage"/>                                                                                                             
		</flowInvoker>                                                                                                                             
		<outputViews>                                                                                                                              
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">                                                                             
				<excelWorkbook fileName="AccAssetstrsf.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">                                               
					<excelSheet id="1" name="银承台账" isTitle="true" iCollName="AccAssetstrsfList" class="com.yucheng.cmis.view.excel.ExcelSheet">            
						<excelColumn dataName="bill_no" name="借据编号" cellType="string" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                
						<excelColumn dataName="cont_no" name="合同编号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                     
						<excelColumn dataName="toorg_no" name="交易对手行号"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>                      
						<excelColumn dataName="toorg_name" name="交易对手行名"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>                             
						<excelColumn dataName="takeover_amt" name="转让金额" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>                              
						<excelColumn dataName="takeover_type"  name="转让方式" dictName="STD_ZB_TAKEOVER_MODE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                
						<excelColumn dataName="takeover_date"  name="转让日期"  dictName="STD_ZB_FIVE_SORT" class="com.yucheng.cmis.view.excel.ExcelColumn"/> 
						<excelColumn dataName="manager_br_id_displayname"  name="管理机构" class="com.yucheng.cmis.view.excel.ExcelColumn"/>               
					 	<excelColumn dataName="acc_status" name="台账状态" dictName="STD_ZB_ACC_TYPE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>    
					</excelSheet>                                                                                                                        
				</excelWorkbook>                                                                                                                       
			</view>                                                                                                                                  
		</outputViews>                                                                                                                             
	</action>                                                                                                                                    

	
</actionDefines>



