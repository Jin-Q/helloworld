<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryAccAssetTransList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.QueryAccAssetTransListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccAssetTrans/queryAccAssetTransList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getAccAssetTransUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.acc.op.accassettrans.QueryAccAssetTransDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccAssetTrans/updateAccAssetTransRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- add by lisj 2015-3-11 需求编号:【XD150303017】 信贷资产证券化改造 -->
	<action id="getAccAssetTrans4SecMSPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.acc.op.accassettrans.QueryAccAssetTransDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/acc/AccAssetTrans/updateAccAssetTrans4SecMS.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateAccAssetTransRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.UpdateAccAssetTransRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getAccAssetTransViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.QueryAccAssetTransDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/acc/AccAssetTrans/queryAccAssetTransDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getAccAssetTransAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/acc/AccAssetTrans/addAccAssetTransRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addAccAssetTransRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.AddAccAssetTransRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteAccAssetTransRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.DeleteAccAssetTransRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageAccAssetTransQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.QueryAccAssetTransListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="AccAssetTransList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 导出成生成excel -->                                                                                                                                     
	<action id="accAssetTransToExcel" type="normal" checkSession="true" >                                                                                           
		<flowInvoker>                                                                                                                                            
			<operation class="com.yucheng.cmis.biz01line.acc.op.accassettrans.ExportAccAssetTransListOp"/>                                                                    
			<transition dest="retPage"/>                                                                                                                           
		</flowInvoker>                                                                                                                                           
		<outputViews>                                                                                                                                            
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">                                                                                           
 				<excelWorkbook fileName="AccAssetTrans.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">                                                          
 					<excelSheet id="1" name="资产台账" isTitle="true" iCollName="AccAssetTransList" class="com.yucheng.cmis.view.excel.ExcelSheet">                       
 						<excelColumn dataName="bill_no" name="借据编号" cellType="string" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                 
 						<excelColumn dataName="cont_no" name="合同编号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>      
 						<excelColumn dataName="asset_type" name="资产类型" dictName="STD_ZB_ASSETTRANS_TYPE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>   
 						<excelColumn dataName="rebuy_date" name="赎回日期"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                       
 						<excelColumn dataName="acc_status" name="资产状态" dictName="STD_ZB_ACC_TRANS_STATUS" class="com.yucheng.cmis.view.excel.ExcelColumn"/>              
 					</excelSheet>                                                                                                                                      
 				</excelWorkbook>                                                                                                                                     
 			</view>                                                                                                                                                
		</outputViews>                                                                                                                                           
	</action>
</actionDefines>



