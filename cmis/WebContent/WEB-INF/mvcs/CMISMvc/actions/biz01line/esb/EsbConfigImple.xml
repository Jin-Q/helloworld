<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryEsbConfigImpleList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.QueryEsbConfigImpleListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/esb/EsbConfigImple/queryEsbConfigImpleList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getEsbConfigImpleUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.QueryEsbConfigImpleDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/esb/EsbConfigImple/updateEsbConfigImpleRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateEsbConfigImpleRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.UpdateEsbConfigImpleRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryEsbConfigImpleList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getEsbConfigImpleViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.QueryEsbConfigImpleDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/esb/EsbConfigImple/queryEsbConfigImpleDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getEsbConfigImpleAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/esb/EsbConfigImple/addEsbConfigImpleRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addEsbConfigImpleRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.AddEsbConfigImpleRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryEsbConfigImpleList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteEsbConfigImpleRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.DeleteEsbConfigImpleRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryEsbConfigImpleList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageEsbConfigImpleQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.esbconfigimple.QueryEsbConfigImpleListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="EsbConfigImpleList">
						<kColl>
							<field id="esb_id" />
							<field id="service_code" />
							<field id="service_sence" />
							<field id="imple_class" />
							<field id="memo" />
							<field id="status" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获取操作主页面 -->
	<action id="queryDaybatTrandInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.daybattrandinfo.QueryDaybatTrandInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/esb/DaybatTrandInfo/queryDaybatTrandInfoList.jsp" />
		</outputViews>
	</action>
	<!-- 分页查询记录列表 -->
		<action id="pageDaybatTrandInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.daybattrandinfo.QueryDaybatTrandInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="DaybatTrandInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 获得查看页面 -->
	<action id="getDaybatTrandInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.daybattrandinfo.QueryDaybatTrandInfoDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/esb/DaybatTrandInfo/queryDaybatTrandInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<action id="reDealFailTrade" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.esb.op.daybattrandinfo.DaybatReDealFailTradeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
</actionDefines>



