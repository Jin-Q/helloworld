		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ind.op.indgroup.QueryIndGroupListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="IndGroupList">
						<kColl>
						
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
					
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	
