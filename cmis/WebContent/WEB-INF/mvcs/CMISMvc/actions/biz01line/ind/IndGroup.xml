			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/ind/IndGroup/queryIndGroupList.jsp" />
		</outputViews>
	</action>
	
	<action id="queryIndGroupPopList" type="normal" checkSession="true" >
