		</outputViews>
	</action>
	
	<!-- 瀛愯〃璇︽儏鏌ヨ -->
	<action id="queryIndModelIndModelGroupDetail" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ind.op.indmodel.indmodelgroup.QueryIndModelIndModelGroupDetailOp"/>
			<transition dest="queryDetail"/>
