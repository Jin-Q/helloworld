<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
			
	<!-- 删除记录 -->
	<action id="deleteArpLawMemberInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.DeleteArpLawMemberInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addArpLawMemberInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.AddArpLawMemberInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
		
	<!-- 债务人信息模块begin -->
	<!-- 获取债务人信息主页面 -->
	<action id="queryArpLawDebtorInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.QueryArpLawDebtorInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/arp/ArpLawMemberInfo/queryArpLawDebtorInfoList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得债务人查看页面 -->
	<action id="getArpLawDebtorInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.QueryArpLawDebtorInfoDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/arp/ArpLawMemberInfo/queryArpLawDebtorInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得债务人新增页面 -->
	<action id="getArpLawDebtorInfoAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/arp/ArpLawMemberInfo/addArpLawDebtorInfoRecord.jsp"/>
	</action>
	<!-- 分页查询记录列表 -->
	<action id="pageArpLawDebtorInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.QueryArpLawDebtorInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="ArpLawMemberInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 债务人信息模块end -->
	
	<!-- 被告人信息模块begin -->
	<!-- 获取被告人信息主页面 -->
	<action id="queryArpLawDefendantInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.QueryArpLawDefendantInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/arp/ArpLawMemberInfo/queryArpLawDefendantInfoList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得被告人查看页面 -->
	<action id="getArpLawDefendantInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arplawmemberinfo.QueryArpLawDefendantInfoDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/arp/ArpLawMemberInfo/queryArpLawDefendantInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得被告人新增页面 -->
	<action id="getArpLawDefendantInfoAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.pubAction.ArpPubNullOp"/>
			<transition dest="addPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="addPage" url="biz01line/arp/ArpLawMemberInfo/addArpLawDefendantInfoRecord.jsp"/>
		</outputViews>		
	</action>
	
	<!-- 被告人信息模块end -->
	
</actionDefines>