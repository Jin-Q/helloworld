<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryArpAssetRentAccInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.QueryArpAssetRentAccInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/arp/ArpAssetRentAccInfo/queryArpAssetRentAccInfoList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getArpAssetRentAccInfoUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.QueryArpAssetRentAccInfoDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/arp/ArpAssetRentAccInfo/updateArpAssetRentAccInfoRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateArpAssetRentAccInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.UpdateArpAssetRentAccInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryArpAssetRentAccInfoList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getArpAssetRentAccInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.QueryArpAssetRentAccInfoDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/arp/ArpAssetRentAccInfo/queryArpAssetRentAccInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getArpAssetRentAccInfoAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/arp/ArpAssetRentAccInfo/addArpAssetRentAccInfoRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addArpAssetRentAccInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.AddArpAssetRentAccInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryArpAssetRentAccInfoList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteArpAssetRentAccInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.DeleteArpAssetRentAccInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryArpAssetRentAccInfoList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageArpAssetRentAccInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.arp.op.arpassetrentaccinfo.QueryArpAssetRentAccInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="ArpAssetRentAccInfoList">
						<kColl>
						</kColl>
					</iColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



