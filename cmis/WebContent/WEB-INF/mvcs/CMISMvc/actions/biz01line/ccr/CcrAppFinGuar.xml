<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCcrAppFinGuarList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappfinguar.QueryCcrAppFinGuarListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppFinGuar/queryCcrAppFinGuarList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCcrAppFinGuarUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.CcrQueryAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppFinGuar/updateCcrAppFinGuarRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 保存指标选项数据及算分 -->
	<action id="creditRating" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CreditRatingOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" class="com.yucheng.cmis.view.ReturnDataView" dataName = "ccrScore"/>
		</outputViews>
	</action>
	<!-- 更新记录 -->
	<action id="updateCcrAppFinGuarRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappfinguar.UpdateCcrAppFinGuarRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCcrAppFinGuarList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCcrAppFinGuarViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.CcrQueryAppOperation"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/ccr/CcrAppFinGuar/queryCcrAppFinGuarDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增向导页面 -->
	<action id="getCcrAppFinGuarAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/ccr/CcrAppFinGuar/addCcrAppFinGuarRecord.jsp"/>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCcrAppFinGuarAddLeadPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrAddAppLeadOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppFinGuar/addCcrAppFinGuarLeadRecord.jsp"/>
		</outputViews>
	</action>
	<!-- 新增记录 -->
	<action id="addCcrAppFinGuarRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrAddAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="serno" class="com.ecc.emp.data.DataField" />
					<field id="cus_id" class="com.ecc.emp.data.DataField" />
					<field id="fnc_year" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 监管机构准入评级 （新增操作）-->
	<action id="getCcrOverseeAdmitRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrOverseeAdmitAddAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrOverseeAdmit/getCcrOverseeAdmit.jsp" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCcrAppFinGuarRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrDeleteAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCcrAppFinGuarList.do" />
		</outputViews>
	</action>
	<!-- 获得打印评级报告页面 -->
	<action id="getCcrReportPage" type="normal" checkSession="true" >
		<jspView id="reportPage" url="jsps/PrintReportTable.jsp"/>
	</action>
	<!-- 检查指定客户的评级申请记录是否存在 -->
	<action id="checkAppExist" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrCheckAppExistOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
							<field id="ccr_result" class="com.ecc.emp.data.DataField"/>
							<field id="ccr_message" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCcrAppFinGuarQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappfinguar.QueryCcrAppFinGuarListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CcrAppInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



