<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCcrAppInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinfo.QueryCcrAppInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInfo/queryCcrAppInfoList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取个人评级操作主页面 -->
	<action id="queryCcrAppIndivInfoList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinfo.QueryCcrAppIndivInfoListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInfo/queryCcrAppIndivInfoList.jsp" />
		</outputViews>
	</action>
	<!-- 获得个人评级新增页面 -->
	<action id="getCcrAppIndivInfoAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/ccr/CcrAppInfo/addCcrAppIndivInfoRecord.jsp"/>
	</action>
	<!-- 获取个人评级操作主页面（评级单独页签） -->
	<action id="queryCcrAppIndivInfoAllList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinfo.QueryCcrAppIndivInfoAllListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInfo/queryCcrAppIndivInfoAllList.jsp" />
		</outputViews>
	</action>
	<!-- 获得个人评级新增页面 -->
	<action id="getCcrAppIndivInfoAllAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/ccr/CcrAppInfo/addCcrAppIndivInfoAllRecord.jsp"/>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCcrAppInfoUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.CcrQueryAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInfo/updateCcrAppInfoRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateCcrAppInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinfo.UpdateCcrAppInfoRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCcrAppInfoList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCcrAppInfoViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.CcrQueryAppOperation"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/ccr/CcrAppInfo/queryCcrAppInfoDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增向导页面 -->
	<action id="getCcrAppInfoAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/ccr/CcrAppInfo/addCcrAppInfoRecord.jsp"/>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCcrAppInfoAddLeadPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrAddAppLeadOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInfo/addCcrAppInfoLeadRecord.jsp"/>
		</outputViews>
	</action>
	<!-- 新增记录 -->
	<action id="addCcrAppInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrAddAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInfo/jump2update.jsp" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCcrAppInfoRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrDeleteAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="msg" class="com.ecc.emp.data.DataField"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!--异步校验模型所属条线与客户所属条线是否拼配 -->
	<action id="doModelMatch" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrModelMatchOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="msg" class="com.ecc.emp.data.DataField"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 获得打印评级报告页面 -->
	<action id="getCcrReportPage" type="normal" checkSession="true" >
		<jspView id="reportPage" url="jsps/PrintReportTable.jsp"/>
	</action>

	<!-- 异步保存建议等级信息 -->
	<action id="updateSugGrade" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrUpdateSugGradeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="ccr_result" class="com.ecc.emp.data.DataField"/>
					<field id="ccr_message" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCcrAppInfoQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinfo.QueryCcrAppInfoListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CcrAppInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 分页查询记录列表（个人分页） -->
		<action id="pageCcrAppInfoIndivQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinfo.QueryCcrAppIndivInfoAllListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CcrAppInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
</actionDefines>



