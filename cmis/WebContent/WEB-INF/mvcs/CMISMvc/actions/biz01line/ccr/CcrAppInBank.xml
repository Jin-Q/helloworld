<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCcrAppInBankList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinbank.QueryCcrAppInBankListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInBank/queryCcrAppInBankList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCcrAppInBankUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.CcrQueryAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInBank/updateCcrAppInBankRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateCcrAppInBankRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinbank.UpdateCcrAppInBankRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCcrAppInBankList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCcrAppInBankViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.CcrQueryAppOperation"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/ccr/CcrAppInBank/queryCcrAppInBankDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增向导页面 -->
	<action id="getCcrAppInBankAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/ccr/CcrAppInBank/addCcrAppInBankRecord.jsp"/>
	</action>
	<!-- 获得新增页面 -->
	<action id="getCcrAppInBankAddLeadPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrAddAppLeadOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInBank/addCcrAppInBankLeadRecord.jsp"/>
		</outputViews>
	</action>
	<!-- 新增记录 -->
	<action id="addCcrAppInBankRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrAddAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrAppInBank/jump2update.jsp" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCcrAppInBankRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.CcrDeleteAppOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCcrAppInBankList.do" />
		</outputViews>
	</action>
	<!-- 获得打印评级报告页面 -->
	<action id="getCcrReportPage" type="normal" checkSession="true" >
		<jspView id="reportPage" url="jsps/PrintReportTable.jsp"/>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCcrAppInBankQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrappinbank.QueryCcrAppInBankListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CcrAppInfoList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



