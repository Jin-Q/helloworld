<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCcrRatDirectList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrRatDirect/queryCcrRatDirectList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取操作主页面 -->
	<action id="queryCcrRatDirectListLS" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectListLSOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrRatDirect/queryCcrRatDirectListLS.jsp" />
		</outputViews>
	</action>
	
	<action id="queryStartCcrDirectView" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/ccr/CcrRatDirect/viewCcrRatDirectDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCcrRatDirectUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrRatDirect/updateCcrRatDirectRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 xukaixi -->
	<action id="updateCcrRatDirectRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.UpdateCcrRatDirectRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCcrRatDirectViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/ccr/CcrRatDirect/queryCcrRatDirectDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCcrRatDirectAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.AddCcrRatDirectRecordDetailOp"/>
			<transition dest="addPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="addPage" url="biz01line/ccr/CcrRatDirect/addCcrRatDirectRecord.jsp"/>
		</outputViews>
	</action>
	
	
		<!--处理评级直接认定流程 -->
	<action id="startCcrDirectApp" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.StartCcrDirectAppOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrRatDirect/startResultTemp.jsp" />
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCcrRatDirectRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.AddCcrRatDirectRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/ccr/CcrRatDirect/insertOperationResult.jsp?url=getCcrRatDirectUpdatePage.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCcrRatDirectRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.DeleteCcrRatDirectRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!--校验客户是否是省级以上龙头企业-->
	<action id="checkCusComHdEnterprise" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.CheckCusComHdEnterpriseOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCcrRatDirectQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CcrAppInfoList">
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 竞争型/战略型客户准入审查表(1) -->
	<action id="CcrAdmitApproveTableOne" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryAdmitApproveTableOp"/>
			<transition dest="showPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="showPage" url="biz01line/report/fnc/reportShowPage.jsp"/>
		</outputViews>
	</action>
	
	<!-- 竞争型/战略型客户准入审查表(1)查看
	<action id="CcrAdmitTableOneView" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryAdmitApproveTableOp"/>
			<transition dest="showPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="showPage" url="biz01line/report/fnc/reportShowPageView.jsp"/>
		</outputViews>
	</action> -->
	
	<!-- 竞争型/战略型客户准入审查表(2) -->
	<action id="CcrAdmitApproveTableTwo" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryAdmitApproveTableOp"/>
			<transition dest="showPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="showPage" url="biz01line/report/fnc/reportShowPage.jsp"/>
		</outputViews>
	</action>
	
	<!-- 竞争型/战略型客户准入审查表(2)查看
	<action id="CcrAdmitTableTwoView" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryAdmitApproveTableOp"/>
			<transition dest="showPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="showPage" url="biz01line/report/fnc/reportShowPageView.jsp"/>
		</outputViews>
	</action> -->
	
	<!-- 分页查询记录列表 -->
	<action id="pageCcrRatDirectLSQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.ccr.op.ccrratdirect.QueryCcrRatDirectListLSOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CcrRatDirectList">
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
</actionDefines>