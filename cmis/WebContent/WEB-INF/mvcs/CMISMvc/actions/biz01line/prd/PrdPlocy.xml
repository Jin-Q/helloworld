<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryPrdPlocyList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdplocy.QueryPrdPlocyListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/prd/PrdPlocy/queryPrdPlocyList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getPrdPlocyUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="true" class="com.yucheng.cmis.biz01line.prd.op.prdplocy.QueryPrdPlocyDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/prd/PrdPlocy/updatePrdPlocyRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updatePrdPlocyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdplocy.UpdatePrdPlocyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryPrdPlocyList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getPrdPlocyViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdplocy.QueryPrdPlocyDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/prd/PrdPlocy/queryPrdPlocyDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getPrdPlocyAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/prd/PrdPlocy/addPrdPlocyRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addPrdPlocyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdplocy.AddPrdPlocyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryPrdPlocyList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deletePrdPlocyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdplocy.DeletePrdPlocyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryPrdPlocyList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pagePrdPlocyQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdplocy.QueryPrdPlocyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="PrdPlocyList">
						<kColl>
							<field id="schemecode" />
							<field id="schemedesc" />
							<field id="ifwarrant" />
							<field id="schemetype" />
							<field id="outline" />
							<field id="videocode" />
							<field id="comments" />
							<field id="inputid" />
							<field id="inputid_displayname" />
							<field id="inputdate" />
							<field id="orgid" />
							<field id="orgid_displayname" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



