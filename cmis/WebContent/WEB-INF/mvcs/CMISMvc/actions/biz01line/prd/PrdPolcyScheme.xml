		<action id="pagePrdPolcySchemeQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.prd.op.prdpolcyscheme.QueryPrdPolcySchemeListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="PrdPolcySchemeList">
						<kColl>
							<field id="schemeid" />
