<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController" />
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker" />
		<map id="outputViews" class="java.util.HashMap" />
		<map id="output" class="com.ecc.emp.data.KeyedCollection" />
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection" />
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection" />
		<map id="field" class="com.ecc.emp.data.DataField" />
	</classMap>

	<!-- 获取操作主页面 -->
	<action id="queryCusIndivList" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.QueryCusIndivListOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndiv/queryCusIndivList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取对私客户树 -->
	<action id="getCusIndivTree" type="normal" checkSession="true">
		<jspView id="addPage" url="biz01line/cus/cusindiv/CusIndiv/queryCusIndivTree.jsp" />
	</action>
	
	<!-- 开户成功后的跳转的编辑页面
	<action id="getCusIndivUpdatePageTwo" type="normal"
		checkSession="true">
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cusbase.GetCusInfoOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndiv/updateCusIndivRecord.jsp" />
		</outputViews>
	</action> -->

	<!-- 更新记录 -->
	<action id="updateCusIndivRecord" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.UpdateCusIndivRecordOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 暂存时更新记录 -->
	<action id="tempUpdateCusIndivRecord" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.TempUpdateCusIndivRecordOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndiv/updateCusIndivRecord.jsp" />
		</outputViews>
	</action>
	<!-- 补录完成 -->
	<action id="updateCusIndivRepRecord" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.UpdateCusIndivRepRecordOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="message" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得修改页面 -->
	<action id="getCusIndivUpdatePage" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.QueryCusIndivDetailOp" />
			<transition dest="updatePage" />
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cusindiv/CusIndiv/updateCusIndivRecord.jsp" />
		</outputViews>
	</action>

	<!-- 获得查看页面 -->
	<action id="getCusIndivViewPage" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.QueryCusIndivDetailOp" />
			<transition dest="updatePage" />
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cusindiv/CusIndiv/queryCusIndivRecord.jsp" />
		</outputViews>
	</action>

	<!-- 分页查询记录列表 -->
	<action id="pageCusIndivQuery" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.QueryCusIndivListOp" />
			<transition dest="queryListRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusBaseList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>

	<!--根据地址获取相应的邮政编码 -->
	<action id="getPostAddr2PcodeOne" type="normal" checkSession="true">
		<flowInvoker>
			<operation
				class="com.yucheng.cmis.biz01line.cus.op.cusindiv.GetPcodebyPostAddrOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" />
					<field id="post_code" />
					<field id="addSort" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 异步查询个人的基本信息! -->
	<action id="getCusIndivByCert" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindiv.QueryCusIndivByCertOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="cus_name" />
					<field id="cus_id" />
					<field id="cus_type" />
					<field id="com_hold_stk_amt" />
					<field id="indiv_sex" />
					<field id="indiv_occ" />
					<field id="indiv_ann_incm" />
					<field id="indiv_dt_of_birth" />
					<field id="indiv_com_job_ttl" />
					<field id="indiv_crtfctn" />
					<field id="indiv_dgr" />
					<field id="indiv_edt" />
					<field id="indiv_hobby" />
					<field id="phone" />
					<field id="mobile" />
					<field id="indiv_rsd_addr" />
					<field id="indiv_com_name" />
					<field id="cust_mgr" />
					<field id="main_br_id" />
					<field id="base_flag" />
				</output>
			</view>
		</outputViews>
	</action>
	
</actionDefines>
