<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusFixAuthorizeList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.QueryCusFixAuthorizeListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusother/CusFixAuthorize/queryCusFixAuthorizeList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCusFixAuthorizeUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.QueryCusFixAuthorizeDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusother/CusFixAuthorize/updateCusFixAuthorizeRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateCusFixAuthorizeRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.UpdateCusFixAuthorizeRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCusFixAuthorizeList.do" />
		</outputViews>
	</action>
	
	<!-- 异步修改状态 -->
	<action id="update4Status" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.Update4StatusOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCusFixAuthorizeViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.QueryCusFixAuthorizeDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cusother/CusFixAuthorize/queryCusFixAuthorizeDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCusFixAuthorizeAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/cusother/CusFixAuthorize/addCusFixAuthorizeRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCusFixAuthorizeRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.AddCusFixAuthorizeRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCusFixAuthorizeRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.DeleteCusFixAuthorizeRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCusFixAuthorizeList.do" />
		</outputViews>
	</action>
	
	<!-- 获得修改密码页面 -->
	<action id="getCusFixPassUpdatePage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/cusother/CusFixAuthorize/updateCusFixAuthorizePassWord.jsp"/>
	</action>
	
	<!-- 修改密码 -->
	<action id="updateCusFixAuthorizePass" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.UpdateCusFixAuthorizePass" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusFixAuthorizeQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusfixauthorize.QueryCusFixAuthorizeListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusFixAuthorizeList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



