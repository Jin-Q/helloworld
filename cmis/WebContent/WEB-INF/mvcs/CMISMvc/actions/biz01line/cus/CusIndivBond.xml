<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 鑾峰彇鎿嶄綔涓婚〉闈� -->
	<action id="queryCusIndivBondList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.QueryCusIndivBondListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndivBond/queryCusIndivBondList.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱缂栬緫椤甸潰 -->
	<action id="getCusIndivBondUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.QueryCusIndivBondDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndivBond/updateCusIndivBondRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 鏇存柊璁板綍 -->
	<action id="updateCusIndivBondRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.UpdateCusIndivBondRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏌ョ湅椤甸潰 -->
	<action id="getCusIndivBondViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.QueryCusIndivBondDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cusindiv/CusIndivBond/queryCusIndivBondDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏂板椤甸潰 -->
	<action id="getCusIndivBondAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/cusindiv/CusIndivBond/addCusIndivBondRecord.jsp"/>
	</action>
	
	<!-- 鏂板璁板綍 -->
	<action id="addCusIndivBondRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.AddCusIndivBondRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 鍒犻櫎璁板綍 -->
	<action id="deleteCusIndivBondRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.DeleteCusIndivBondRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 鍒嗛〉鏌ヨ璁板綍鍒楄〃 -->
		<action id="pageCusIndivBondQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivbond.QueryCusIndivBondListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusIndivBondList">
						<kColl>
							<field id="cus_id" />
							<field id="indiv_bond_id" />
							<field id="indiv_bond_typ" />
							<field id="indiv_bond_eva_amt" />
							<field id="indiv_bond_pub" />
							<field id="indiv_bond_str_dt" />
							<field id="indiv_bond_end_dt" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



