<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusScaleApplyList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.QueryCusScaleApplyListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscogniz/CusScaleApply/queryCusScaleApplyList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCusScaleApplyUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.QueryCusScaleApplyDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscogniz/CusScaleApply/updateCusScaleApplyRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateCusScaleApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.UpdateCusScaleApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCusScaleApplyViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.QueryCusScaleApplyDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cuscogniz/CusScaleApply/queryCusScaleApplyDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 查询(历史)记录列表 -->
	<action id="queryCusScaleApplyHisList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.QueryCusScaleApplyHisListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/cus/cuscogniz/CusScaleApply/queryCusScaleApplyHisList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCusScaleApplyAddPage" type="normal" checkSession="true" >
			<jspView id="addPage" url="biz01line/cus/cuscogniz/CusScaleApply/addCusScaleApplyRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCusScaleApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.AddCusScaleApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCusScaleApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.DeleteCusScaleApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusScaleApplyQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.cusscale.QueryCusScaleApplyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusScaleApplyList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



