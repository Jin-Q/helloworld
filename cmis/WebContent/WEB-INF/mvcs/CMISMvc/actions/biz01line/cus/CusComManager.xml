<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 查询记录列表 -->
	<action id="queryCusComManagerList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.QueryCusComManagerListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/cus/cuscom/CusComManager/queryCusComManagerList.jsp" />
		</outputViews>
	</action>
	<!-- 复制法人代表为实际控制人 -->
	<action id="copyFarenToKongzhiren" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.CopyFarenToKongzhirenOp"/>
			<transition dest="asynPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView id="asynPage" class="com.yucheng.cmis.view.JSONView" >
				<output>
					<field id="backMsg"></field>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 校验列表页面是否存在"实际控制人" -->
	<action id="checkComMrgTyp" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.CheckComMrgTypOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView id="queryListRes" class="com.yucheng.cmis.view.JSONView" >
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 获取新增页面 -->
	<action id="getCusComManagerAddPage" type="normal" checkSession="true" >
		<jspView id="queryListRes" url="biz01line/cus/cuscom/CusComManager/addCusComManagerRecord.jsp" />
	</action>
	
	<!-- 新增信息 -->
	<action id="addCusComManagerRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.AddCusComManagerRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<view id="queryListRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!--异步从cus_com表中查询信息 -->
	<action id="getCusCom4manager" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.CusCom4managerOp" />
			<transition dest="queryDetailRes" />
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<iColl id="cusList" class="com.ecc.emp.data.IndexedCollection">
						<kColl class="com.ecc.emp.data.KeyedCollection" />
					</iColl>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!--异步从cus_indiv表中查询信息 -->
	<action id="getCusIndiv4manager" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.CusIndiv4managerOp" />
			<transition dest="queryDetailRes" />
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<iColl id="cusList" class="com.ecc.emp.data.IndexedCollection">
						<kColl class="com.ecc.emp.data.KeyedCollection" />
					</iColl>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCusComManagerUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.QueryCusComManagerDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscom/CusComManager/updateCusComManagerRecord.jsp" />
		</outputViews>
	</action>

	<!-- 修改信息 -->
	<action id="updateCusComManagerRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.UpdateCusComManagerRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<view id="queryListRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 删除信息 -->
	<action id="deleteCusComManagerRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.DeleteCusComManagerRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
			<outputViews>
			<view id="queryListRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 查询详情 -->
	<action id="queryCusComManagerDetail" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.QueryCusComManagerDetailOp"/>
			<transition dest="queryDetailRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryDetailRes" url="biz01line/cus/cuscom/CusComManager/queryCusComManagerDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageCusComManagerQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscommanager.QueryCusComManagerListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusComManagerList">
						<kColl>
							<field id="com_mrg_name" />
							<field id="com_mrg_typ" />
							<field id="com_mrg_cert_typ" />
							<field id="com_mrg_cert_code" />
							<field id="input_br_id" />
							<field id="input_id" />
							<field id="cus_id" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>
