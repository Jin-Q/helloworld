<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 鑾峰彇鎿嶄綔涓婚〉闈� -->
	<action id="queryCusIndivInsurancesList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.QueryCusIndivInsurancesListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndivInsurances/queryCusIndivInsurancesList.jsp" />
		</outputViews>
	</action>
	
	<!-- 鑾峰緱缂栬緫椤甸潰 -->
	<action id="getCusIndivInsurancesUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.QueryCusIndivInsurancesDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusindiv/CusIndivInsurances/updateCusIndivInsurancesRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 鏇存柊璁板綍 -->
	<action id="updateCusIndivInsurancesRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.UpdateCusIndivInsurancesRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 鑾峰緱鏌ョ湅椤甸潰 -->
	<action id="getCusIndivInsurancesViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.QueryCusIndivInsurancesDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cusindiv/CusIndivInsurances/queryCusIndivInsurancesDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCusIndivInsurancesAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/cusindiv/CusIndivInsurances/addCusIndivInsurancesRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCusIndivInsurancesRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.AddCusIndivInsurancesRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 鍒犻櫎璁板綍 -->
	<action id="deleteCusIndivInsurancesRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.DeleteCusIndivInsurancesRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusIndivInsurancesQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusindivinsurances.QueryCusIndivInsurancesListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusIndivInsuList">
						<kColl>
							<field id="cus_id" />
							<field id="indiv_ins_id" />
							<field id="indiv_ins_cvg" />
							<field id="indiv_ins_com" />
							<field id="indiv_ins_tot_amt" />
							<field id="indiv_ins_str_dt" />
							<field id="indiv_ins_end_dt" />
							<field id="indiv_ins_amt" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



