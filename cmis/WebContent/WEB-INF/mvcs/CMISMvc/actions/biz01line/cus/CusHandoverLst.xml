<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusHandoverLstList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.cushand.op.cushandoverlst.QueryCusHandoverLstListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusHandoverLst/queryCusHandoverLstList.jsp" />
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCusHandoverLstRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.cushand.op.cushandoverlst.AddCusHandoverLstRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusHandoverApp/refreshList.jsp?method=doQuery()" />
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteCusHandoverLstRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.cushand.op.cushandoverlst.DeleteCusHandoverLstRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 一次移交所有客户 -->
    <action id="addCusHandoverLstRecordMany" type="normal" checkSession="true" >
        <flowInvoker>
            <operation class="com.yucheng.cmis.biz01line.cus.cushand.op.cushandoverlst.AddCusHandoverLstRecordOp"/>
            <transition dest="retPage"/>
        </flowInvoker>
        <outputViews>
           <view id="retPage" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
                    <field id="flag" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
        </outputViews>
    </action>
	
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusHandoverLstQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.cushand.op.cushandoverlst.QueryCusHandoverLstListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusHandoverLstList">
						<kColl>
							
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



