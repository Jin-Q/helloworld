<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 查询记录列表 -->
	<action id="queryCusGrpInfoApplyList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.QueryCusGrpInfoApplyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="biz01line/cus/group/CusGrpInfoApply/queryCusGrpInfoApplyList.jsp" />
		</outputViews>
	</action>

	<!-- 获取新增页面 --> 
	<action id="getCusGrpInfoApplyAddPage" type="normal" checkSession="true" >
			<jspView id="queryListRes" url="biz01line/cus/group/CusGrpInfoApply/addCusGrpInfoApplyRecord.jsp" />
	</action>
	
	<!-- 获取新增页面(变更) --> 
	<action id="getCusGrpInfoApplyModAddPage" type="normal" checkSession="true" >
		<jspView id="queryListRes" url="biz01line/cus/group/CusGrpInfoApply/addCusGrpInfoApplyModRecord.jsp" />
	</action>
	
	<!-- 新增信息 -->
	<action id="addCusGrpInfoApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.AddCusGrpInfoApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCusGrpInfoApplyUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.QueryCusGrpInfoApplyDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/group/CusGrpInfoApply/updateCusGrpInfoApplyRecord.jsp" />
		</outputViews>
	</action>

	<!-- 修改信息 -->
	<action id="updateCusGrpInfoApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.UpdateCusGrpInfoApplyRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<view id="queryListRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 删除信息 -->
	<action id="deleteCusGrpInfoApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.DeleteCusGrpInfoApplyRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<view id="queryListRes" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
		</outputViews>
	</action>
	
	<!-- 查询详情 -->
	<action id="queryCusGrpInfoApplyDetail" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.QueryCusGrpInfoApplyDetailOp"/>
			<transition dest="queryDetailRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryDetailRes" url="biz01line/cus/group/CusGrpInfoApply/queryCusGrpInfoApplyDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageCusGrpInfoApplyQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.group.ops.cusgrpinfoapply.QueryCusGrpInfoApplyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusGrpInfoApplyList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
		
</actionDefines>