<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryModifyHistoryList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.QueryModifyHistoryListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/ModifyHistory/queryModifyHistoryList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getModifyHistoryUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.QueryModifyHistoryDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/ModifyHistory/updateModifyHistoryRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateModifyHistoryRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.UpdateModifyHistoryRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryModifyHistoryList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getModifyHistoryViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.QueryModifyHistoryDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/ModifyHistory/queryModifyHistoryDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getModifyHistoryAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/ModifyHistory/addModifyHistoryRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addModifyHistoryRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.AddModifyHistoryRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryModifyHistoryList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteModifyHistoryRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.DeleteModifyHistoryRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryModifyHistoryList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageModifyHistoryQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.QueryModifyHistoryListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="ModifyHistoryList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
                        <field id="currentPage" />
                        <field id="maxLine" />
                        <field id="recordSize" />
                    </kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
        <action id="pageModifyHistoryViewQuery" type="normal" checkSession="true" >
        <flowInvoker>
            <operation class="com.yucheng.cmis.biz01line.cus.op.modifyhistory.QueryModifyHistoryDetailOp"/>
            <transition dest="queryListRes"/>
        </flowInvoker>
        <outputViews>
            <jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
                <output>
                    <iColl id="ModifyHistoryDetailList">
                        <kColl>
                        </kColl>
                    </iColl>
                    <kColl id="pageInfo">
                        <field id="currentPage" />
                        <field id="maxLine" />
                        <field id="recordSize" />
                    </kColl>
                </output>
            </jsonView>
        </outputViews>
    </action>
	 
</actionDefines>



