		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusBlkList/importResult.jsp" />
		</outputViews>
	</action>
	<!-- 导出成生成excel -->
	<action id="cusBlkListExpBatchToExcel"  type="normal"  checkSession="true" >                                                                                           
		<flowInvoker>                                                                                                                                            
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusblklist.CusBlkListExpBatchToExcelOp"/>                                                                    
			<transition dest="retPage"/>                                                                                                                           
		</flowInvoker>                                                                                                                                           
		<outputViews>                                                                                                                                            
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">                                                                                           
 				<excelWorkbook fileName="CusBlkList.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">                                                          
 					<excelSheet id="1" name="共享客户清单" isTitle="true" iCollName="CusBlkListList" class="com.yucheng.cmis.view.excel.ExcelSheet">                       
 						<excelColumn dataName="xuhao" name="序号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                              
 						<excelColumn dataName="cus_name" name="客户名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>      
 						<excelColumn dataName="cert_type" name="证件类型"  dictName="STD_ZB_CERT_TYP" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                       
 						<excelColumn dataName="cert_code" name="证件号码" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                          
 						<excelColumn dataName="legal_name" name="法定代表人" class="com.yucheng.cmis.view.excel.ExcelColumn"/>     
 						<excelColumn dataName="legal_addr_displayname"  name="地址" class="com.yucheng.cmis.view.excel.ExcelColumn"/>   
 						<excelColumn dataName="black_type"  name="客户类型"  dictName="STD_ZB_EVENT_TYP" class="com.yucheng.cmis.view.excel.ExcelColumn"/> 
 						<excelColumn dataName="black_reason"  name="客户描述" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                      
 					 	<excelColumn dataName="data_source" name="数据来源"  dictName="STD_ZB_DATA_SOURCE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>  
 					 	<excelColumn dataName="input_br_id_displayname" name="登记机构"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					 	<excelColumn dataName="input_id_displayname" name="登记人"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					 	<excelColumn dataName="input_date" name="登记日期"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					 	<excelColumn dataName="manager_br_id_displayname" name="管理机构"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					 	<excelColumn dataName="manager_id_displayname" name="责任人"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					 	<excelColumn dataName="status" name="状态"   dictName="STD_CUS_BLK_STATUS" class="com.yucheng.cmis.view.excel.ExcelColumn"/>                                     
 					</excelSheet>                                                                                                                                      
 				</excelWorkbook>                                                                                                                                     
 			</view>                                                                                                                                                
		</outputViews>                                                                                                                                           
	</action>    
		
	
	
	<!-- 获得编辑页面 -->
	<action id="getCusBlkListUpdatePage" type="normal" checkSession="true" >
