<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusModifyHistoryCfgList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.QueryCusModifyHistoryCfgListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/CusModifyHistoryCfg/queryCusModifyHistoryCfgList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增时引导页面 -->
	<action id="getCusModifyCfgLeadPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/CusModifyHistoryCfg/addCusModifyCfgLead.jsp"/>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getCusModifyHistoryCfgUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.QueryCusModifyHistoryCfgDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/CusModifyHistoryCfg/updateCusModifyHistoryCfgRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateCusModifyHistoryCfgRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.UpdateCusModifyHistoryCfgRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/CusModifyHistoryCfg/getAddModifyHistoryCfgList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCusModifyHistoryCfgViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.QueryCusModifyHistoryCfgDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/CusModifyHistoryCfg/queryCusModifyHistoryCfgDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCusModifyHistoryCfgAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.GetCusModifyHistoryCfgAddOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/CusModifyHistoryCfg/addCusModifyHistoryCfgRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCusModifyHistoryCfgRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.AddCusModifyHistoryCfgRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/CusModifyHistoryCfg/getAddModifyHistoryCfgList.jsp" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteCusModifyHistoryCfgRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.DeleteCusModifyHistoryCfgRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCusModifyHistoryCfgList.do" />
		</outputViews>
	</action>
	
	<!-- 更新配置字段字典信息 -->
	<action id="updateModifyHistoryCfgRecords" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.UpdateModifyHistoryCfgRecordsOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 检查表模型配置是否已经存在 -->
	<action id="checkTableModelIsCfg" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.CheckTableModelIsCfgOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusModifyHistoryCfgQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusmodifyhistorycfg.QueryCusModifyHistoryCfgListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusModifyHistoryCfgList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



