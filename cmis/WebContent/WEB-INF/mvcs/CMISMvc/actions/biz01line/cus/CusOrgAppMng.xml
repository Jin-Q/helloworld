<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusOrgAppMngList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.QueryCusOrgAppMngListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscogniz/EvalOrgCognizAppMng/queryCusOrgAppMngList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取评估机构信息pop框 -->
	<action id="queryCusOrgAppMngPopList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.QueryCusOrgAppMngListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscogniz/EvalOrgCognizAppMng/queryCusOrgAppMngPopList.jsp" />
		</outputViews>
	</action>
	<!-- 获得编辑页面 -->
	<action id="getCusOrgAppMngUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.QueryCusOrgAppMngDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscogniz/EvalOrgCognizAppMng/updateCusOrgAppMngRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateCusOrgAppMngRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.UpdateCusOrgAppMngRecordOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<view id="updatePage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getCusOrgAppMngViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.QueryCusOrgAppMngDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/cus/cuscogniz/EvalOrgCognizAppMng/queryCusOrgAppMngDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCusOrgAppMngAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/cuscogniz/EvalOrgCognizAppMng/addCusOrgAppMngRecord.jsp"/>
	</action>
		
	<!-- 准入审批表 -->
	<action id="RpCusOrgApproveRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.psp.op.pspchecktask.GetCurCheckTabHelp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cuscogniz/EvalOrgCognizAppMng/rpCusOrgApproveRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addCusOrgAppMngRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.AddCusOrgAppMngRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCusOrgAppMngList.do" />
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteCusOrgAppMngRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.DeleteCusOrgAppMngRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryCusOrgAppMngList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusOrgAppMngQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscogniz.evalorgcognizappmng.QueryCusOrgAppMngListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusOrgAppMngList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>