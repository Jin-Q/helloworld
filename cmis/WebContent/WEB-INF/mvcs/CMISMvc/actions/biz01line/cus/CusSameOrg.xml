	<!--modified by lisj 2015-2-28 需求编号：【XD150213011】增加同业授信批量导入功能, 导出成生成excel -->
	<action id="expBatchToExcel" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cussameorg.ExpBatchToExcelOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">
 				<excelWorkbook fileName="CusSameOrg.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">
 					<excelSheet id="1" name="同业客户" isTitle="true" iCollName="CusSameOrgList" class="com.yucheng.cmis.view.excel.ExcelSheet">
 						<excelColumn dataName="cus_id" name="客户码" cellType="string" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="same_org_cnname" name="同业机构(行)名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="same_org_type" name="同业机构类型" dictName="STD_ZB_INTER_BANK_ORG" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="country" name="国别" dictName="STD_GB_2659-2000" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="reg_cap_amt" name="注册资金(万元)" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="assets" name="总资产(万元)" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="crd_grade" name="信用等级" dictName="STD_ZB_FINA_GRADE" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					</excelSheet>
 				</excelWorkbook>
 			</view>
		</outputViews>
	</action>
	
