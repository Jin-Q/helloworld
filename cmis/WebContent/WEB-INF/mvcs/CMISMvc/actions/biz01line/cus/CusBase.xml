	<!-- 检查帐号唯一性 -->
	<action id="getAccNoOnly" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscom.QueryAccOp" />
			<transition dest="queryDetailRes" />
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="accNoFlag" />
				</output>
			</view>
		</outputViews>
	</action>

	<!-- 客户信息关联搜索 -->
	<action id="GetCusRelTreeOp" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.cusRelTree.op.GetCusRelTreeOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusBase/queryCusRelTree.jsp" />
		</outputViews>
	</action>
	<action id="GetIndivCusRelTreeOp" type="normal" checkSession="true">
        <flowInvoker>
            <operation class="com.yucheng.cmis.biz01line.cus.cusRelTree.op.GetIndivCusRelTreeOp" />
            <transition dest="retPage" />
        </flowInvoker>
        <outputViews>
            <jspView id="retPage" url="biz01line/cus/cusbase/CusBase/queryCusRelTree.jsp" />
        </outputViews>
    </action>

	<!-- 获得查看页面 -->
	<action id="getCusViewPage" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.GetCusRecordOp" />
			<transition dest="com" condition="($retValue='com')" />
			<transition dest="indiv" condition="($retValue='indiv')" />
		</flowInvoker>
		<outputViews>
			<jspView id="com" url="biz01line/cus/cuscom/CusCom/queryCusComTree.jsp" />
			<jspView id="indiv" url="biz01line/cus/cusindiv/CusIndiv/queryCusIndivTree.jsp" />
		</outputViews>
	</action>
	
	<!-- 开正式户时从Ecif查询客户信息 -->
	<action id="GetCusFromEcif" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.conOtherSys.GetCusFromEcif" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<iColl id="cusList" class="com.ecc.emp.data.IndexedCollection">
						<kColl class="com.ecc.emp.data.KeyedCollection" />
					</iColl>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 开临时户时将客户信息发送Ecif并返回客户码 -->
	<action id="SendCusMsg" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.conOtherSys.SendCusMsg" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" />
					<field id="cusId" />
					<field id="errorMsg" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 从Ecif获取基本存款账户信息 -->
	<action id="queryBasAccByCusId" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.conOtherSys.QueryBasAccByCusId" />
			<transition dest="retPage"  />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<iColl id="accList" class="com.ecc.emp.data.IndexedCollection">
						<kColl class="com.ecc.emp.data.KeyedCollection" />
					</iColl> 
					<field id="msg" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得客户类别 对公（com）对私（indiv） -->
	<action id="checkCusTypeById" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.GetCusRecordOp" />
			<transition dest="retPage"  />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得客户类别 对公（com）对私（indiv） -->
	<action id="queryCusTypeByCusId" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryCusTypeByCusIdOp" />
			<transition dest="retPage"  />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" />
				</output>
			</view>
		</outputViews>
	</action>
	
</actionDefines>



