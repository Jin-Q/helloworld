<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController" />
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker" />
		<map id="outputViews" class="java.util.HashMap" />
		<map id="output" class="com.ecc.emp.data.KeyedCollection" />
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection" />
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection" />
		<map id="field" class="com.ecc.emp.data.DataField" />
	</classMap>
	<!-- 获得对公客户新增页面 -->
	<action id="newCusComAddPage" type="normal" checkSession="true">
		<jspView id="addPage" url="biz01line/cus/cusbase/CusBase/newCusComRecord.jsp" />
	</action>
	<!-- 获得对私客户新增页面 -->
	<action id="newCusIndivAddPage" type="normal" checkSession="true">
		<jspView id="addPage" url="biz01line/cus/cusbase/CusBase/newCusIndivRecord.jsp" />
	</action>

	<!-- 对私新增记录 -->
	<action id="addCusBaseIndivRecord" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.AddCusBaseRecordOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="cus_id" class="com.ecc.emp.data.DataField" />
					<field id="cusName" class="com.ecc.emp.data.DataField"/>
					<field id="mainBrId" class="com.ecc.emp.data.DataField" />
					<field id="custMgr" class="com.ecc.emp.data.DataField" />
					<field id="custMgrName" class="com.ecc.emp.data.DataField" />
					<field id="mainBrName" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>

	<!-- 对公新增记录 -->
	<action id="addCusBaseComRecord" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.AddCusBaseRecordOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="cus_id" class="com.ecc.emp.data.DataField" />
					<field id="mainBrId" class="com.ecc.emp.data.DataField" />
					<field id="custMgr" class="com.ecc.emp.data.DataField" />
					<field id="custMgrName" class="com.ecc.emp.data.DataField" />
					<field id="mainBrName" class="com.ecc.emp.data.DataField" />
					<field id="cusName" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>

	<!-- 客户信息pop -->
	<action id="queryCusBasePop" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryCusBaseListOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusBase/queryCusBaseListPop.jsp" />
		</outputViews>
	</action>

	<!-- 所有客户信息pop -->
	<action id="queryAllCusPop" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryAllCusListOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusBase/queryAllCusListPop.jsp" />
		</outputViews>
	</action>
	
	<!-- 查询关联客户POP框（个人额度申请专用，包含主借款人及配偶、共同债务人及配偶、保证人及配偶） -->
	<action id="queryRelaCusByLmtSernoPop" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryRelaCusByLmtSernoListOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbase/CusBase/queryRelaCusByLmtSernoListPop.jsp" />
		</outputViews>
	</action>

	<!-- 分页查询记录列表 -->
	<action id="pageCusBaseQuery" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryCusBaseListOp" />
			<transition dest="queryListRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusBaseList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<action id="pageAllCusQuery" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryAllCusListOp" />
			<transition dest="queryListRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusBaseList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<action id="pageRelaCusByLmtSernoQuery" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.QueryRelaCusByLmtSernoListOp" />
			<transition dest="queryListRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusBaseList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>

	<!-- 根据客户码获取客户名称
	<action id="getCusBaseInfo" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbase.getCusBaseByCusId" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
