	<action id="getCusJsonByCert" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.custmp.QueryCusJsonByCertOpForDt" />
			<transition dest="queryDetailRes" />
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<iColl id="cusList" class="com.ecc.emp.data.IndexedCollection">
						<kColl class="com.ecc.emp.data.KeyedCollection" />
					</iColl>
				</output>
			</view>
		</outputViews>
	</action>
	<!-- 贷款卡号唯一性检查 -->
	<action id="getCusLoanCardOnly" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscom.QueryCusLoanCardOp" />
			<transition dest="queryDetailRes" />
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="loanCardIdFlag" />
				</output>
			</view>
		</outputViews>
	</action>
	<!-- 检查证件号码唯一性
	<action id="getCusCertCodeOnly" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cuscom.QueryCertCodeOp" />
			<transition dest="queryDetailRes" />
		</flowInvoker>
		<outputViews>
			<view id="queryDetailRes" class="com.yucheng.cmis.view.JSONView">
