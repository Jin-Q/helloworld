<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusAffilList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusaffil.QueryCusAffilListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusother/CusAffil/queryCusAffilList.jsp" />
		</outputViews>
	</action>
	
	<!-- 导出成excel -->
	<action id="expCusAffilToExcel" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusaffil.ExpCusAffilToExcelOp"/>
			<transition dest="retPage"/>
		</flowInvoker>		
		<outputViews>
		   <view id="retPage" class="com.yucheng.cmis.view.ExcelView">
 				<excelWorkbook fileName="CusAffil.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">
 					<excelSheet id="1" name="关联关系" isTitle="true" iCollName="CusSameOrgList" class="com.yucheng.cmis.view.excel.ExcelSheet">
 						<excelColumn dataName="affil_rela" name="关联关系" dictName="STD_ZB_AFFIL_RELA" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="cus_id" name="客户号" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="cus_name" name="客户名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="auth_crd_balance" name="授信金额" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="cus_affil_id" name="关联客户码"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="cus_affil_name" name="关联客户名称"  class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="affil_auth_crd_balance" name="关联授信金额" cellType='numeric' class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					</excelSheet>
 				</excelWorkbook>
 			</view>
		</outputViews>
	</action>
		
	<!-- 分页查询记录列表 -->
		<action id="pageCusAffilQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusother.cusaffil.QueryCusAffilListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusAffilList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



