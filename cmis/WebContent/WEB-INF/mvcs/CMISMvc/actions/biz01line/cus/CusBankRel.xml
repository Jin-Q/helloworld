<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryCusBankRelList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbankcusrel.QueryCusBankRelListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/cus/cusbank/CusBankRel/queryCusBankRelList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getCusBankRelAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/cus/cusbank/CusBankRel/addCusBankRelRecord.jsp"/>
	</action>
	
	
	<!-- 分页查询记录列表 -->
		<action id="pageCusBankRelQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.cus.op.cusbankcusrel.QueryCusBankRelListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="CusBankRelList">
						<kColl>
							<field id="cus_id" />
							<field id="cus_name" />
							<field id="cus_bank_rel" />
							<field id="cus_bank_rel_name" />
							<field id="reg_name" />
							<field id="reg_org" />
							<field id="reg_date" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



