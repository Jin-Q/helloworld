<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryQryParamDicList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.QueryQryParamDicListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/qry/QryParamDic/queryQryParamDicList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getQryParamDicUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.QueryQryParamDicDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/qry/QryParamDic/updateQryParamDicRecord.jsp" />
		</outputViews>
	</action>
	<!-- 获得POP弹出页面 -->
	<action id="getQryParamPopPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.QueryQryParamDicListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/qry/QryParamDic/queryQryParamDicPopList.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateQryParamDicRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.UpdateQryParamDicRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryQryParamDicList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getQryParamDicViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.QueryQryParamDicDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/qry/QryParamDic/queryQryParamDicDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getQryParamDicAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/qry/QryParamDic/addQryParamDicRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addQryParamDicRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.AddQryParamDicRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryQryParamDicList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteQryParamDicRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.DeleteQryParamDicRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryQryParamDicList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageQryParamDicQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.qry.op.qryparamdic.QueryQryParamDicListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="QryParamDicList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>					
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



