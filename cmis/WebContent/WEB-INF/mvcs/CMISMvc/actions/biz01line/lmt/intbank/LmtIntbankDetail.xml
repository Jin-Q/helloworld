<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtIntbankDetailList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.QueryLmtIntbankDetailListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/intbank/LmtIntbankDetail/queryLmtIntbankDetailList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getLmtIntbankDetailUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.QueryLmtIntbankDetailDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/intbank/LmtIntbankDetail/updateLmtIntbankDetailRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtIntbankDetailRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.UpdateLmtIntbankDetailRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtIntbankDetailList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtIntbankDetailViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.QueryLmtIntbankDetailDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/intbank/LmtIntbankDetail/queryLmtIntbankDetailDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getLmtIntbankDetailAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="lmt/intbank/LmtIntbankDetail/addLmtIntbankDetailRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addLmtIntbankDetailRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.AddLmtIntbankDetailRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtIntbankDetailList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteLmtIntbankDetailRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.DeleteLmtIntbankDetailRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtIntbankDetailList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtIntbankDetailQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtintbankdetail.QueryLmtIntbankDetailListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtIntbankDetailContList">
						<kColl>
						</kColl>
					</iColl>
					<iColl id="LmtIntbankDetailIqpList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



