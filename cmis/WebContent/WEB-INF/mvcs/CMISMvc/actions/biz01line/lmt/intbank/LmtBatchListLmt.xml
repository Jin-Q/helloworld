<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtBatchListLmtList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.QueryLmtBatchListLmtListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/intbank/LmtBatchListLmt/queryLmtBatchListLmtList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getLmtBatchListLmtUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.QueryLmtBatchListLmtDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/intbank/LmtBatchListLmt/updateLmtBatchListLmtRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtBatchListLmtRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.UpdateLmtBatchListLmtRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtBatchListLmtList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtBatchListLmtViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.QueryLmtBatchListLmtDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/intbank/LmtBatchListLmt/queryLmtBatchListLmtDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getLmtBatchListLmtAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/intbank/LmtBatchListLmt/addLmtBatchListLmtRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addLmtBatchListLmtRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.AddLmtBatchListLmtRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtBatchListLmtList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteLmtBatchListLmtRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.DeleteLmtBatchListLmtRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtBatchListLmtList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtBatchListLmtQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.intbank.lmtbatchlistlmt.QueryLmtBatchListLmtListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtBatchListLmtList">
						<kColl>
							<field id="serno" />
							<field id="cus_id" />
							<field id="cur_type" />
							<field id="lmt_amt" />
							<field id="term_type" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



