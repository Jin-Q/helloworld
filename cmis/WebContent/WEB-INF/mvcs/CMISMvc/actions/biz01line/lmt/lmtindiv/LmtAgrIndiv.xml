<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtAgrIndivList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.QueryLmtAgrIndivListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAgrIndiv/queryLmtAgrIndivList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtAgrIndivListForCusTree" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.QueryLmtAgrIndivListForCusTreeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAgrIndiv/queryLmtAgrIndivListForCusTree.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtAgrIndivRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.UpdateLmtAgrIndivRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtAgrIndivList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtAgrIndivViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.QueryLmtAgrIndivDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/LmtIndiv/LmtAgrIndiv/queryLmtAgrIndivDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得自助额度页面 -->
	<action id="getLmtAgrIndivSelfPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.GetLmtAgrIndivSelfPageOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAgrIndiv/lmtAgrIndivSelfRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addLmtAgrIndivRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.AddLmtAgrIndivRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtAgrIndivList.do" />
		</outputViews>
	</action>
	
	<!-- 检查是否有在途的冻结申请 -->
	<action id="checkLmtAgrIndivFrozenApp" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.CheckLmtAgrIndivFrozenOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<!-- <jspView id="retPage" url="biz01line/lmt/LmtAgrInfo/getLmtFrozenAppRecord.jsp" /> -->
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="serno" class="com.ecc.emp.data.DataField"/>
					<field id="appStatus" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 生成冻结申请 -->
	<action id="getLmtIndivFrozenUpdateRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.GetLmtAgrIndivFrozenAppOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAgrIndiv/addLmtFrozenAgrIndiv.jsp" />
		</outputViews>
	</action>
	
	<!-- 查看冻结申请详情 -->
	<action id="getLmtIndivFrozenUpdOrViePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAgrIndiv/queryLmtFrozenAgrIndiv.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtAgrIndivQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.QueryLmtAgrIndivListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAgrIndivList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtAgrIndivForCusTreeQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtagrindiv.QueryLmtAgrIndivListForCusTreeOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAgrIndivList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



