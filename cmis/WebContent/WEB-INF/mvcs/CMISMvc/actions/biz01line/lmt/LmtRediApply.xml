					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<action id="queryLmtAppJointCoopRediList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtrediapply.QueryLmtAppJointCoopRediListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtRediApply/queryLmtAppJointCoopRediList.jsp" />
		</outputViews>
	</action>
	<!-- 分页查询记录列表 -->
		<action id="pageLmtAppJointCoopRediQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtrediapply.QueryLmtAppJointCoopRediListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
