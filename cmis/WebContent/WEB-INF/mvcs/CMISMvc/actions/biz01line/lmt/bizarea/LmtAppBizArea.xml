<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtAppBizAreaList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.QueryLmtAppBizAreaListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/bizarea/LmtAppBizArea/queryLmtAppBizAreaList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑修改页面 -->
	<action id="getLmtAppBizAreaUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.QueryLmtAppBizAreaDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/bizarea/LmtAppBizArea/updateLmtAppBizAreaRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtAppBizAreaRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.UpdateLmtAppBizAreaRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
		    <view id="retPage" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
                    <field id="flag" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="queryLmtAppBizAreaDetails" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.QueryLmtAppBizAreaDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/bizarea/LmtAppBizArea/queryLmtAppBizArea.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增向导页面 -->
	<action id="getLmtAppBizAreaAddWizPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/bizarea/LmtAppBizArea/addLmtAppBizAreaWiz.jsp"/>
	</action>
	<!-- 根据OP返回值获得新增页面 -->
	<action id="getLmtAppBizAreaAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/bizarea/LmtAppBizArea/addLmtAppBizAreaRecord.jsp"/>
	</action>
	<!-- 新增保存  -->
	<action id="addLmtAppBizAreaRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.AddLmtAppBizAreaRecordOp"/>
				<transition dest="addPage" />
		</flowInvoker>
		<outputViews>
			<view id="addPage" class="com.yucheng.cmis.view.JSONView">
                <output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="serno" class="com.ecc.emp.data.DataField"/>
                </output>
            </view>
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteLmtAppBizAreaRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.DeleteLmtAppBizAreaRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtAppBizAreaQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtappbizarea.QueryLmtAppBizAreaListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAppBizAreaList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



