<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtAppIndivList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivListOp"/>
			<transition dest="appPage" condition="($retValue='app')"/>
			<transition dest="hisPage" condition="($retValue='his')"/>
		</flowInvoker>
		<outputViews>
			<jspView id="appPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/queryLmtAppIndivList.jsp" />
			<jspView id="hisPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/queryLmtAppIndivHistoryList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtAppIndivListForCusTree" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivListForCusTreeOp"/>
			<transition dest="appPage" />
		</flowInvoker>
		<outputViews>
			<jspView id="appPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/queryLmtAppIndivListForCusTree.jsp" />
		</outputViews>
	</action>
	
	<!-- 授信新增向导页 -->
	<action id="getLmtIndivLeadPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/addLmtIndivLead.jsp"/>
	</action>
	
	<!-- 异步查询客户是否存在在途、未提交的申请信息 -->
	<action id="searchLmtIndivInfo" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.SearchLmtIndivInfo" />
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="result" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getLmtAppIndivUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/addLmtAppIndivRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 获得自助额度页面 -->
	<action id="getLmtIndivSelfPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.GetLmtIndivSelfPageOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/lmtIndivSelfRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtAppIndivRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.UpdateLmtAppIndivRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output>
					<field id="flag"/>
					<field id="serno"/>
					<field id="msg"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtAppIndivViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/queryLmtAppIndivDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getLmtAppIndivAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.GetLmtAppIndivAddPageOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<!-- <jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/addLmtAppIndivRecord.jsp"/> -->
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="msg" class="com.ecc.emp.data.DataField"/>
					<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获取冻结解冻列表页面 -->
	<action id="queryLmtFrozenAppIndivList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtFrozenAppIndivListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/queryLmtFrozenAppIndivList.jsp" />
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addLmtAppIndivRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.AddLmtAppIndivRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteLmtAppIndivRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.DeleteLmtAppIndivRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获取非自助额度列表页面 -->
	<action id="queryLmtAppIndivDetailsList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappdetails.QueryLmtAppDetailsListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndiv/LmtAppIndiv/queryLmtAppIndivDetailsList.jsp" />
		</outputViews>
	</action>
	
	<!-- 将申请跟分项数据过渡到协议跟台账  暂时没有流程时处理方法，有流程时在流程审批通过中处理  -->
	<action id="submitLmtAppIndiv" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.submitLmtAppIndiv"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtAppIndivList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageLmtAppIndivQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAppIndivList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageLmtAppIndivForCusTreeQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtAppIndivListForCusTreeOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAppIndivList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 分页查询个人额度冻结解冻列表 -->
		<action id="pageLmtFrozenAppIndivQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindiv.lmtappindiv.QueryLmtFrozenAppIndivListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAppIndivList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



