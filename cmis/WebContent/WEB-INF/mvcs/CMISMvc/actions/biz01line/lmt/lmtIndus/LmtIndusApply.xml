<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtIndusApplyList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.QueryLmtIndusApplyListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndus/LmtIndusApply/queryLmtIndusApplyList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getLmtIndusApplyUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.QueryLmtIndusApplyDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndus/LmtIndusApply/updateLmtIndusApplyRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtIndusApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.UpdateLmtIndusApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtIndusApplyViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.QueryLmtIndusApplyDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/LmtIndus/LmtIndusApply/queryLmtIndusApplyDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getLmtIndusApplyAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.GetLmtIndusApplySernoOp"/>
			<transition dest="addPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="addPage" url="biz01line/lmt/LmtIndus/LmtIndusApply/addLmtIndusApplyRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addLmtIndusApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.AddLmtIndusApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
						<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteLmtIndusApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.DeleteLmtIndusApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="flag" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
		
	<!-- 分页查询记录列表 -->
		<action id="pageLmtIndusApplyQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.QueryLmtIndusApplyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtIndusApplyList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 唯一校验 -->
	<action id="checkUniqueType" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.CheckUniqueTypeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 虚拟提交 -->
	<action id="submitLmtIndusFlow" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtindusapply.LmtIndusFlowOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
						<field id="operMsg" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 变更申请向导页 -->
	<action id="changeLmtIndusApplyLead" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/LmtIndus/LmtIndusApply/changeLmtIndusApplyLead.jsp"/>
	</action>
	
</actionDefines>