<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtGrpApplyList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.QueryLmtGrpApplyListOp"/>
			<transition dest="appPage" condition="($retValue='app')"/>
			<transition dest="hisPage" condition="($retValue='his')"/>
		    <!--/**modified by wangj XD150114003_信贷系统联保和集团关联授信审批意见查询需求修改 begin */-->
			<transition dest="cusHisPage" condition="($retValue='cusHis')"/>
		</flowInvoker>
		<outputViews>
			<jspView id="appPage" url="biz01line/lmt/LmtAppGrp/queryLmtGrpApplyList.jsp" />
			<jspView id="hisPage" url="biz01line/lmt/LmtAppGrp/queryLmtGrpAppHistoryList.jsp" />
			<jspView id="cusHisPage" url="biz01line/lmt/LmtAppGrp/queryLmtGrpAppHistoryList4Cus.jsp" />
		<!--/**modified by wangj XD150114003_信贷系统联保和集团关联授信审批意见查询需求修改 end */-->
		</outputViews>
	</action>
	
	<!-- 授信新增向导页 -->
	<action id="getLmtGrpLeadPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/LmtAppGrp/addLmtGrpLead.jsp"/>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getLmtGrpApplyUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.QueryLmtGrpApplyDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtAppGrp/updateLmtGrpApplyRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateLmtGrpApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.UpdateLmtGrpApplyRecordOp"/>
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="message"  class="com.ecc.emp.data.DataField"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtGrpApplyViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.QueryLmtGrpApplyDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/LmtAppGrp/queryLmtGrpApplyDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 下一步获得新增页面 -->
	<action id="getLmtGrpApplyAddPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.GetAddLmtGrpApplyAddPageOp"/>
			<transition dest="addPage" condition="($retValue='addPage')"/>
			<transition dest="updatePage" condition="($retValue='updatePage')"/>
		</flowInvoker>
		<outputViews>
			<jspView id="addPage" url="biz01line/lmt/LmtAppGrp/addLmtGrpApplyRecord.jsp"/>
			<jspView id="updatePage" url="biz01line/lmt/LmtAppGrp/updateLmtGrpApplyRecord.jsp"/>
		</outputViews>
	</action>
	
	<!-- 新增记录 -->
	<action id="addLmtGrpApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.AddLmtGrpApplyRecordOp"/>
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteLmtGrpApplyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.DeleteLmtGrpApplyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<!--<jspView id="retPage" url="operationResult.jsp?url=queryLmtApplyList.do" />-->
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="msg" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtGrpApplyQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.QueryLmtGrpApplyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtAppGrpList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 异步查询客户是否存在在途、未提交的申请信息 -->
	<action id="searchLmtGrpInfo" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.SearchLmtGrpInfo" />
			<transition dest="queryJsonRes" />
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryJsonRes">
				<output>
					<field id="result" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 获取成员申请列表页面 -->
	<action id="queryLmtMemberAppList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.QueryLmtMemberAppListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtAppGrp/queryLmtMemberAppList.jsp" />
		</outputViews>
	</action>
	
	<!-- 设置成员额度向导页面   -->
	<action id="getLmtGrpMemberLeadPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="biz01line/lmt/LmtAppGrp/updateLmtGrpMemberLead.jsp"/>
	</action>
	
	<!-- 将申请跟分项数据过渡到协议跟台账  暂时没有流程时处理方法，有流程时在流程审批通过中处理  -->
	<action id="submitLmtAppGrp" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.submitLmtAppGrp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryLmtGrpApplyList.do?type=app" />
		</outputViews>
	</action>
	
	<!--  变更集团成员授信类别    -->
	<action id="updateLine4GrpMemberApp" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtappgrp.UpdateLine4GrpMemberApp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="msg" class="com.ecc.emp.data.DataField"/>
					<field id="serno" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
</actionDefines>



