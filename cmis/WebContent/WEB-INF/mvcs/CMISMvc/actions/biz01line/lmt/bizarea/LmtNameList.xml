<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryLmtNameListList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtnamelist.QueryLmtNameListListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/bizarea/LmtNameList/queryLmtNameListList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getLmtNameListViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtnamelist.QueryLmtNameListDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/bizarea/LmtNameList/queryLmtNameListDetail.jsp" />
			<!--<jspView id="updatePage" url="biz01line/lmt/bizarea/LmtNameList/queryLmtCoopAgrDetails.jsp" />-->
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageLmtNameListQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtnamelist.QueryLmtNameListListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="LmtNameListList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!--************************************名单额度处理********************************************************-->
	<!-- 获得查看页面 -->
	<action id="getLmtAgrDetailsJointViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.bizarea.lmtnamelist.GetLmtAgrDetailsJointViewOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="biz01line/lmt/LmtAppJointGuar/LmtNameList_joint/queryLmtAgrDetailsJoint.jsp" />
		</outputViews>
	</action>
</actionDefines>



