	<!-- Excel导入页面 -->
	<action id="importLmtIndusListByExcel" type="normal" checkSession="true" >
		<jspView id="retPage" url="biz01line/lmt/LmtIndus/LmtIndusListApply/importLmtIndusListByExcel.jsp" />
	</action>
	
	<!-- Excel导入处理 -->
	<action id="dealLmtIndusListByExcel" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtinduslistapply.DealLmtIndusListByExcelOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="biz01line/lmt/LmtIndus/LmtIndusListApply/transList.jsp?returnMethod=getImportList"/>
		</outputViews>
