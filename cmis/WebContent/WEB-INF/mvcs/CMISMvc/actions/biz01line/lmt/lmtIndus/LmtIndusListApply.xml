	<action id="importLmtIndusListApplyRecords" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.biz01line.lmt.op.lmtinduslistapply.ImportLmtIndusListApplyRecordsOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
