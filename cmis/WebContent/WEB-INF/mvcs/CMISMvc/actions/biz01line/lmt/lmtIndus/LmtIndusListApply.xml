						<field id="flag" class="com.ecc.emp.data.DataField"/>
						<field id="success_amount" class="com.ecc.emp.data.DataField"/>
						<field id="fail_cusId" class="com.ecc.emp.data.DataField"/>
						<field id="repeat_cusId" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
