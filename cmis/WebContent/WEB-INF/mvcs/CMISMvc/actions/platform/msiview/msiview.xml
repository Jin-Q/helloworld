<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryMsiViewList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.msiview.op.ViewAllModualServicesOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/msiview/queryMsiViewList.jsp" />
		</outputViews>
	</action>
	
	<action id="getMsiViewTree" checkSession="true">
		<jspView id="queryMsiViewTree.jsp" url="platform/msiview/queryMsiViewTree.jsp"/>
	</action>
	
	<!-- 模块服务接口树图查看-->
	<action id="getMsiViewTreeData">
		<flowInvoker>
			<operation op="resourceTree" class="com.yucheng.cmis.platform.msiview.op.ViewAllModualServicesTreeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" class="com.yucheng.cmis.view.ReturnDataView" />
		</outputViews>
	</action>
	
	<!-- 模块、服务集查看   -->
	<action id="getMsiViewClassView">
		<flowInvoker>
			<operation op="resourceTree" class="com.yucheng.cmis.platform.msiview.op.ViewAllModualServicesTreeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/msiview/queryClassView.jsp" />
		</outputViews>
	</action>
	
	<!-- 服务查看   -->
	<action id="getMsiViewMethodView">
		<flowInvoker>
			<operation op="resourceTree" class="com.yucheng.cmis.platform.msiview.op.ViewAllModualServicesTreeOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/msiview/queryMethodView.jsp" />
		</outputViews>
	</action>
	
	
	
	<!-- 查看模块依赖关系 -->
	<action id="viewModualRelyOn" type="normal" checkSession="false" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.msiview.op.ViewModualRelyOnRelationOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/msiview/viewModualRelyOnRelation.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageMsiViewList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.msiview.op.ViewAllModualServicesOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="ModualServiceList">
						<kColl>
							<field id="modual_name"></field>
							<field id="method_name" />
							<field id="method_desc" />
							<field id="in_param" />
							<field id="out_param" />
							<field id="example" />
						</kColl>
					</iColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



