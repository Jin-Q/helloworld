<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	<!-- 添加重载菜单 -->
	<action id="reloadCashe" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/systemreload/systemReloadFile.jsp"/>
	</action>
	<!-- 重载触发的方法 -->
	<action id="beginReloadSystemInfo" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.reload.op.CMISReloadSystem"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/systemreload/systemReloadResult.jsp" />
		</outputViews>
	</action>
	
</actionDefines>



