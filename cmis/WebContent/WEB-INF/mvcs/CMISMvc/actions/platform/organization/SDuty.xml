<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="querySDutyList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.QuerySDutyListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/SDuty/querySDutyList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getSDutyUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.organization.sduty.op.QuerySDutyDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/SDuty/updateSDutyRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateSDutyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.UpdateSDutyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="msg" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getSDutyViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.QuerySDutyDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/organization/SDuty/querySDutyDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getSDutyAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/organization/SDuty/addSDutyRecord.jsp"/>
	</action>
	
	<!-- 新增记录 
	<action id="addSDutyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.AddSDutyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=querySDutyList.do" />
		</outputViews>
	</action>
	
	-->
	<action id="addSDutyRecord" type="normal" checkSession="true">
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.AddSDutyRecordOp" />
			<transition dest="retPage" />
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="dutyno" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteSDutyRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.DeleteSDutyRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="msg" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>

	<action id="SDutyExcel" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.QuerySDutyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
		   <view id="queryListRes" class="com.yucheng.cmis.view.ExcelView">
 				<excelWorkbook fileName="dutyManage.xls" class="com.yucheng.cmis.view.excel.ExcelWorkbook">
 					<excelSheet id="1" name="aa1" isTitle="true" iCollName="SDutyList" class="com.yucheng.cmis.view.excel.ExcelSheet">
 						<excelColumn dataName="dutyno" name="岗位码" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="dutyname" name="岗位名称" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="organno" name="机构码" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 						<excelColumn dataName="depno" name="部门码" class="com.yucheng.cmis.view.excel.ExcelColumn"/>
 					</excelSheet>
 				</excelWorkbook>
 			</view>
		</outputViews>
	</action>
		
	
	<!-- 分页查询记录列表 -->
		<action id="pageSDutyQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.sduty.op.QuerySDutyListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="SDutyList">
						<kColl>
							<field id="dutyno" />
							<field id="dutyname" />
							<field id="organno" />
							<field id="depno" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



