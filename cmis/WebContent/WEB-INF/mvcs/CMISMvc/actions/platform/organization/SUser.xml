		</outputViews>
	</action>
	
	<!-- 重置密码 -->
	<action id="resetPasswordSUserRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suser.op.ResetPasswordSUserRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
