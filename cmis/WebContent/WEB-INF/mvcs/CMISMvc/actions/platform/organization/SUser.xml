				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField" />
					<field id="org" class="com.ecc.emp.data.DataField" />
					<field id="orgName" class="com.ecc.emp.data.DataField" />
				</output>
			</view>
		</outputViews>
	</action>
	
	<!-- 选择用户pop不加过滤-->
	<action id="getAllSUserPopListOp" type="normal" checkSession="true" >
