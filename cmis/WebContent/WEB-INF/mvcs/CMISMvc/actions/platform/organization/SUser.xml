	<!-- 修改密码页面 -->
	<action id="modifyPassword" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suser.op.ModifyPasswordOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/organization/SUser/ModifyPassword.jsp" />
		</outputViews>
	</action>
	
	
	<!--  检查密码-->
	<action id="checkPassword"  type="normal" checkSession="true" >
		<flowInvoker>
			<operation  updateCheck="false" class="com.yucheng.cmis.platform.organization.suser.op.CheckPasswordOp" />
			<transition dest="res" condition="($retValue='success')"/>
		</flowInvoker>
		<outputViews>
			<view id="res" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="result" class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	
		<!--修改密码 -->
	<action id="ModifyPasswordUpdate" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suser.op.ModifyPasswordUpdateOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/SUser/operationPasswordResult.jsp?url=getSUserViewPage.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageSUserQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suser.op.QuerySUserListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="SUserList">
						<kColl>
							<field id="actorno" />
							<field id="actorname" />
							<field id="telnum" />
							<field id="idcardno" />
							<field id="orgid" />
							<field id="state" />
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 异步校验所选择客户经理是否归属某个团队 -->
	<action id="CheckSUserRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suser.op.CheckSUserPopListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
