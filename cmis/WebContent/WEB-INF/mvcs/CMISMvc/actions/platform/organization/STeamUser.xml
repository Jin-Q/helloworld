<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	<!-- 获取操作主页面 -->
	<action id="querySTeamUserList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.QuerySTeamUserListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/STeamUser/querySTeamUserList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getSTeamUserUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.organization.steamuser.op.QuerySTeamUserDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/STeamUser/updateSTeamUserRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateSTeamUserRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.UpdateSTeamUserRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
					<field id="msg" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 获得查看页面 -->
	<action id="getSTeamUserViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.QuerySTeamUserDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/organization/STeamUser/querySTeamUserDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getSTeamUserAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/organization/STeamUser/addSTeamUserRecord.jsp"/>
	</action>
	
	<!-- 删除记录 -->
	<action id="deleteSTeamUserRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.DeleteSTeamUserRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 引入成员信息操作-->
	<action id="introTeamUser" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.IntroTeamUserOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="retPage">
				<output>
					<field id="flag" />
					<field id="msg" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 获取成员信息返回给pop-->
	<action id="querySTeamUserIntroList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.QuerySTeamUserPopListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/STeamUser/querySTeamUserListIntroPop.jsp" />
		</outputViews>
	</action>
	<!-- 分页查询成员记录列表 -->
	<action id="pageSTeamUserIntroQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.QuerySTeamUserPopListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="SUserList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
		
	<!-- 分页查询记录列表 -->
	<action id="pageSTeamUserQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.steamuser.op.QuerySTeamUserListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="STeamUserList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



