<?xml version="1.0" encoding="UTF-8" ?>
<!-- 用户角色、岗位、机构设置 -->
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	<!-- 用户角色设置 -->
	<action id="listUserRole" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suserroledutyorg.op.QueryUserRoleListOperations"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/userRoleDutyOrg/addRoleToUser.jsp" />
		</outputViews>
	</action>
	<action id="addRoleToUser" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suserroledutyorg.op.AddRoleToUserOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/userRoleDutyOrg/roleUserResult.jsp" />
		</outputViews>
	</action>
	
	<!-- 用户岗位设置 -->
	<action id="listDutyUser" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suserroledutyorg.op.QueryUserDutyListOperations"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/userRoleDutyOrg/addDutyToUser.jsp" />
		</outputViews>
	</action>
	<action id="addDutyToUser" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suserroledutyorg.op.AddDutyToUserOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/userRoleDutyOrg/roleUserResult.jsp" />
		</outputViews>
	</action>
	
	<!-- 用户机构设置 -->
	<action id="listDeptUser" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suserroledutyorg.op.QueryUserDeptListOperations"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/userRoleDutyOrg/addDeptToUser.jsp" />
		</outputViews>
	</action>
	<action id="addDeptToUser" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.organization.suserroledutyorg.op.AddDeptToUserOperation"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/organization/userRoleDutyOrg/roleUserResult.jsp" />
		</outputViews>
	</action>
</actionDefines>