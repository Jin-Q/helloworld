<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<action id="queryWfHumanstatesList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfHumanstatesListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/wfhumanstates/queryWfHumanstatesList.jsp" />
		</outputViews>
	</action>
	
	<action id="getWfHumanstatesUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfHumanstatesDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/wfhumanstates/updateWfHumanstatesRecord.jsp" />
		</outputViews>
	</action>
	
	<action id="updateWfHumanstatesRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.UpdateWfHumanstatesRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfHumanstatesList.do" />
		</outputViews>
	</action>
	
	<action id="getWfHumanstatesViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfHumanstatesDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/workflow/wfhumanstates/queryWfHumanstatesDetail.jsp" />
		</outputViews>
	</action>
	
	<action id="getWfHumanstatesAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/workflow/wfhumanstates/addWfHumanstatesRecord.jsp"/>
	</action>
	
	<action id="addWfHumanstatesRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.AddWfHumanstatesRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfHumanstatesList.do" />
		</outputViews>
	</action>	
	
	<action id="deleteWfHumanstatesRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.DeleteWfHumanstatesRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfHumanstatesList.do" />
		</outputViews>
	</action>
	
	<action id="pageWfHumanstatesQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfHumanstatesListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="WfHumanstatesList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 检查是否有日期重叠的记录 -->
	<action id="queryWfHumanstatsByTime" checkSession="true" type="normal">
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfHumanstatsByTimeOp"/>
			<transition dest="ret"/>
		</flowInvoker>
		<outputViews>
			<jsonView id="ret" class="com.yucheng.cmis.view.JSONView">
				<output>
					<kColl id="retKcoll">
						<field id="result"/>
						<field id="begintime"/>
						<field id="endtime"/>
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<action id="queryWfAgentLogList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfAgentLogListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/wfhumanstates/queryWfAgentLogList.jsp" />
		</outputViews>
	</action>
	
	<action id="pageWfAgentLogQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfhumanstates.QueryWfAgentLogListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="WfiAgentLogList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	
</actionDefines>



