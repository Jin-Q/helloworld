<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 查询记录列表 -->
	<action id="queryWfiSignTaskList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.QueryWfiSignTaskListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryListRes" url="platform/workflow/meetingsign/wfisigntask/queryWfiSignTaskList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getWfiSignTaskUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.QueryWfiSignTaskDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/meetingsign/wfisigntask/updateWfiSignTaskRecord.jsp" />
		</outputViews>
	</action>
	<!-- 初始化会签会议 -->
	<action id="initSignTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.InitWfiSignTaskOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/meetingsign/wfisigntask/updateWfiSignTaskRecord.jsp" />
		</outputViews>
	</action>
	<!-- 开始会签会议 -->
	<action id="beginSignTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.BeginWfiSignTaskOp"/>
			<transition dest="json"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="json">
				<output>
					<field id="success"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 终止会签会议 -->
	<action id="finishSignTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.FinishWfiSignTaskOp"/>
			<transition dest="json"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="json">
				<output>
					<field id="success"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 重开会签会议 -->
	<action id="rebeginSignTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.RebeginWfiSignTaskOp"/>
			<transition dest="json"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="json">
				<output>
					<field id="success"/>
					<field id="taskId"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 取消会签会议 -->
	<action id="cancelSignTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.CancelWfiSignTaskOp"/>
			<transition dest="json"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="json">
				<output>
					<field id="success"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 会签会议提交下一审批人 -->
	<action id="submitSignTask" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.SubmitWfiSignTaskOp"/>
			<transition dest="json"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="json">
				<output>
					<field id="success"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	<!-- 复位贷审会成员投票 -->
	<action id="resetSignVote" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.taskAction.ResetWfiSignVoteStatusOp"/>
			<transition dest="json"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="json">
				<output>
					<field id="success"/>
				</output>
			</jsonView>
		</outputViews>
	</action>

	<!-- 修改信息 -->
	<action id="updateWfiSignTaskRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.UpdateWfiSignTaskRecordOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<field id="success"/>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	
	<!-- 查询详情 -->
	<action id="queryWfiSignTaskDetail" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.QueryWfiSignTaskDetailOp"/>
			<transition dest="queryDetailRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryDetailRes" url="platform/workflow/meetingsign/wfisigntask/queryWfiSignTaskDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageWfiSignTaskQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.QueryWfiSignTaskListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="WfiSignTaskList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 查询可选会签成员（按节点配置办理人） -->
	<action id="queryWfiSignTaskUserPop" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.QueryWfiSignTaskUserPopOp"/>
			<transition dest="queryDetailRes"/>
		</flowInvoker>
		<outputViews>
			<jspView id="queryDetailRes" url="platform/workflow/meetingsign/wfisigntask/queryWfiSignTaskUserPop.jsp" />
		</outputViews>
	</action>
	<action id="pageWfiSignTaskUserPopQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.meetingsign.op.wfisigntask.QueryWfiSignTaskUserPopOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="SUserList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>
