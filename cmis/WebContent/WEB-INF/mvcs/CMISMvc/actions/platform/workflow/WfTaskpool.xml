<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryWfTaskpoolList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.QueryWfTaskpoolListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/taskpool/queryWfTaskpoolList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getWfTaskpoolUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.op.taskpool.QueryWfTaskpoolDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/taskpool/updateWfTaskpoolRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateWfTaskpoolRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.UpdateWfTaskpoolRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfTaskpoolList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getWfTaskpoolViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.QueryWfTaskpoolDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/workflow/taskpool/queryWfTaskpoolDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getWfTaskpoolAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/workflow/taskpool/addWfTaskpoolRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addWfTaskpoolRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.AddWfTaskpoolRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfTaskpoolList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteWfTaskpoolRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.DeleteWfTaskpoolRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfTaskpoolList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
	<action id="pageWfTaskpoolQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.QueryWfTaskpoolListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="WfTaskpoolList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 设置项目池关联岗位页面 -->
	<action id="getWfTaskpoolDutyPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.GetWfTaskpoolDutyPageOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/workflow/taskpool/wfTaskpoolDutyPage.jsp" />
		</outputViews>
	</action>
	<!-- 设置项目池关联岗位 -->
	<action id="setWfTaskpoolUsers" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.SetWfTaskpoolUsersOp"/>
			<transition dest="res"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="res">
				<output>
					<field id="flag" />
				</output>
			</jsonView>
		</outputViews>
	</action>
	
	<!-- 查询引擎客户端提供给项目池配置时使用 -->
	<action id="queryWfClientPopList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.taskpool.QueryWfClientPopListOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/workflow/taskpool/queryWfClientPopList.jsp" />
		</outputViews>
	</action>
	
</actionDefines>



