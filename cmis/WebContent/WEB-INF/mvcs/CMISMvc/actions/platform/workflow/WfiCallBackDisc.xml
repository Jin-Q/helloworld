<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryWfiCallBackDiscList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.QueryWfiCallBackDiscListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/WfiCallBackDisc/queryWfiCallBackDiscList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getWfiCallBackDiscUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.QueryWfiCallBackDiscDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/WfiCallBackDisc/updateWfiCallBackDiscRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateWfiCallBackDiscRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.UpdateWfiCallBackDiscRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfiCallBackDiscList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getWfiCallBackDiscViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.QueryWfiCallBackDiscDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/workflow/WfiCallBackDisc/queryWfiCallBackDiscDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getWfiCallBackDiscAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/workflow/WfiCallBackDisc/addWfiCallBackDiscRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addWfiCallBackDiscRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.AddWfiCallBackDiscRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfiCallBackDiscList.do" />
		</outputViews>
	</action>	
	
	<action id="CheckWfiCallBackDiscByEnName" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.CheckWfiCallBackDiscRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<view id="retPage" class="com.yucheng.cmis.view.JSONView">
				<output class="com.ecc.emp.data.KeyedCollection">
					<field id="flag" class="com.ecc.emp.data.DataField"/>
					<field id="msg"  class="com.ecc.emp.data.DataField"/>
				</output>
			</view>
		</outputViews>
	</action>
	<!-- 分页查询记录列表 -->
		<action id="pageWfiCallBackDiscQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wficallbackdisc.QueryWfiCallBackDiscListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="WfiCallBackDiscList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



