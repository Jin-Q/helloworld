<?xml version="1.0" encoding="UTF-8"?>
<actionDefines>
	<classMap>
		<map id="action" class="com.yucheng.cmis.ad.mvc.ADRequestController"/>
		<map id="flowInvoker" class="com.yucheng.cmis.ad.mvc.ADFlowInvoker"/>
		<map id="outputViews" class="java.util.HashMap"/>
		<map id="output" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="kColl" class="com.ecc.emp.data.KeyedCollection"/>
		<map id="iColl" class="com.ecc.emp.data.IndexedCollection"/>
		<map id="field" class="com.ecc.emp.data.DataField"/>
	</classMap>
	
	<!-- 获取操作主页面 -->
	<action id="queryWfiWorkflow2orgList" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.QueryWfiWorkflow2orgListOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/WfiWorkflow2org/queryWfiWorkflow2orgList.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得编辑页面 -->
	<action id="getWfiWorkflow2orgUpdatePage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation updateCheck="false" class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.QueryWfiWorkflow2orgDetailOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="platform/workflow/WfiWorkflow2org/updateWfiWorkflow2orgRecord.jsp" />
		</outputViews>
	</action>
	
	<!-- 更新记录 -->
	<action id="updateWfiWorkflow2orgRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.UpdateWfiWorkflow2orgRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfiWorkflow2orgList.do" />
		</outputViews>
	</action>
	
	<!-- 获得查看页面 -->
	<action id="getWfiWorkflow2orgViewPage" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.QueryWfiWorkflow2orgDetailOp"/>
			<transition dest="updatePage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="updatePage" url="platform/workflow/WfiWorkflow2org/queryWfiWorkflow2orgDetail.jsp" />
		</outputViews>
	</action>
	
	<!-- 获得新增页面 -->
	<action id="getWfiWorkflow2orgAddPage" type="normal" checkSession="true" >
		<jspView id="addPage" url="platform/workflow/WfiWorkflow2org/addWfiWorkflow2orgRecord.jsp"/>
	</action>
	
	<!-- 新增记录 -->
	<action id="addWfiWorkflow2orgRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.AddWfiWorkflow2orgRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfiWorkflow2orgList.do" />
		</outputViews>
	</action>	
	
	<!-- 删除记录 -->
	<action id="deleteWfiWorkflow2orgRecord" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.DeleteWfiWorkflow2orgRecordOp"/>
			<transition dest="retPage"/>
		</flowInvoker>
		<outputViews>
			<jspView id="retPage" url="operationResult.jsp?url=queryWfiWorkflow2orgList.do" />
		</outputViews>
	</action>
	
	<!-- 分页查询记录列表 -->
		<action id="pageWfiWorkflow2orgQuery" type="normal" checkSession="true" >
		<flowInvoker>
			<operation class="com.yucheng.cmis.platform.workflow.op.wfiworkflow2org.QueryWfiWorkflow2orgListOp"/>
			<transition dest="queryListRes"/>
		</flowInvoker>
		<outputViews>
			<jsonView class="com.yucheng.cmis.view.JSONView" id="queryListRes">
				<output>
					<iColl id="WfiWorkflow2orgList">
						<kColl>
						</kColl>
					</iColl>
					<kColl id="pageInfo">
						<field id="currentPage" />
						<field id="maxLine" />
						<field id="recordSize" />
					</kColl>
				</output>
			</jsonView>
		</outputViews>
	</action>
	
</actionDefines>



